    },
    {
      "key": "06d2470",
      "nodeTitle": "spawned_river_friend",
      "sourceText": "Карти показують, як повертає річка.",
      "sourceEnText": "Maps show how the river turns.",
      "audioFile": "fr_10_06d2470.ogg",
      "voiceProfile": "yaroslava",
      "actorId": "ADULT_F",
      "durationMs": 2601,
