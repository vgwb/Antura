      "textHash": "752a6e58e034c257d3d338506bf609af691c8107e31c974c3a734b4e04d591ed"
    },
    {
      "key": "0b22d58",
      "nodeTitle": "luxembourg_npc",
      "sourceText": "Мойен! Я из Люксембурга!",
      "sourceEnText": "Moien! I’m from Luxembourg!",
      "audioFile": "fr_00_0b22d58.ogg",
      "voiceProfile": "felix",
      "actorId": "SENIOR_M",
      "durationMs": 1750,
