      "nodeTitle": "italy_npc",
      "sourceText": "Грацие! Это мой флаг!",
      "sourceEnText": "Grazie! That's my flag!",
      "audioFile": "fr_00_081ac66.ogg",
      "voiceProfile": "felix",
      "actorId": "KID_M",
