    {
      "key": "0b18893",
      "nodeTitle": "belgium_npc",
      "sourceText": "Мы также говорим по-французски!",
      "sourceEnText": "We also speak French!",
      "audioFile": "fr_00_0b18893.ogg",
      "voiceProfile": "felix",
      "actorId": "SENIOR_F",
