      "audioFile": "fr_00_0ab3dd9.ogg",
      "voiceProfile": "felix",
      "actorId": "KID_M",
      "durationMs": 3474,
      "updatedAt": "2025-11-04T16:06:12Z",
      "textHash": "38481142ea564784efac2d3ac1e4a93836bac825c7d98383a966636c1a3b56ce"
    },
    {
      "key": "0b11066",
      "nodeTitle": "item_flag_belgium",
      "sourceText": "Флаг Бельгии.",
      "sourceEnText": "Flag of Belgium.",
      "audioFile": "fr_00_0b11066.ogg",
      "voiceProfile": "felix",
      "actorId": "NARRATOR",
