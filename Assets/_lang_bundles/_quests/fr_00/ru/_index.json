      "textHash": "1b827afa9d549bc3f86bc38af45431f72e7520000f25acef46f2a9fe16ffe93e"
    },
    {
      "key": "09cf6c9",
      "nodeTitle": "spain_npc",
      "sourceText": "Мадрид — столица Испании.",
      "sourceEnText": "Madrid is the capital of Spain.",
      "audioFile": "fr_00_09cf6c9.ogg",
      "voiceProfile": "felix",
      "actorId": "NARRATOR",
      "durationMs": 2090,
      "updatedAt": "2025-11-04T16:05:01Z",
