    },
    {
      "key": "09fe8fc",
      "nodeTitle": "npc_swiss",
      "sourceText": "Привет! Я из Швейцарии!",
      "sourceEnText": "Hello! I’m from Switzerland!",
      "audioFile": "fr_00_09fe8fc.ogg",
      "voiceProfile": "felix",
      "actorId": "ADULT_F",
      "durationMs": 2038,
      "updatedAt": "2025-11-04T16:04:03Z",
      "textHash": "bc0370a79863d792a5de2e4525fce6439d3dafde0e26fad9ad3f8c126c1b9f2f"
    },
    {
      "key": "0a20eed",
      "nodeTitle": "france_npc",
      "sourceText": "Антура перепутала все флаги!",
      "sourceEnText": "We have a problem. Antura mixed up all the flags!",
      "audioFile": "fr_00_0a20eed.ogg",
      "voiceProfile": "felix",
      "actorId": "KID_F",
      "durationMs": 2038,
      "updatedAt": "2025-11-04T16:04:36Z",
      "textHash": "a57506b713a7c5f77b8d02c18b6b302c19125d69af3f057b60e760c93a337900"
    },
    {
      "key": "0a23cce",
      "nodeTitle": "spawned_italy",
      "sourceText": "Италия на карте похожа на ботинок!",
      "sourceEnText": "Italy looks like a boot on the map!",
      "audioFile": "fr_00_0a23cce.ogg",
      "voiceProfile": "felix",
      "actorId": "KID_M",
      "durationMs": 2456,
