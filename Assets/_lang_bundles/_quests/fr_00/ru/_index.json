      "textHash": "01020541f598d9a27e9c404ad98f7bd33f9291cf2d3ff97a6684e0e0e07c00c5"
    },
    {
      "key": "0bf6b0d",
      "nodeTitle": "npc_swiss",
      "sourceText": "Мы славимся заснеженными горами и сыром.",
      "sourceEnText": "We're famous for snowy mountains and cheese.",
      "audioFile": "fr_00_0bf6b0d.ogg",
      "voiceProfile": "felix",
      "actorId": "ADULT_F",
      "durationMs": 2821,
