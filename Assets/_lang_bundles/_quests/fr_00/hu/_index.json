    {
      "key": "001f54f",
      "nodeTitle": "italy_npc",
      "sourceText": "Segíts nekik megtalálni a belga zászlót!",
      "sourceEnText": "Help them find the Belgian flag!",
      "audioFile": "fr_00_001f54f.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 2403,
      "updatedAt": "2025-11-03T13:41:21Z",
