    {
      "key": "07e8390",
      "nodeTitle": "schools_activity_done",
      "sourceText": "Bon travail!",
      "sourceEnText": "Good job!",
      "audioFile": "fr_02_07e8390.ogg",
      "voiceProfile": "louis",
      "actorId": "SENIOR_M",
      "durationMs": 1149,
