      "key": "0879a44",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "J'aime dessiner et colorier.",
      "sourceEnText": "I like to draw and color.",
      "audioFile": "fr_02_0879a44.ogg",
      "voiceProfile": "louis",
      "actorId": "KID_F",
