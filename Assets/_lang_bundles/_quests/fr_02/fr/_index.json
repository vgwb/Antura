    {
      "key": "06478e9",
      "nodeTitle": "school_3",
      "sourceText": "Bonjour ! C'est un collège pour les élèves de 11 à 15 ans.",
      "sourceEnText": "Hi! This is a middle school for ages 11 to 15.",
      "audioFile": "fr_02_06478e9.ogg",
      "voiceProfile": "louis",
      "actorId": "KID_M",
      "durationMs": 3709,
      "updatedAt": "2025-10-09T10:50:03Z",
      "textHash": "f631124f4f6dda6e75e9e92f86f62c8a3cbe91ec89e15d61803da70afe89dfaf"
    },
    {
      "key": "0671097",
      "nodeTitle": "school_canteen",
      "sourceText": "Bon appétit!",
      "sourceEnText": "Enjoy your meal!",
      "audioFile": "fr_02_0671097.ogg",
      "voiceProfile": "louis",
      "actorId": "SENIOR_F",
      "durationMs": 1097,
      "updatedAt": "2025-10-09T10:51:40Z",
      "textHash": "097c95e03ecc0e0836bc975bfb686ff9e77982e1c41ff85cbcadd1d00ddccec5"
    },
    {
      "key": "068e48b",
      "nodeTitle": "classroom_1",
      "sourceText": "Ce n'est pas votre cours. Essayez le cours d'à côté !",
      "sourceEnText": "This is not your class. Try the next door!",
      "audioFile": "fr_02_068e48b.ogg",
      "voiceProfile": "mademoiselle",
      "actorId": "ADULT_M",
      "durationMs": 2821,
      "updatedAt": "2025-10-09T10:50:28Z",
      "textHash": "96c2aac294a9ca40a56dbb26daa5bc408677397a4d19ea20b8381fb305ab80a9"
    },
    {
      "key": "078fff9",
      "nodeTitle": "schools_activity_done",
      "sourceText": "Voici.",
      "sourceEnText": "Here you go.",
      "audioFile": "fr_02_078fff9.ogg",
      "voiceProfile": "louis",
      "actorId": "SENIOR_M",
      "durationMs": 862,
      "updatedAt": "2025-10-09T10:51:00Z",
      "textHash": "a6fde012c9cea68afb28277e6cd26e750b04fc28bef1a7f0c894fd51b8d30015"
