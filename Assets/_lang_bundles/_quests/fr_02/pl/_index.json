    },
    {
      "key": "093e91d",
      "nodeTitle": "task_class_done",
      "sourceText": "Tak! Możesz rozpocząć lekcję już teraz.",
      "sourceEnText": "Yes! You can start the lesson now.",
      "audioFile": "fr_02_093e91d.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2952,
      "updatedAt": "2026-01-12T06:30:28Z",
      "textHash": "f66eb59097661c0756d78820a74be9f23ab66870d9e6e7f77ea9ee12da4ab15d"
    },
    {
      "key": "094eace",
      "nodeTitle": "school_4_talk_man",
      "sourceText": "Ale najpierw musisz na to zasłużyć!",
      "sourceEnText": "But first, you have to earn it!",
      "audioFile": "fr_02_094eace.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2508,
      "updatedAt": "2026-01-12T06:30:47Z",
      "textHash": "c9c8f73e0e1c3a149d52859ef8aa91fdb4ca509df4ef72adf50064783be085e0"
    },
    {
      "key": "096b721",
      "nodeTitle": "school_1",
      "sourceText": "Witamy! To przedszkole dla dzieci w wieku od 3 do 5 lat.",
      "sourceEnText": "Welcome! This is a nursery school for children aged 3 to 5.",
      "audioFile": "fr_02_096b721.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 4127,
      "updatedAt": "2026-01-12T06:28:45Z",
      "textHash": "24a63eadfce34a31bed9d838f3994c5ab52b9991459fec554dc7bc9953db752c"
    },
    {
      "key": "0978619",
      "nodeTitle": "school_canteen",
      "sourceText": "Spójrz na dzisiejsze menu!",
      "sourceEnText": "Look at the menu for today!",
      "audioFile": "fr_02_0978619.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_F",
      "durationMs": 1802,
      "updatedAt": "2026-01-12T06:30:36Z",
      "textHash": "fa5131a46015f94e7e91870257c2d5432560a08d56d16ce297b5fff271cc20b9"
    },
    {
      "key": "09ac4f1",
      "nodeTitle": "post_quest_activity",
      "sourceText": "Teraz narysuj mapę swojej klasy!",
      "sourceEnText": "Now draw a map of your classroom!",
      "audioFile": "fr_02_09ac4f1.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2508,
      "updatedAt": "2026-01-12T16:32:59Z",
      "textHash": "353df09a65271182c1602a747bd1b5c2f74060afd850cf1efcadd01446a1fdb0"
    },
    {
      "key": "09bfe7f",
      "nodeTitle": "classroom_1",
      "sourceText": "Dzieci rozpoczynają naukę w szkole podstawowej w wieku 6 lat.",
      "sourceEnText": "Children start primary school at 6 years old.",
      "audioFile": "fr_02_09bfe7f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3709,
      "updatedAt": "2026-01-12T06:29:30Z",
      "textHash": "8b3a5d2603e7bb24b85571e86a9026bbb0decf0c6a049f58b35f09e998f1f18d"
    },
    {
      "key": "09c12ac",
      "nodeTitle": "school_charte",
      "sourceText": "niezależnie od ich przekonań.",
      "sourceEnText": "no matter their beliefs.",
      "audioFile": "fr_02_09c12ac.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1907,
      "updatedAt": "2026-01-12T06:30:42Z",
      "textHash": "aedd9e9db82a5e916dc3127279eeae02cc0b3e300957b9e3b4e9be428f1b4385"
    },
    {
      "key": "0a5c8f1",
      "nodeTitle": "activity_match_done",
      "sourceText": "Nie do końca dobrze. Spróbuj ponownie.",
      "sourceEnText": "Not quite right. Try again.",
      "audioFile": "fr_02_0a5c8f1.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2691,
      "updatedAt": "2026-01-12T16:31:48Z",
      "textHash": "810ede64242fd21cdaa077c512842afbf4a84bbfaeb367d4167ad33694ddc25f"
    },
    {
      "key": "0aeea98",
      "nodeTitle": "spawned_kid_m",
      "sourceText": "Uwielbiam gry wideo!",
      "sourceEnText": "I love video games!",
      "audioFile": "fr_02_0aeea98.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 1384,
      "updatedAt": "2026-01-12T16:34:21Z",
      "textHash": "82a119dfc4aae8250873375eab3a02a56e5ef62f06fb9fcad4cea0806a6f541c"
    },
    {
      "key": "0b22d07",
      "nodeTitle": "school_4_talk_man",
      "sourceText": "Teraz znajdź swoją szkołę!",
      "sourceEnText": "Now go find your school!",
      "audioFile": "fr_02_0b22d07.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1802,
      "updatedAt": "2026-01-12T16:32:16Z",
      "textHash": "0846f0a3da4fe03fef61f32649617c2042f91d79a0b3d214e7e7cecd5ea2a536"
    },
    {
      "key": "0b411ce",
      "nodeTitle": "classroom_3",
      "sourceText": "dla uczniów w wieku 8 lat.",
      "sourceEnText": "for students who are 8 years old.",
      "audioFile": "fr_02_0b411ce.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2116,
      "updatedAt": "2026-01-12T06:29:48Z",
      "textHash": "c1fc74856f14f999f22ab461e317fc3504b9940c2e4f5b9571b58d37b2c4ab57"
    },
    {
      "key": "0b43290",
      "nodeTitle": "task_backpack",
      "sourceText": "Może ktoś to ma,",
      "sourceEnText": "Maybe someone has it,",
      "audioFile": "fr_02_0b43290.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1332,
      "updatedAt": "2026-01-12T06:29:18Z",
      "textHash": "482a2d34fb1a68197b6d11baed7195cfa701cbc7163fcabe8e56c8e610b6a219"
    },
    {
      "key": "0b82cb5",
      "nodeTitle": "spawned_kid_m",
      "sourceText": "Gram w piłkę nożną z przyjaciółmi.",
      "sourceEnText": "I play soccer with friends.",
      "audioFile": "fr_02_0b82cb5.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 2116,
      "updatedAt": "2026-01-12T16:34:09Z",
      "textHash": "b661207d73e9b9a5e2ee36f7a16a883f65549741e7cfb655f63301bbeb84c6d4"
    },
    {
      "key": "0b91268",
      "nodeTitle": "school_2_talk",
      "sourceText": "Zapraszamy! Wasza klasa jest na końcu korytarza.",
      "sourceEnText": "Come in! Your classroom is down the hall.",
      "audioFile": "fr_02_0b91268.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3605,
      "updatedAt": "2026-01-12T06:29:24Z",
      "textHash": "b666dd34d152d330771f2f6354b0fcf450f5aede2078ba2959cd2dcc4813609c"
    },
    {
      "key": "0bce01a",
      "nodeTitle": "classroom_3",
      "sourceText": "Cześć! To jest sala wykładowa CE2.",
      "sourceEnText": "Hi! This is the CE2 classroom.",
      "audioFile": "fr_02_0bce01a.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 3004,
      "updatedAt": "2026-01-12T06:29:45Z",
      "textHash": "7d55592b46f449281f78aa5d82b54368f40d12d534ff44bf4f03fe2515f91038"
    },
    {
      "key": "0c282e5",
      "nodeTitle": "task_backpack",
      "sourceText": "Spróbuj popytać dookoła.",
      "sourceEnText": "Try asking around.",
      "audioFile": "fr_02_0c282e5.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1855,
      "updatedAt": "2026-01-12T06:29:15Z",
      "textHash": "a224d22c6c81f305495ca837260b004aca59bed6ec4cb928aa4307554f36fa8d"
    },
    {
      "key": "0c3b4fe",
      "nodeTitle": "school_4_talk_man",
      "sourceText": "Cześć! Znalazłem plecak po drodze.",
      "sourceEnText": "Hello! I found a backpack on my way here.",
      "audioFile": "fr_02_0c3b4fe.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2638,
      "updatedAt": "2026-01-12T06:29:13Z",
      "textHash": "0bdc7f77144e748f7ffabcfffed039b78d983356c8c3c5417bc1a3c453069618"
    },
    {
      "key": "0ce9646",
      "nodeTitle": "school_4_talk_man",
      "sourceText": "Może to twoje?",
      "sourceEnText": "Maybe it's yours?",
      "audioFile": "fr_02_0ce9646.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1202,
      "updatedAt": "2026-01-12T16:31:59Z",
      "textHash": "06344ce63e872be792dc360128eb0ef0dd8374811b9a9f319a76a0ed92f1d920"
    },
    {
      "key": "0cee587",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "Moim ulubionym przedmiotem jest sztuka.",
      "sourceEnText": "My favorite subject is art.",
      "audioFile": "fr_02_0cee587.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 2873,
      "updatedAt": "2026-01-12T16:33:48Z",
      "textHash": "5809d2afe23c86dffa56b7be8abe701332c9f1fdb0985c7e577a42fa7e5e549c"
    },
    {
      "key": "0d88d5b",
      "nodeTitle": "spawned_teacher",
      "sourceText": "Uczymy się pisania kursywnego.",
      "sourceEnText": "We learn cursive writing.",
      "audioFile": "fr_02_0d88d5b.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 1907,
      "updatedAt": "2026-01-12T16:35:37Z",
      "textHash": "56f667df42a68ac0f84c80ff55c41b74f83bf56e47d38e8ab4c436abd0720720"
    },
    {
      "key": "0d8d4aa",
      "nodeTitle": "spawned_adult",
      "sourceText": "Lubię, kiedy moje dzieci opowiadają mi, czego się nauczyły.",
      "sourceEnText": "I like when my kids tell me what they learned.",
      "audioFile": "fr_02_0d8d4aa.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 3187,
      "updatedAt": "2026-01-12T16:35:03Z",
      "textHash": "e8305d5aad0fe6192f7bce2898673bdbac6ba6daf62d96f03be30ab12da63d5f"
    },
    {
      "key": "0da284c",
      "nodeTitle": "task_find_school_desc",
      "sourceText": "Znajdź swoją szkołę!",
      "sourceEnText": "Find your school!",
      "audioFile": "fr_02_0da284c.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1515,
      "updatedAt": "2026-01-12T16:30:50Z",
      "textHash": "960dd5d134e61733cc41a8f9ac747b6b0a095f58fbb66a92b371528b5a4dc5c7"
    },
    {
      "key": "0dd7977",
      "nodeTitle": "school_2_welcome",
      "sourceText": "Wygląda na to, że zapomniałeś plecaka.",
      "sourceEnText": "It looks like you forgot your backpack.",
      "audioFile": "fr_02_0dd7977.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2456,
      "updatedAt": "2026-01-12T06:28:54Z",
      "textHash": "d21baee66105536f8992ab70cdc451cb412e14a499fd615dd5cda381b47cbd6d"
    },
    {
      "key": "0df97f8",
      "nodeTitle": "school_3",
      "sourceText": "Jesteś już prawie wystarczająco dorosły, ale jeszcze nie. Szukaj dalej!",
      "sourceEnText": "You are almost old enough, but not yet. Keep looking!",
      "audioFile": "fr_02_0df97f8.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 4310,
      "updatedAt": "2026-01-12T06:29:01Z",
      "textHash": "2e50e6e99eb5cc07fdf2b9e22e1b4aed487f22172df18e8e80627fa64eab7f0a"
    },
    {
      "key": "0e1ff90",
      "nodeTitle": "schools_activity_done",
      "sourceText": "Przepraszamy! Spróbuj ponownie.",
      "sourceEnText": "Sorry! Try again.",
      "audioFile": "fr_02_0e1ff90.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1907,
      "updatedAt": "2026-01-12T06:29:34Z",
      "textHash": "84eff76902d357f7860e192c0512cf28c5ff092e2d29885648bd5ed038918858"
    },
    {
      "key": "0e3ad75",
      "nodeTitle": "task_backpack",
      "sourceText": "Znajdź swój plecak i wróć.",
      "sourceEnText": "Find your backpack and then come back.",
      "audioFile": "fr_02_0e3ad75.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2038,
      "updatedAt": "2026-01-12T06:30:31Z",
      "textHash": "01c720897233c2c1aa956f183e2e429b63e1bda80b011a4f0e6f8fd12e86a980"
    },
    {
      "key": "0e3f1fa",
      "nodeTitle": "activity_match_done",
      "sourceText": "Świetna robota!",
      "sourceEnText": "Great job!",
      "audioFile": "fr_02_0e3f1fa.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1620,
      "updatedAt": "2026-01-12T06:30:18Z",
      "textHash": "2dda8d176b2b2adb6db3949779e604e575ee141cef8a534fefc9ad8b089edaa0"
    },
    {
      "key": "0e4aa95",
      "nodeTitle": "classroom_3",
      "sourceText": "To nie twoje zajęcia, ale jesteś blisko!",
      "sourceEnText": "This is not your class, but you are close!",
      "audioFile": "fr_02_0e4aa95.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2769,
      "updatedAt": "2026-01-12T06:29:51Z",
      "textHash": "2e5535933a5984df17020b8bb48f42c69ef6c993ae6a76e0d6d06d7ed998b54c"
    },
    {
      "key": "0f65a1b",
      "nodeTitle": "quest_start",
      "sourceText": "Znajdź swoją szkołę i swoją klasę!",
      "sourceEnText": "Find your school and your classroom!",
      "audioFile": "fr_02_0f65a1b.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2534,
      "updatedAt": "2026-01-12T06:28:42Z",
      "textHash": "52e382b59a5dcdc208ce05f36708199bb25ab3bb8edaef1fe1463f7c1c3876be"
    },
    {
      "key": "0f6b039",
      "nodeTitle": "spawned_teacher",
      "sourceText": "Zajęcia w szkole trwają od 8:00 do 16:00.",
      "sourceEnText": "School is from 8 a.m. to 4 p.m.",
      "audioFile": "fr_02_0f6b039.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 3187,
      "updatedAt": "2026-01-12T16:35:14Z",
      "textHash": "de3fa40cb326dcc527db828eaac7b57d9376677f230af5dde005442eb8f61356"
    },
    {
      "key": "0f8995f",
      "nodeTitle": "classroom_5",
      "sourceText": "We Francji uczymy się pisać pismem odręcznym.",
      "sourceEnText": "In France, we learn to write in cursive.",
      "audioFile": "fr_02_0f8995f.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3239,
      "updatedAt": "2026-01-12T06:30:09Z",
      "textHash": "f67f33bcf192157be07c98db42cddead1e45e2fd5cfd156e8cdfcc0a4ab890cc"
    },
    {
      "key": "0f90123",
      "nodeTitle": "classroom_5",
      "sourceText": "Zacznijmy.",
      "sourceEnText": "Let's start.",
      "audioFile": "fr_02_0f90123.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1202,
      "updatedAt": "2026-01-12T06:30:04Z",
      "textHash": "440e189357a85cc63dec6f8f76b1df8d487a1ded70f3c3bd7601c69f843f1859"
    }
  ]
