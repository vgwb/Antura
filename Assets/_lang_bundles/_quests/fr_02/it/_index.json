    },
    {
      "key": "02797f8",
      "nodeTitle": "classroom_5",
      "sourceText": "Ecco fatto! Benvenuti su CM2!",
      "sourceEnText": "There you are! Welcome to CM2!",
      "audioFile": "fr_02_02797f8.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 2273,
