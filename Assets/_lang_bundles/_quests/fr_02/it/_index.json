    {
      "key": "0624437",
      "nodeTitle": "school_2_talk",
      "sourceText": "Ecco fatto! Entriamo e iniziamo la lezione.",
      "sourceEnText": "There it is! Let's go inside and start the lesson.",
      "audioFile": "fr_02_0624437.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 2769,
