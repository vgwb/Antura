    {
      "key": "022962d",
      "nodeTitle": "quest_end",
      "sourceText": "Il gioco è completato! Congratulazioni!",
      "sourceEnText": "The game is complete! Congratulations!",
      "audioFile": "fr_02_022962d.ogg",
      "voiceProfile": "francesco",
      "actorId": "NARRATOR",
