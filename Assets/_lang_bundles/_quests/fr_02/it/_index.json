      "textHash": "5957765cf5663dbad3c5ed37ca675eeb5a2c2bcdd87b7be3357321be4da36289"
    },
    {
      "key": "0630d14",
      "nodeTitle": "school_4",
      "sourceText": "Sei troppo giovane. Prova un'altra scuola.",
      "sourceEnText": "You are too young. Try another school.",
      "audioFile": "fr_02_0630d14.ogg",
      "voiceProfile": "francesco",
