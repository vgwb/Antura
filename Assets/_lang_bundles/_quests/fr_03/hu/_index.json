  "lines": [
    {
      "key": "00b1d04",
      "nodeTitle": "spawned_guide",
      "sourceText": "Tudod, ki Jules Verne?",
      "sourceEnText": "Do you know who Jules Verne is?",
      "audioFile": "fr_03_00b1d04.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1384,
      "updatedAt": "2025-11-24T09:30:17Z",
      "textHash": "659ea08763f2329c3a7b72b0e0e5e3f05c7847da06f109c52c103d69692b1fea"
    },
    {
      "key": "00be1f0",
      "nodeTitle": "spawned_kid_visitor",
      "sourceText": "Egyszer szeretnék lufizni.",
      "sourceEnText": "I want to ride a BALLOON one day.",
      "audioFile": "fr_03_00be1f0.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 1672,
      "updatedAt": "2025-11-24T09:30:07Z",
      "textHash": "bf51db80603e5f46e300b6556ace80d942003509471c3344d1f2ebfe3c361ca5"
    },
    {
      "key": "06e1473",
      "nodeTitle": "spawned_guide",
      "sourceText": "KÖNYVEIBEN szereplő számos ötlet valósággá vált.",
      "sourceEnText": "Many ideas in his BOOKS became real.",
      "audioFile": "fr_03_06e1473.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3239,
      "updatedAt": "2025-11-24T09:30:12Z",
      "textHash": "27248bf30c784f999796c3043d02ce2ecd6a537fe53c5134780db9166d98f91c"
    },
    {
      "key": "06fb1cd",
      "nodeTitle": "spawned_guide",
      "sourceText": "Nem, ki ő?",
      "sourceEnText": "No, who is he?",
      "audioFile": "fr_03_06fb1cd.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 862,
      "updatedAt": "2025-11-24T09:30:20Z",
      "textHash": "381a33ef6deb94e7a87a7cd7cbf38d90706c654c0ef8b213b31367682f5d1119"
    },
    {
      "key": "07ee86b",
      "nodeTitle": "spawned_kid_visitor",
      "sourceText": "A HOLDRA SZÁLLÍTÓ RAKÉTA gyorsnak tűnik.",
      "sourceEnText": "The ROCKET to the MOON looks fast.",
      "audioFile": "fr_03_07ee86b.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 2534,
      "updatedAt": "2025-11-24T09:30:10Z",
      "textHash": "4c76a9d1f37ac5943a4b71da097232ef8b3ee814215ee3efd5d679f809d8ba1d"
    },
    {
      "key": "07f7b4b",
      "nodeTitle": "spawned_guide",
      "sourceText": "Igen, ő ÍRÓ!",
      "sourceEnText": "Yes, he is a WRITER!",
      "audioFile": "fr_03_07f7b4b.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1149,
      "updatedAt": "2025-11-24T09:30:18Z",
      "textHash": "75ef88573d381e49c9d902d58f685338265dea1ebf455f1a6a97474e48bb4db7"
    },
    {
      "key": "08669ce",
      "nodeTitle": "spawned_kid_visitor",
      "sourceText": "A Nautilus tengeralattjáró lenyűgöző.",
      "sourceEnText": "The SUBMARINE Nautilus is amazing.",
      "audioFile": "fr_03_08669ce.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 2403,
      "updatedAt": "2025-11-24T09:30:14Z",
      "textHash": "e3c07f1e0126e358c356c347d47b6588a080b7fac8bc1375e1516efa0daf5bc5"
    },
    {
      "key": "0c790cf",
      "nodeTitle": "spawned_guide",
      "sourceText": "Jules Verne francia ÍRÓ volt. Itt született a közelben.",
      "sourceEnText": "Jules Verne was a French WRITER. He was born near here.",
      "audioFile": "fr_03_0c790cf.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3239,
      "updatedAt": "2025-11-24T09:30:22Z",
      "textHash": "2b7f9ef93dba9bfac75ad3b30fd72917246db7ea329c09ec2a32a3e8052c13ba"
    },
    {
      "key": "0e81901",
      "nodeTitle": "spawned_kid_visitor",
      "sourceText": "Tetszik a VILÁGról szóló történet.",
      "sourceEnText": "I like the story about the WORLD.",
      "audioFile": "fr_03_0e81901.ogg",
      "voiceProfile": "johnny",
      "actorId": "KID_M",
      "durationMs": 2168,
      "updatedAt": "2025-11-24T09:30:05Z",
      "textHash": "33c7256c9b8a99a913e13f66e27a7a27a9042cca4d0c7af4ade6efa06d1025a0"
    },
    {
      "key": "0f92a05",
      "nodeTitle": "spawned_guide",
      "sourceText": "Nagyon híres kalandkönyveket írt.",
      "sourceEnText": "He wrote very famous adventure BOOKS.",
      "audioFile": "fr_03_0f92a05.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1802,
      "updatedAt": "2025-11-24T09:30:24Z",
      "textHash": "93412c2272ccc3299a4e8bb10ae9bfe6be618a8cc72d67c71bdebf529caf9561"
    }
  ]
