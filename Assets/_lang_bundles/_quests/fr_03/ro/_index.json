      "textHash": "9b5616924a44320f1cce9f8f8a8b46c4739529143723758b6d486234eb370e99"
    },
    {
      "key": "06fb1cd",
      "nodeTitle": "spawned_guide",
      "sourceText": "Nu, cine este el?",
      "sourceEnText": "No, who is he?",
      "audioFile": "fr_03_06fb1cd.ogg",
      "voiceProfile": "cristi",
      "actorId": "ADULT_M",
      "durationMs": 1280,
