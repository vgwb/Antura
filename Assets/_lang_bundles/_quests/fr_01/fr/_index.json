      "textHash": "16d39a88684301222d22c0e8b4254048d2502db43500d646284c948ac1dbd6b0"
    },
    {
      "key": "talk_tutor_0a",
      "nodeTitle": "talk_tutor",
      "sourceText": "Êtes-vous déjà venu ici ?",
      "sourceEnText": "Have you ever been here?",
      "audioFile": "fr_01_talk_tutor_0a.ogg",
      "voiceProfile": "clement_fr",
      "actorId": "NARRATOR",
      "durationMs": 1347,
      "updatedAt": "2026-01-27T22:57:59Z",
      "textHash": "f0daa86d6050dc59c148cda4aa2868c5c741ab92e6da902c23996b49da4128f8"
    },
    {
      "key": "talk_tutor_0b",
      "nodeTitle": "talk_tutor",
      "sourceText": "Oui!",
      "sourceEnText": "Yes!",
      "audioFile": "fr_01_talk_tutor_0b.ogg",
      "voiceProfile": "clement_fr",
      "actorId": "NARRATOR",
      "durationMs": 557,
