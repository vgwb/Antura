    },
    {
      "key": "talk_tutor_0",
      "nodeTitle": "talk_tutor",
      "sourceText": "Bonjour et bienvenue à Paris !",
      "sourceEnText": "Hallo and welcome to Paris!",
      "audioFile": "fr_01_talk_tutor_0.ogg",
      "voiceProfile": "clement_fr",
      "actorId": "NARRATOR",
