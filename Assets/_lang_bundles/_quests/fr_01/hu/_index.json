{
  "version": "1",
  "questId": "fr_01",
  "lang": "hu",
  "lines": [
    {
      "key": "002620f",
      "nodeTitle": "post_quest_activity",
      "sourceText": "Le akarod rajzolni az Eiffel-tornyot?",
      "sourceEnText": "Do you want to draw the Eiffel Tower?",
      "audioFile": "fr_01_002620f.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:01:53Z",
      "textHash": "b3ebdb579b025c6b7e61ca9d667841d8d865e58de91e2a30819e9843e813dd57"
    },
    {
      "key": "002796f",
      "nodeTitle": "spawned_man",
      "sourceText": "Mit csinálsz?",
      "sourceEnText": "What are you doing?",
      "audioFile": "fr_01_002796f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1022,
      "updatedAt": "2026-01-28T02:00:42Z",
      "textHash": "de93432bbdc6e3d65cc9b3f2fdbb4e2570ef4be84a2c332538e5e31ab21a7f0a"
    },
    {
      "key": "006fcf2",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Jó látogatást!",
      "sourceEnText": "Enjoy your visit!",
      "audioFile": "fr_01_006fcf2.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1115,
      "updatedAt": "2026-01-28T02:00:15Z",
      "textHash": "c0641c92e930386fabe69afe4a635a77c36c31d959fe62efd9309f9f3e5826d8"
    },
    {
      "key": "0070084",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Helló. Mit szeretnél tudni?",
      "sourceEnText": "Hello. What do you want to know?",
      "audioFile": "fr_01_0070084.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1672,
      "updatedAt": "2026-01-28T02:00:21Z",
      "textHash": "0fbbd8f52edb161bd2dfeb975038cda3141c4327385e8c6951aa5905fe09f410"
    },
    {
      "key": "0097a65",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Nézz körül, és gyűjts össze érméket.",
      "sourceEnText": "Look around and pick up coins.",
      "audioFile": "fr_01_0097a65.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:01:09Z",
      "textHash": "ff93d2ee970926f09b5adb7dee28a314f9fb31a037d926b54818aedf51ba1ca8"
    },
    {
      "key": "00f1d2f",
      "nodeTitle": "baguette_salt",
      "sourceText": "Sót találtál.",
      "sourceEnText": "You found salt.",
      "audioFile": "fr_01_00f1d2f.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1300,
      "updatedAt": "2026-01-28T02:00:06Z",
      "textHash": "a400ec7b0622a958f36fca377a7a605490f98f0a5351ed5cf8fb96011f6ec739"
    },
    {
      "key": "0141851",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Jegy az Eiffel-toronyba való feljutáshoz.",
      "sourceEnText": "A ticket to go up the Eiffel Tower.",
      "audioFile": "fr_01_0141851.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2508,
      "updatedAt": "2026-01-28T02:00:51Z",
      "textHash": "645062e63830a5b5ef1b804eb1a7523e74deae2636c9fd8d3a5848cf27eced25"
    },
    {
      "key": "01fa210",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Franciaország nagy születésnapját ünnepelték.",
      "sourceEnText": "It was to celebrate a big birthday for France.",
      "audioFile": "fr_01_01fa210.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2833,
      "updatedAt": "2026-01-28T02:02:01Z",
      "textHash": "d538d31a61545c57fe1b0e62c71799904278aa95c90c05ffe0fd6d2a8d687c89"
    },
    {
      "key": "025865d",
      "nodeTitle": "baguette_yeast",
      "sourceText": "Élesztőt találtál!",
      "sourceEnText": "You found yeast!",
      "audioFile": "fr_01_025865d.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1486,
      "updatedAt": "2026-01-28T02:00:11Z",
      "textHash": "6ad29217f74643f441b46bf12b4c91bc10dc70530d21e5117f64b24b0bfc6425"
    },
    {
      "key": "02b627d",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Párizsban vagyunk.",
      "sourceEnText": "We are in Paris.",
      "audioFile": "fr_01_02b627d.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1115,
      "updatedAt": "2026-01-28T02:00:29Z",
      "textHash": "ae50ee603bba2db42f0a997168305b104c0e0a8a092ee3d480488634f9a56352"
    },
    {
      "key": "03dc852",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Egy bagett.",
      "sourceEnText": "A baguette.",
      "audioFile": "fr_01_03dc852.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1022,
      "updatedAt": "2026-01-28T01:59:35Z",
      "textHash": "77180f540d09a00c96be17bd723ddd83e5354dc8087c5702853cbcc14595dfcc"
    },
    {
      "key": "041403d",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "Jó szórakozást!",
      "sourceEnText": "Bonjour!",
      "audioFile": "fr_01_041403d.ogg",
      "voiceProfile": "shelley",
      "actorId": "KID_F",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:02:04Z",
      "textHash": "038ed9e17f75c6ed87598102223a4b5361c6db56b4fb6bc42b565c39317e51fd"
    },
    {
      "key": "04986a3",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "Ca va?",
      "sourceEnText": "Ca va?",
      "audioFile": "fr_01_04986a3.ogg",
      "voiceProfile": "shelley",
      "actorId": "KID_F",
      "durationMs": 697,
      "updatedAt": "2026-01-28T02:02:05Z",
      "textHash": "f2ea51e57e73e29fecf4439f2b4b8ea4c3bf99a688993ef5b9ac0e79a7daba27"
    },
    {
      "key": "04e74ad",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Itt a jegyed.",
      "sourceEnText": "Here is your ticket.",
      "audioFile": "fr_01_04e74ad.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1022,
      "updatedAt": "2026-01-28T01:59:43Z",
      "textHash": "2f7d516c63dce67512a43206dc3e8a37910601abec64cf7a70698e7a8df4e9c0"
    },
    {
      "key": "0585a5e",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Menj lifttel, vagy használd a lépcsőt!",
      "sourceEnText": "Take the elevator or use the stairs!",
      "audioFile": "fr_01_0585a5e.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T01:59:46Z",
      "textHash": "c598639188ffc0b1387961118fdf9763a6fa3525bf758438f29e7c0af5064b60"
    },
    {
      "key": "05a38a8",
      "nodeTitle": "spawned_man",
      "sourceText": "Megyek kenyeret venni a pékségbe.",
      "sourceEnText": "I am going to buy bread at the bakery.",
      "audioFile": "fr_01_05a38a8.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T02:00:45Z",
      "textHash": "f02a1761ad5dda4fed821318a295df563de0597bc8a70dad127c7de0e75f7efd"
    },
    {
      "key": "05eabcf",
      "nodeTitle": "spawned_man",
      "sourceText": "Honnan származol?",
      "sourceEnText": "Where do you come from?",
      "audioFile": "fr_01_05eabcf.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:00:47Z",
      "textHash": "02db8b00d9f71a320dfe0c250a44e861090c91fb364a431ca0b85f038d2a4d7b"
    },
    {
      "key": "06022b0",
      "nodeTitle": "baguette_flour",
      "sourceText": "Lisztet találtál!",
      "sourceEnText": "You found flour!",
      "audioFile": "fr_01_06022b0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1161,
      "updatedAt": "2026-01-28T02:00:08Z",
      "textHash": "f250f20026e77c699e009681ca9f13488a49195d245444bb4ca5dc662a7f8d0d"
    },
    {
      "key": "0635a6a",
      "nodeTitle": "spawned_man",
      "sourceText": "Nem ebben az országban születtem.",
      "sourceEnText": "I was not born in this country.",
      "audioFile": "fr_01_0635a6a.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:00:48Z",
      "textHash": "c37e4077d3c67589e5e7062171d61b0176ec653c804e6561791ad10b2bb3a39a"
    },
    {
      "key": "069cbb3",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "A jegy ára 5 fillér",
      "sourceEnText": "The ticket costs 5 coins.",
      "audioFile": "fr_01_069cbb3.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1393,
      "updatedAt": "2026-01-28T01:59:48Z",
      "textHash": "36e3211c9d4bad4412bc60c940172f2b4575818b6f9d1d06bddb351b7772b4a0"
    },
    {
      "key": "06cccc0",
      "nodeTitle": "talk_baker",
      "sourceText": "Most szükségem van a sóra.",
      "sourceEnText": "Now I need the salt.",
      "audioFile": "fr_01_06cccc0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T01:59:31Z",
      "textHash": "705a8fb127e976109347d334701c6afa03932d1c12f1a6248f5ce5aaf328d2d3"
    },
    {
      "key": "06f3fa2",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Ez a Notre-Dame katedrális.",
      "sourceEnText": "This is Notre-Dame Cathedral.",
      "audioFile": "fr_01_06f3fa2.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1811,
      "updatedAt": "2026-01-28T01:59:51Z",
      "textHash": "50d98e7c5432b05021441678643729d8b22d02e34fbd88a58780cffbb5e55003"
    },
    {
      "key": "06f3fa2_cathedral",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "A katedrális jelentése: egy nagyon nagy templom.",
      "sourceEnText": "A cathedral is a very big church.",
      "audioFile": "fr_01_06f3fa2_cathedral.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3529,
      "updatedAt": "2026-01-28T02:00:53Z",
      "textHash": "11eafdb6e7fe6262af8fbcdbef574236d07b173581ed7b078173f1cd76bce31b"
    },
    {
      "key": "0718e4a",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Csak hogy körülnézzek.",
      "sourceEnText": "Just to look around.",
      "audioFile": "fr_01_0718e4a.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1393,
      "updatedAt": "2026-01-28T02:01:56Z",
      "textHash": "5f65bd90a757d3197c30777abbeb9067344590f69e2d1a274300625bb5902bfa"
    },
    {
      "key": "076b3e3",
      "nodeTitle": "baker_finish",
      "sourceText": "Lássuk, emlékszel-e arra, amit Párizsban tanultál.",
      "sourceEnText": "Let's see if you remember what you learned in Paris.",
      "audioFile": "fr_01_076b3e3.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3019,
      "updatedAt": "2026-01-28T02:02:08Z",
      "textHash": "0fb9ff966c670bfa40821a3bbc32a6d992d61958d8c10e6656dd1ce422643beb"
    },
    {
      "key": "07a113f",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Ez Párizs különleges jelképe!",
      "sourceEnText": "It is a special symbol of Paris!",
      "audioFile": "fr_01_07a113f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:00:26Z",
      "textHash": "5bddca4c7a6967989b57247052d13a49c3f08f5750f84b13dd0f61b4afca82f5"
    },
    {
      "key": "07b94e9",
      "nodeTitle": "spawned_man",
      "sourceText": "Vannak kérdései?",
      "sourceEnText": "Do you have questions?",
      "audioFile": "fr_01_07b94e9.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1300,
      "updatedAt": "2026-01-28T02:00:36Z",
      "textHash": "fabfedca2444f8631a98b7a4c8acd89da9d37aa149c5798aaaec562f5b241a4b"
    },
    {
      "key": "083dfcc",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Menj fel a tetőre azzal a portállal!",
      "sourceEnText": "Go up to the roof through that portal!",
      "audioFile": "fr_01_083dfcc.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T01:59:52Z",
      "textHash": "307b9a07014c135b0fd832ff5609a66f3b3d7929d36d4723eb17655fa0f055dc"
    },
    {
      "key": "08654e6",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Igen! Miért kérdezed?",
      "sourceEnText": "Yes! Why do you ask?",
      "audioFile": "fr_01_08654e6.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T02:00:32Z",
      "textHash": "82322370752e59e390821dde08402af7866aa5c1f271225f651582f0f169ad58"
    },
    {
      "key": "089abda",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Láttam Anturát felmenni a torony tetejére.",
      "sourceEnText": "I saw Antura going up to the top of the tower.",
      "audioFile": "fr_01_089abda.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2461,
      "updatedAt": "2026-01-28T01:59:45Z",
      "textHash": "674cf0457cf69bd08182832335232262a6ed2b4ea2c54b316348ed3f7529bdb1"
    },
    {
      "key": "08bede4",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Valódi ez a hely?",
      "sourceEnText": "Is this place real?",
      "audioFile": "fr_01_08bede4.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1161,
      "updatedAt": "2026-01-28T02:00:30Z",
      "textHash": "ac41ae395c44a00f0a2d64ed661bf91ed35d232b56213368da866ea410da016a"
    },
    {
      "key": "08c1973",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Az Eiffel-torony 300 méter magas.",
      "sourceEnText": "The Eiffel Tower is 300 meters tall.",
      "audioFile": "fr_01_08c1973.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T01:59:37Z",
      "textHash": "2416f1b8a8ac79772b4f16d27710fda57f231efabb566c9c1605c906317a4650"
    },
    {
      "key": "09a0ead",
      "nodeTitle": "talk_notre_dame_roof",
      "sourceText": "2019-ben volt egy nagy tűzeset, de helyrehoztuk.",
      "sourceEnText": "There was a big fire in 2019, but it is fixed now.",
      "audioFile": "fr_01_09a0ead.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3344,
      "updatedAt": "2026-01-28T02:00:12Z",
      "textHash": "62fe176e25b6f82738e2171da8375d5b9399fe2882a1b8f1221d0c481e441fd0"
    },
    {
      "key": "09be864",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Jegy kell a feljutáshoz.",
      "sourceEnText": "You need a ticket to go up.",
      "audioFile": "fr_01_09be864.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1440,
      "updatedAt": "2026-01-28T02:01:59Z",
      "textHash": "79e0513a5c90a2632d33010d5dbb8a5f9cd1cc863db8c8ef35b9c3cd3680472c"
    },
    {
      "key": "09dd1da",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Hol vagyunk?",
      "sourceEnText": "Where are we?",
      "audioFile": "fr_01_09dd1da.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 836,
      "updatedAt": "2026-01-28T02:00:27Z",
      "textHash": "0331bbf4c42092ce03e975b419ffe5860b11cbb3570b10c71ba9ea5ee1e38e9f"
    },
    {
      "key": "09e454b",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Jó reggelt. Mit szeretne?",
      "sourceEnText": "Good morning. What do you want?",
      "audioFile": "fr_01_09e454b.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1440,
      "updatedAt": "2026-01-28T01:59:49Z",
      "textHash": "7ee22be3a7f43865cfc4cacfbe4174f0e066ee6f2491d637e1249205c112b6a8"
    },
    {
      "key": "09e5c3b",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Gustave Eiffel építtette 1887-ben.",
      "sourceEnText": "Gustave Eiffel built it in 1887.",
      "audioFile": "fr_01_09e5c3b.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2554,
      "updatedAt": "2026-01-28T01:59:39Z",
      "textHash": "b1824c6f47774b698d02993801e83fa157aa6aa8c618cfe965b74db3341abecd"
    },
    {
      "key": "0a3f4e1",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "1889-ben nagy vásár volt Párizsban.",
      "sourceEnText": "There was a big fair in Paris in 1889.",
      "audioFile": "fr_01_0a3f4e1.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2554,
      "updatedAt": "2026-01-28T02:01:30Z",
      "textHash": "508d4db616fbe95c0718b31c6beb3a8657ba5388010e8e8b9962417da9978a3b"
    },
    {
      "key": "0ac5a72",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Ez egy nagyon régi templom. Régen épült.",
      "sourceEnText": "It is a very old church. It was built long ago.",
      "audioFile": "fr_01_0ac5a72.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2740,
      "updatedAt": "2026-01-28T02:01:08Z",
      "textHash": "59d34b0ece3723829588a1ea67fcc904ce997f1f4b622800117568bfd6f82d71"
    },
    {
      "key": "0aee9bb",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Szeretnék felmenni az Eiffel-toronyba.",
      "sourceEnText": "I would like to go up the Eiffel Tower.",
      "audioFile": "fr_01_0aee9bb.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:01:58Z",
      "textHash": "858152be3a5c9e334df3b9dd25da004908aff2d53325fcf05bba4693c9c7b254"
    },
    {
      "key": "0ba42cd",
      "nodeTitle": "coins_collected",
      "sourceText": "Most már van elég érméd a jegy megvásárlásához.",
      "sourceEnText": "You have enough coins now to buy the ticket.",
      "audioFile": "fr_01_0ba42cd.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2694,
      "updatedAt": "2026-01-28T02:00:56Z",
      "textHash": "44fb7039ad9e03e46ab5f16b0378eb1904eb1a929366e9b2955bde693fa8dd01"
    },
    {
      "key": "0bc62a3",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Hát... úgy néz ki, mint egy videójáték.",
      "sourceEnText": "Well... it looks like a video game.",
      "audioFile": "fr_01_0bc62a3.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T02:00:33Z",
      "textHash": "e4a27726f777237cb368b22dd86d8e869449cfe148880598e77419539c342ce2"
    },
    {
      "key": "0bda18a",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Imádom Párizst!",
      "sourceEnText": "I love Paris!",
      "audioFile": "fr_01_0bda18a.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1115,
      "updatedAt": "2026-01-28T02:02:02Z",
      "textHash": "2fdad61a1294b587cdb50ad0b677fe1a7d3974aaf048cce1b6558f48e1a28522"
    },
    {
      "key": "0c4d1f6",
      "nodeTitle": "baguette_water",
      "sourceText": "Ez víz.",
      "sourceEnText": "This is water.",
      "audioFile": "fr_01_0c4d1f6.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 929,
      "updatedAt": "2026-01-28T02:00:09Z",
      "textHash": "f6b08bc7f500973031e15c45079b7355eb09e47b399bfdf410a0074988717352"
    },
    {
      "key": "0cbdcce",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Van itt a közelben egy pékség. De később nyit.",
      "sourceEnText": "There is a bakery near here. But it opens later.",
      "audioFile": "fr_01_0cbdcce.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2694,
      "updatedAt": "2026-01-28T02:00:01Z",
      "textHash": "d8a5cf4f21500122e8765b9e5e401477996186269158d4e22abc16bdbac9bdb1"
    },
    {
      "key": "0d11596",
      "nodeTitle": "quest_end",
      "sourceText": "Gratulálok!",
      "sourceEnText": "Congratulations!",
      "audioFile": "fr_01_0d11596.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 975,
      "updatedAt": "2026-01-28T01:59:54Z",
      "textHash": "f18ab1d1ef61c0be05b8aeb3ccb5d19f0f5284e5ce6a9c8d5918e040cbf2bdea"
    },
    {
      "key": "0d11596a",
      "nodeTitle": "quest_end",
      "sourceText": "Tetszett neked Párizs?",
      "sourceEnText": "Did you like Paris?",
      "audioFile": "fr_01_0d11596a.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1347,
      "updatedAt": "2026-01-28T02:01:12Z",
      "textHash": "b5583af35c092d5830bc2f9e162fd781f120cb29d4c325de2e711037b95c020d"
    },
    {
      "key": "0d59ade",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Vasból van!",
      "sourceEnText": "It is made of iron!",
      "audioFile": "fr_01_0d59ade.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1068,
      "updatedAt": "2026-01-28T01:59:40Z",
      "textHash": "a837318e9e89c819cb06eb28ec75713b293e85099565765a12525ac004819a8c"
    },
    {
      "key": "0d59ade_fair",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Réges-régen egy nagy vásárra építették.",
      "sourceEnText": "It was built for a big fair long ago.",
      "audioFile": "fr_01_0d59ade_fair.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2508,
      "updatedAt": "2026-01-28T02:00:55Z",
      "textHash": "fd280d6c9cba2f8e9760b138c16e5e372dfa838fc469c55e793bd46016a2dd13"
    },
    {
      "key": "0d6f3c4",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Az Eiffel-tornyot arra a nagy bulira építették.",
      "sourceEnText": "The Eiffel Tower was built for that big party.",
      "audioFile": "fr_01_0d6f3c4.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2740,
      "updatedAt": "2026-01-28T02:01:31Z",
      "textHash": "ade5889071d6997c3746770ed79cb6ca29a8d16186f761e3481a9ad2511e7543"
    },
    {
      "key": "0d91dc0",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Mi az Eiffel-torony?",
      "sourceEnText": "What is the Eiffel Tower?",
      "audioFile": "fr_01_0d91dc0.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:00:23Z",
      "textHash": "e20a60b4c6b21eb9ffe6c355ed4b14755db906e7e391ef57b693a6a02135b5dc"
    },
    {
      "key": "0da46e8",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Isten hozott az Eiffel-torony tetején!",
      "sourceEnText": "Welcome to the top of the Eiffel Tower!",
      "audioFile": "fr_01_0da46e8.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:02:10Z",
      "textHash": "71b6564235e1cfdeda8738a4ede689ffccc121192d23d4500913913bba701a72"
    },
    {
      "key": "0ee51fc",
      "nodeTitle": "spawned_man",
      "sourceText": "Búcsú",
      "sourceEnText": "Goodbye",
      "audioFile": "fr_01_0ee51fc.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 604,
      "updatedAt": "2026-01-28T02:00:50Z",
      "textHash": "8e5e3c377d702867ae177ab55db615e0bca23a545e68b86067a369f9d89e0f1c"
    },
    {
      "key": "0f17af0",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Egy magas vastorony, körülbelül 300 méter magas.",
      "sourceEnText": "A tall iron tower, about 300 meters high.",
      "audioFile": "fr_01_0f17af0.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 3065,
      "updatedAt": "2026-01-28T02:00:24Z",
      "textHash": "2a2d714a966cd8986e1a90aef662702d1377d584f3d120fb532622cef6321a5f"
    },
    {
      "key": "0f18ad3",
      "nodeTitle": "spawned_man",
      "sourceText": "Láttad már Anturát?",
      "sourceEnText": "Have you seen Antura?",
      "audioFile": "fr_01_0f18ad3.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1300,
      "updatedAt": "2026-01-28T02:00:38Z",
      "textHash": "382bb6e37cebe1a99443a198d33bf569511215419dec9776c351a15b6b3f25bc"
    },
    {
      "key": "0f7f9d8",
      "nodeTitle": "gargoyle",
      "sourceText": "Nézd azt a szobrot! Nem ijesztő?",
      "sourceEnText": "Look at that statue! Is it scary?",
      "audioFile": "fr_01_0f7f9d8.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1904,
      "updatedAt": "2026-01-28T02:01:55Z",
      "textHash": "02791aaca7fa5a32d5f48156341bb9d6307d57a2d0da097e1b832f45d7aee3a5"
    },
    {
      "key": "0f9dd62",
      "nodeTitle": "spawned_man",
      "sourceText": "Nem. Ki az az Antura?",
      "sourceEnText": "No. Who is Antura?",
      "audioFile": "fr_01_0f9dd62.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T02:00:41Z",
      "textHash": "f99ad3b3bc1c5ae7ad54f7e7ea25f1857822d6926fc052990686506075373583"
    },
    {
      "key": "0fe0732",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Semmi. Viszlát.",
      "sourceEnText": "Nothing. Bye.",
      "audioFile": "fr_01_0fe0732.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 1161,
      "updatedAt": "2026-01-28T02:00:35Z",
      "textHash": "072f5710d60bfbbe1dfb3129dbb8b77534ff54f4fb4160e5c2cfc6a22ecab1f8"
    },
    {
      "key": "already_solved",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Ezt a részt már megoldottuk.",
      "sourceEnText": "We already solved this part.",
      "audioFile": "fr_01_already_solved.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1672,
      "updatedAt": "2026-01-28T02:02:07Z",
      "textHash": "e6bd61587c30593bb8cfeb53241378d906f0454ef99443e3322e195cf6d1ab80"
    },
    {
      "key": "arc_0",
      "nodeTitle": "talk_arc",
      "sourceText": "Ez a Diadalív.",
      "sourceEnText": "This is the Arc de Triomphe.",
      "audioFile": "fr_01_arc_0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_F",
      "durationMs": 1115,
      "updatedAt": "2026-01-28T01:59:58Z",
      "textHash": "b758c2dae31489b99fb243dccb7dee821b8f5295dbc7e8567c90b77945bda4e9"
    },
    {
      "key": "arc_0a",
      "nodeTitle": "talk_arc",
      "sourceText": "Olyan emberek előtt tiszteleg, akik Franciaországért harcoltak.",
      "sourceEnText": "It honors people who fought for France.",
      "audioFile": "fr_01_arc_0a.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_F",
      "durationMs": 3576,
      "updatedAt": "2026-01-28T01:59:42Z",
      "textHash": "89f6aaf28542ef92272dee14241579598d349bcb5c7d01674e8b659d564aa85b"
    },
    {
      "key": "baker_0",
      "nodeTitle": "talk_baker",
      "sourceText": "Sziasztok! Én vagyok a pék. Minden nap kenyeret sütök.",
      "sourceEnText": "Hello! I am a baker. I make bread every day.",
      "audioFile": "fr_01_baker_0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3251,
      "updatedAt": "2026-01-28T02:01:19Z",
      "textHash": "d4d41e8c18d547a2c8dcfdfca53641133cda6bcf3da7df77f35b32e978261dbc"
    },
    {
      "key": "baker_1",
      "nodeTitle": "talk_baker",
      "sourceText": "Ma bagettet szeretnék sütni...",
      "sourceEnText": "Today I want to make a baguette...",
      "audioFile": "fr_01_baker_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:01:17Z",
      "textHash": "02b716aea86f04f47b1cd307fcae98e095469226ace4a4f3332dc377d0e20e9b"
    },
    {
      "key": "baker_2",
      "nodeTitle": "talk_baker",
      "sourceText": "De elvesztettem a hozzávalókat!",
      "sourceEnText": "But I lost the ingredients!",
      "audioFile": "fr_01_baker_2.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:00:44Z",
      "textHash": "33effe528c35e57dd1a0ea25a139d97bbc872e67a0e7206046abe8a196a2221e"
    },
    {
      "key": "baker_3",
      "nodeTitle": "talk_baker",
      "sourceText": "Egy nagy kék kutya ellopta a lisztemet!",
      "sourceEnText": "A big blue dog stole my flour!",
      "audioFile": "fr_01_baker_3.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T01:59:32Z",
      "textHash": "3a9730de51da2b8f806e130c73f390455b5f9f1dbe0e8b72a9fda5047b66bd17"
    },
    {
      "key": "baker_4",
      "nodeTitle": "talk_baker",
      "sourceText": "Tudsz segíteni megtalálni őket?",
      "sourceEnText": "Can you help me find them?",
      "audioFile": "fr_01_baker_4.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1904,
      "updatedAt": "2026-01-28T02:00:17Z",
      "textHash": "37995487f3bc64528db5c6fec3375e65d8e449eb13aa1d7ae6714ee3e003db8f"
    },
    {
      "key": "baker_5",
      "nodeTitle": "talk_baker",
      "sourceText": "Először is, menjünk az Eiffel-toronyhoz. Lisztre van szükségünk.",
      "sourceEnText": "Go to the Eiffel Tower.",
      "audioFile": "fr_01_baker_5.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3483,
      "updatedAt": "2026-01-28T02:00:05Z",
      "textHash": "f13efecefd8f1dc3ecb46565ab641590d69236b065b7c254f6aa4e2ac03188f3"
    },
    {
      "key": "baker_finish_0",
      "nodeTitle": "baker_finish",
      "sourceText": "Megtaláltad az összes hozzávalót!",
      "sourceEnText": "You found all the ingredients!",
      "audioFile": "fr_01_baker_finish_0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1950,
      "updatedAt": "2026-01-28T02:00:14Z",
      "textHash": "8bfa3fe67a29dfdccab69f268639b12b518c2dd4513237a46a904d9f7c48fd53"
    },
    {
      "key": "baker_finish_1",
      "nodeTitle": "baker_finish",
      "sourceText": "Most már tudok bagettet sütni.",
      "sourceEnText": "Now I can make a baguette.",
      "audioFile": "fr_01_baker_finish_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T02:00:20Z",
      "textHash": "c322b3b5b3a7045532dfa7972eb312475a83081d2788f71234040725dea83bfe"
    },
    {
      "key": "baker_finish_2",
      "nodeTitle": "final_activity_done",
      "sourceText": "Párizsban friss kenyér illata van!",
      "sourceEnText": "Paris smells like fresh bread!",
      "audioFile": "fr_01_baker_finish_2.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T02:00:39Z",
      "textHash": "dd0e5b242f3e53beadec5cf8a685069e308374269cfc2a62282d88ea2ba27fe6"
    },
    {
      "key": "baker_r1",
      "nodeTitle": "talk_baker",
      "sourceText": "Remek! Van lisztünk.",
      "sourceEnText": "Great! We have flour.",
      "audioFile": "fr_01_baker_r1.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1300,
      "updatedAt": "2026-01-28T02:01:34Z",
      "textHash": "e2a92bc3e43e2b8b05594af9eede913676e114722d9afb2492910200d98fece6"
    },
    {
      "key": "baker_r2",
      "nodeTitle": "talk_baker",
      "sourceText": "Menj a Notre-Dame-ba.",
      "sourceEnText": "Go to Notre-Dame.",
      "audioFile": "fr_01_baker_r2.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:01:33Z",
      "textHash": "f06f95c066eece96d04f99003d283b44917260eb2c88dec2f01f8e7a420dd1a7"
    },
    {
      "key": "baker_r3",
      "nodeTitle": "talk_baker",
      "sourceText": "Só! Szép munka!",
      "sourceEnText": "Salt! Well done.",
      "audioFile": "fr_01_baker_r3.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1440,
      "updatedAt": "2026-01-28T02:01:22Z",
      "textHash": "0bef9d8f1f6d3e602d5e6222a44f2ae6d88864f72e35f33c6303f99c410b1d67"
    },
    {
      "key": "baker_r4",
      "nodeTitle": "talk_baker",
      "sourceText": "Antura fogta a vizet, és elment a Louvre-ba.",
      "sourceEnText": "Antura took the water and went to the Louvre.",
      "audioFile": "fr_01_baker_r4.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:01:20Z",
      "textHash": "60eba9eded22c8de569bb5d6035f741f74ece3c2a88eb6364ed9115256f3fdd5"
    },
    {
      "key": "baker_r5",
      "nodeTitle": "talk_baker",
      "sourceText": "Tökéletes! Van vizünk.",
      "sourceEnText": "Perfect! We have water.",
      "audioFile": "fr_01_baker_r5.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1393,
      "updatedAt": "2026-01-28T02:01:15Z",
      "textHash": "7167112d9dd4ea4817c71bd54211e3c9757534ec6d18f6b8c7c3fda20c6c5cb2"
    },
    {
      "key": "baker_r6",
      "nodeTitle": "talk_baker",
      "sourceText": "Talán az élesztő a Diadalívnél van.",
      "sourceEnText": "Maybe the yeast is at the Arc de Triomphe.",
      "audioFile": "fr_01_baker_r6.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T02:01:14Z",
      "textHash": "b758b6cc35b244f94322dae2c8518644a29ed2c31c05cce9f276ddb2eaf44c45"
    },
    {
      "key": "chest_unlocked",
      "nodeTitle": "eiffell_activity_done",
      "sourceText": "A láda most már nyitva van.",
      "sourceEnText": "The chest is now unlocked.",
      "audioFile": "fr_01_chest_unlocked.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T01:59:55Z",
      "textHash": "4b96c95a3391a4825333f3167281d990aebefd986f55e4e6e33844d2980e0c80"
    },
    {
      "key": "come_back_later",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Gyere vissza később.",
      "sourceEnText": "Come back later.",
      "audioFile": "fr_01_come_back_later.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1347,
      "updatedAt": "2026-01-28T02:00:03Z",
      "textHash": "7707995b000a896277d16b1252fd2aff5238e5b692520e69af9571ea2fba783c"
    },
    {
      "key": "fr01_bakery_1",
      "nodeTitle": "npc_bakery",
      "sourceText": "A friss kenyér illata boldoggá teszi az embereket.",
      "sourceEnText": "Fresh bread smell makes people happy.",
      "audioFile": "fr_01_fr01_bakery_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2833,
      "updatedAt": "2026-01-28T02:01:48Z",
      "textHash": "513325975b8f0a7ed0b0538f930d05dca1823e18af3eae242d523e69ccf7a8dc"
    },
    {
      "key": "fr01_bakery_2",
      "nodeTitle": "npc_bakery",
      "sourceText": "A bagetthez lisztet, vizet, élesztőt és sót kell használni.",
      "sourceEnText": "A baguette needs flour, water, yeast, and salt.",
      "audioFile": "fr_01_fr01_bakery_2.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3251,
      "updatedAt": "2026-01-28T02:01:50Z",
      "textHash": "dced2a37c9af507fabe0ea72597236883e2f78c0f51f772ace5f532547cc19b6"
    },
    {
      "key": "fr01_bakery_2a",
      "nodeTitle": "npc_bakery",
      "sourceText": "Az élesztő segíti a kenyér kelését.",
      "sourceEnText": "Yeast helps bread rise.",
      "audioFile": "fr_01_fr01_bakery_2a.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:01:47Z",
      "textHash": "c770022f78209cde50635f8cd8aeb933d00f6caa560917b57fb2fb115e668276"
    },
    {
      "key": "fr01_bakery_3",
      "nodeTitle": "npc_bakery",
      "sourceText": "A pékek nagyon korán kelnek, hogy elkezdjék a kenyérsütést.",
      "sourceEnText": "Bakers wake up very early to start making bread.",
      "audioFile": "fr_01_fr01_bakery_3.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3204,
      "updatedAt": "2026-01-28T02:01:52Z",
      "textHash": "10c81824219b4a314d83622395fe7f6d4617c9f96c7806bb4624c0dc444ddea4"
    },
    {
      "key": "fr01_eiffel_elevator_1",
      "nodeTitle": "npc_eiffell_elevator",
      "sourceText": "A lift segít az embereknek feljutni a toronyba.",
      "sourceEnText": "The elevator helps people go up the tower.",
      "audioFile": "fr_01_fr01_eiffel_elevator_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:00:58Z",
      "textHash": "fec5e5c8706078b59e00bcb55039a22b97877e0314bc59d1200f7388ea80dc04"
    },
    {
      "key": "fr01_notredame_base_3",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "A templom egy olyan hely, ahová az emberek imádkozni járnak.",
      "sourceEnText": "A church is a place where people go to pray.",
      "audioFile": "fr_01_fr01_notredame_base_3.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 3158,
      "updatedAt": "2026-01-28T02:01:42Z",
      "textHash": "b73c715c6e7b989ef80c740b9a569eda4512655abd670246e039d92496ae6df1"
    },
    {
      "key": "fr01_notredame_roof_1",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "A tetőről Párizs nagy részét be lehet látni.",
      "sourceEnText": "From the roof you can see much of Paris.",
      "audioFile": "fr_01_fr01_notredame_roof_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:01:44Z",
      "textHash": "890cc835df821166c7cc4b239dd2fa4a40ebde4baafca1798e51961c3fb2154c"
    },
    {
      "key": "fr01_notredame_roof_2",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Gargoyle-oknak nevezett kőlények ülnek itt.",
      "sourceEnText": "Stone creatures called gargoyles sit up here.",
      "audioFile": "fr_01_fr01_notredame_roof_2.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2461,
      "updatedAt": "2026-01-28T02:01:45Z",
      "textHash": "9bd0eec6bc8a8d117203a8caf394fffa0ad492c7b2d2d10c6440b6f6dc7cdbf2"
    },
    {
      "key": "fr01_notredame_roof_2b",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Ezek a kőkarok segítenek megtartani a falakat.",
      "sourceEnText": "These stone arms help hold up the walls.",
      "audioFile": "fr_01_fr01_notredame_roof_2b.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2879,
      "updatedAt": "2026-01-28T02:01:01Z",
      "textHash": "7745d41dffd087bdc737cc0d0651873a837a6e7500a40bcd65bf94c6f051b4da"
    },
    {
      "key": "fr01_notredame_roof_2c",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "A nagy kerek ablakot rózsaablaknak hívják.",
      "sourceEnText": "The big round window is called a rose window.",
      "audioFile": "fr_01_fr01_notredame_roof_2c.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2368,
      "updatedAt": "2026-01-28T02:01:00Z",
      "textHash": "fdf09ac5387e3e153c6d7c257ad4100b1754ebf92d9c67222f49154616d2835d"
    },
    {
      "key": "fr01_region_1",
      "nodeTitle": "npc_paris_region",
      "sourceText": "Párizs az Île-de-France nevű régióban található.",
      "sourceEnText": "Paris is in a place called Île-de-France.",
      "audioFile": "fr_01_fr01_region_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_F",
      "durationMs": 2740,
      "updatedAt": "2026-01-28T02:01:28Z",
      "textHash": "38cd0fd475038c9247fa59d42db38dfa6bcb16f5b02f8f89005cdc4868c18f95"
    },
    {
      "key": "go_back_baker",
      "nodeTitle": "baguette_flour",
      "sourceText": "Menj vissza a cukrászdához.",
      "sourceEnText": "Go back to the baker.",
      "audioFile": "fr_01_go_back_baker.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T01:59:57Z",
      "textHash": "9f138874549da552a2391e6e1a77979710835aa8889b162c4963139baa6dec26"
    },
    {
      "key": "louvre_0",
      "nodeTitle": "talk_louvre",
      "sourceText": "Ez a Louvre, egy híres múzeum.",
      "sourceEnText": "This is the Louvre, a famous museum.",
      "audioFile": "fr_01_louvre_0.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_F",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T01:59:34Z",
      "textHash": "93d9cba323d76873dae9695d9f5eeb9193012d3f53674e1c6e35073973e4011f"
    },
    {
      "key": "puzzle_done",
      "nodeTitle": "final_activity_done",
      "sourceText": "Remek! Megoldottad a rejtvényt.",
      "sourceEnText": "Great! You solved the puzzle.",
      "audioFile": "fr_01_puzzle_done.ogg",
      "voiceProfile": "shelley",
      "actorId": "SENIOR_M",
      "durationMs": 1858,
      "updatedAt": "2026-01-28T02:00:00Z",
      "textHash": "2b7d36adbf5a559256f2f769c3dd10446ac04a9abc4a31765a7c182db4e613dd"
    },
    {
      "key": "solve_puzzle",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "A láda kinyitásához oldd meg a rejtvényt!",
      "sourceEnText": "To open the chest, solve the puzzle!",
      "audioFile": "fr_01_solve_puzzle.ogg",
      "voiceProfile": "shelley",
      "actorId": "Default",
      "durationMs": 2229,
      "updatedAt": "2026-01-28T02:00:18Z",
      "textHash": "dfff27ef59f2b3f6fe297cf4c14bed26026b241486c0dc9120078916609317da"
    },
    {
      "key": "start",
      "nodeTitle": "quest_start",
      "sourceText": "Párizsban vagyunk, Franciaország fővárosában.",
      "sourceEnText": "We are in Paris, the capital of France.",
      "audioFile": "fr_01_start.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 3204,
      "updatedAt": "2026-01-28T02:01:03Z",
      "textHash": "661e03b1d7245427d6eca946cea7eefb8bf128fc93e9c07534b6bd476d77b27b"
    },
    {
      "key": "start_1",
      "nodeTitle": "quest_start",
      "sourceText": "Ma az Eiffel-tornyot fogjuk felfedezni",
      "sourceEnText": "Today we will explore the Eiffel Tower",
      "audioFile": "fr_01_start_1.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:01:26Z",
      "textHash": "d1518c6d435ddce465db4d119909cf79497211e0731b6712e7f17945686253f5"
    },
    {
      "key": "start_1a",
      "nodeTitle": "quest_start",
      "sourceText": "és a Notre-Dame",
      "sourceEnText": "and Notre-Dame.",
      "audioFile": "fr_01_start_1a.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:01:25Z",
      "textHash": "032bfee3c1cefe8d56e51cea06f05dd6698c1a9aec8f145e063c229861381e72"
    },
    {
      "key": "start_1b",
      "nodeTitle": "quest_start",
      "sourceText": "de előbb együnk egy bagettet!",
      "sourceEnText": "But first, let's eat a baguette!",
      "audioFile": "fr_01_start_1b.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T02:01:23Z",
      "textHash": "ec55466790e57d41677adb3aecc95e5041ba46392226778e778999fc12d72e92"
    },
    {
      "key": "start_2",
      "nodeTitle": "quest_start",
      "sourceText": "Készen állsz? Kezdjük!",
      "sourceEnText": "Are you ready? Let's start!",
      "audioFile": "fr_01_start_2.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1486,
      "updatedAt": "2026-01-28T02:01:04Z",
      "textHash": "94d64080e8517f48d29d3bc53d01706859877c773569b4dc9505826bba2ef25f"
    },
    {
      "key": "talk_tutor",
      "nodeTitle": "talk_tutor",
      "sourceText": "Láttam Anturát elmenni a pékhez. Menjünk oda!",
      "sourceEnText": "I saw Antura go to the baker. Let's go there!",
      "audioFile": "fr_01_talk_tutor.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 2786,
      "updatedAt": "2026-01-28T02:01:06Z",
      "textHash": "e57f8db38261f36cdc0737306e44df4295d4edbbbca1086fd685e153ef9cc9e7"
    },
    {
      "key": "talk_tutor_0",
      "nodeTitle": "talk_tutor",
      "sourceText": "Helló, és üdv Párizsban!",
      "sourceEnText": "Hello! Have you ever been here in Paris!",
      "audioFile": "fr_01_talk_tutor_0.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1347,
      "updatedAt": "2026-01-28T02:01:11Z",
      "textHash": "daae40a590f4551d948e610b0b21982a9fd013d2f6dc835a1815a74c47e6f9c7"
    },
    {
      "key": "talk_tutor_0b",
      "nodeTitle": "talk_tutor",
      "sourceText": "Igen!",
      "sourceEnText": "Yes!",
      "audioFile": "fr_01_talk_tutor_0b.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 604,
      "updatedAt": "2026-01-28T02:01:40Z",
      "textHash": "db0dfaf89d933960b4c5ff041e41dcb3b2fc9f0941172f3a20ec1ba3b2e91b7d"
    },
    {
      "key": "talk_tutor_0c",
      "nodeTitle": "talk_tutor",
      "sourceText": "Remek! Lássuk, emlékszel-e ezekre a helyekre.",
      "sourceEnText": "Great! Let's see if you remember these places.",
      "audioFile": "fr_01_talk_tutor_0c.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 2368,
      "updatedAt": "2026-01-28T02:01:39Z",
      "textHash": "cb85a2aa1c80b21332698e32bf45a6051ab979eb49ab9e69aa90042b11b45b0c"
    },
    {
      "key": "talk_tutor_0d",
      "nodeTitle": "talk_tutor",
      "sourceText": "Nem.",
      "sourceEnText": "No.",
      "audioFile": "fr_01_talk_tutor_0d.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 650,
      "updatedAt": "2026-01-28T02:01:37Z",
      "textHash": "9fd4625fd03aabdee273ad1be973258741071282b5f0db16c9bb2156e5d1bd58"
    },
    {
      "key": "talk_tutor_0e",
      "nodeTitle": "talk_tutor",
      "sourceText": "Remélem, egyszer majd eljössz ide!",
      "sourceEnText": "I hope you can come here one day!",
      "audioFile": "fr_01_talk_tutor_0e.ogg",
      "voiceProfile": "shelley",
      "actorId": "NARRATOR",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:01:36Z",
      "textHash": "503cf9f45fe01f56fc827e52ae16180ff1d7761a29ade76a46c33f05c26f55cc"
    }
  ]
