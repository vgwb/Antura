{
  "version": "1",
  "questId": "fr_01",
  "lang": "pl",
  "lines": [
    {
      "key": "002620f",
      "nodeTitle": "post_quest_activity",
      "sourceText": "Chcesz narysować Wieżę Eiffla?",
      "sourceEnText": "Do you want to draw the Eiffel Tower?",
      "audioFile": "fr_01_002620f.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1950,
      "updatedAt": "2026-01-28T02:08:35Z",
      "textHash": "75dcb4e503f29f4a06f7a2e6ab7f08d7c1925ffb0a0f9c85d744f582e38f616f"
    },
    {
      "key": "002796f",
      "nodeTitle": "spawned_man",
      "sourceText": "Co robisz?",
      "sourceEnText": "What are you doing?",
      "audioFile": "fr_01_002796f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 836,
      "updatedAt": "2026-01-28T02:06:50Z",
      "textHash": "fc6dbe2aa4b079fac86a713cd35dc91a8b5b29b57ebd9c1751835550052043ae"
    },
    {
      "key": "006fcf2",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Życzymy miłej wizyty!",
      "sourceEnText": "Enjoy your visit!",
      "audioFile": "fr_01_006fcf2.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T02:06:11Z",
      "textHash": "e9d483d9d3f114e9f9402c94136107fc5988e9831306d7308d6679fbf8efd0b1"
    },
    {
      "key": "0070084",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Cześć. Co chcesz wiedzieć?",
      "sourceEnText": "Hello. What do you want to know?",
      "audioFile": "fr_01_0070084.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:06:20Z",
      "textHash": "703178e89d5af54d8ee14ca2e5901d758d1816cbe91d8fea8abc5e994edc3956"
    },
    {
      "key": "0097a65",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Rozejrzyj się i podnieś monety.",
      "sourceEnText": "Look around and pick up coins.",
      "audioFile": "fr_01_0097a65.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:07:28Z",
      "textHash": "624efee9f107dd43f075b099a64158e80d4c15ed1394c36571d9bc649310fb81"
    },
    {
      "key": "00f1d2f",
      "nodeTitle": "baguette_salt",
      "sourceText": "Znalazłeś sól.",
      "sourceEnText": "You found salt.",
      "audioFile": "fr_01_00f1d2f.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1393,
      "updatedAt": "2026-01-28T02:05:56Z",
      "textHash": "7d338acd1771ae7e9dea5a1bd51639edd8811b108a0eff685ec19ca7dea64bf9"
    },
    {
      "key": "0141851",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Bilet na wejście na Wieżę Eiffla.",
      "sourceEnText": "A ticket to go up the Eiffel Tower.",
      "audioFile": "fr_01_0141851.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2229,
      "updatedAt": "2026-01-28T02:07:01Z",
      "textHash": "241162365c473bdadb47409d7d371918a9bb49c9968f1462404b345566221082"
    },
    {
      "key": "01fa210",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Miało to na celu uczczenie wielkich urodzin Francji.",
      "sourceEnText": "It was to celebrate a big birthday for France.",
      "audioFile": "fr_01_01fa210.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3065,
      "updatedAt": "2026-01-28T02:08:45Z",
      "textHash": "ba94fd97c3cc03836971fc5035efbe60ec3b26c1a7db36a3002c69f5191d8e09"
    },
    {
      "key": "025865d",
      "nodeTitle": "baguette_yeast",
      "sourceText": "Znalazłeś drożdże!",
      "sourceEnText": "You found yeast!",
      "audioFile": "fr_01_025865d.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T02:06:03Z",
      "textHash": "4cdcc081b2103e540e864a0b380c1683761980d02bdfbf2a3aa58deda09f86b2"
    },
    {
      "key": "02b627d",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Jesteśmy w Paryżu.",
      "sourceEnText": "We are in Paris.",
      "audioFile": "fr_01_02b627d.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T02:06:32Z",
      "textHash": "de3bca735d18ba9378c48738ce40c69f752e61163f2cd1ea1da316f873ab4f59"
    },
    {
      "key": "03dc852",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Bagietka.",
      "sourceEnText": "A baguette.",
      "audioFile": "fr_01_03dc852.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 975,
      "updatedAt": "2026-01-28T02:05:07Z",
      "textHash": "625fa9317bebe3d2364cfc320e0470c5808b2bcd7f0b18b1ee0812145ba1d299"
    },
    {
      "key": "041403d",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "Dzień dobry!",
      "sourceEnText": "Bonjour!",
      "audioFile": "fr_01_041403d.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 975,
      "updatedAt": "2026-01-28T02:08:49Z",
      "textHash": "369b135b04e0049847cf31b3566a18630669332fc0951c0b29b486e4a737d4bc"
    },
    {
      "key": "04986a3",
      "nodeTitle": "spawned_kid_f",
      "sourceText": "Ca va?",
      "sourceEnText": "Ca va?",
      "audioFile": "fr_01_04986a3.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 836,
      "updatedAt": "2026-01-28T02:08:51Z",
      "textHash": "ce0efa8c56925119793515e413efe296a0439e4f6085b4ad1fb0193e0cd06d0d"
    },
    {
      "key": "04e74ad",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Oto twój bilet.",
      "sourceEnText": "Here is your ticket.",
      "audioFile": "fr_01_04e74ad.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1300,
      "updatedAt": "2026-01-28T02:05:20Z",
      "textHash": "41699287deca1dfd43bbb65aa3d5f47a1f7d4609a768e27e480c9feb2be82966"
    },
    {
      "key": "0585a5e",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Skorzystaj z windy lub idź schodami!",
      "sourceEnText": "Take the elevator or use the stairs!",
      "audioFile": "fr_01_0585a5e.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:05:26Z",
      "textHash": "2f283b3ccb9b7af102ca3c9b3594685a75ed7a5a84901cdc78b18aebb019a210"
    },
    {
      "key": "05a38a8",
      "nodeTitle": "spawned_man",
      "sourceText": "Zamierzam kupić chleb w piekarni.",
      "sourceEnText": "I am going to buy bread at the bakery.",
      "audioFile": "fr_01_05a38a8.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1950,
      "updatedAt": "2026-01-28T02:06:54Z",
      "textHash": "424696e2c85da297714d4555478ae831eb5cb0433793c025ce32a7e7b89bbf01"
    },
    {
      "key": "05eabcf",
      "nodeTitle": "spawned_man",
      "sourceText": "Skąd pochodzisz?",
      "sourceEnText": "Where do you come from?",
      "audioFile": "fr_01_05eabcf.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1115,
      "updatedAt": "2026-01-28T02:06:55Z",
      "textHash": "a2ebeace8c6f5da6581ac4169ba403ba9d1b5a9dbf7c5a925fb19a9425c408bd"
    },
    {
      "key": "06022b0",
      "nodeTitle": "baguette_flour",
      "sourceText": "Znalazłeś mąkę!",
      "sourceEnText": "You found flour!",
      "audioFile": "fr_01_06022b0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T02:05:59Z",
      "textHash": "2a3162212ca01f5714016f1fb08abb9e3016bce05db3f57ba71c01a96b66f744"
    },
    {
      "key": "0635a6a",
      "nodeTitle": "spawned_man",
      "sourceText": "Nie urodziłem się w tym kraju.",
      "sourceEnText": "I was not born in this country.",
      "audioFile": "fr_01_0635a6a.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T02:06:57Z",
      "textHash": "c7eb1c672e8934c5d14c4551107fa915dc6c2595ed8dc0b05d02964d079e9ecc"
    },
    {
      "key": "069cbb3",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Bilet kosztuje 5 monet",
      "sourceEnText": "The ticket costs 5 coins.",
      "audioFile": "fr_01_069cbb3.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T02:05:28Z",
      "textHash": "ea14f102374062045adcaf784e4bcf8b339f919653c2f77e783c87fc07d1f2b4"
    },
    {
      "key": "06cccc0",
      "nodeTitle": "talk_baker",
      "sourceText": "Teraz potrzebuję soli.",
      "sourceEnText": "Now I need the salt.",
      "audioFile": "fr_01_06cccc0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1579,
      "updatedAt": "2026-01-28T02:05:00Z",
      "textHash": "a8b29eab370d070697d30e4028e2385618afef2ddf33904efc27d0956376ead8"
    },
    {
      "key": "06f3fa2",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "To jest katedra Notre-Dame.",
      "sourceEnText": "This is Notre-Dame Cathedral.",
      "audioFile": "fr_01_06f3fa2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2136,
      "updatedAt": "2026-01-28T02:05:33Z",
      "textHash": "cf7858281eb13669134088cb439f3033592f362315b4516bedb08fabeb862fe6"
    },
    {
      "key": "06f3fa2_cathedral",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Katedra oznacza bardzo duży kościół.",
      "sourceEnText": "A cathedral is a very big church.",
      "audioFile": "fr_01_06f3fa2_cathedral.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:07:03Z",
      "textHash": "f163743c76d465b26017be8314887017e285d6a7477fbcd6f08870acbdf2d8ca"
    },
    {
      "key": "0718e4a",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Po prostu rozejrzeć się.",
      "sourceEnText": "Just to look around.",
      "audioFile": "fr_01_0718e4a.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T02:08:40Z",
      "textHash": "d6a97642321cbac6e1189e0d36d41990243f1a0cc62132bdc902d1c80bfaee40"
    },
    {
      "key": "076b3e3",
      "nodeTitle": "baker_finish",
      "sourceText": "Zobaczmy, czy pamiętasz to, czego nauczyłeś się w Paryżu.",
      "sourceEnText": "Let's see if you remember what you learned in Paris.",
      "audioFile": "fr_01_076b3e3.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3251,
      "updatedAt": "2026-01-28T02:08:56Z",
      "textHash": "abe7784676aa82468b2e8072d93bfa79e9965fd5b2d506d3a8b8e23d5fe9f060"
    },
    {
      "key": "07a113f",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "To wyjątkowy symbol Paryża!",
      "sourceEnText": "It is a special symbol of Paris!",
      "audioFile": "fr_01_07a113f.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2229,
      "updatedAt": "2026-01-28T02:06:27Z",
      "textHash": "ac5d6d41ac3e02783138543d8baed6acf8d0c5e667ec3a234c84cacb2428e17f"
    },
    {
      "key": "07b94e9",
      "nodeTitle": "spawned_man",
      "sourceText": "Masz pytania?",
      "sourceEnText": "Do you have questions?",
      "audioFile": "fr_01_07b94e9.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 929,
      "updatedAt": "2026-01-28T02:06:43Z",
      "textHash": "e22fd2f36a70d4dd4ff944e685d79ca5526c3e2576359984bdd25bae5896171a"
    },
    {
      "key": "083dfcc",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Wejdź na dach tym portalem!",
      "sourceEnText": "Go up to the roof through that portal!",
      "audioFile": "fr_01_083dfcc.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1811,
      "updatedAt": "2026-01-28T02:05:35Z",
      "textHash": "93e13c027ae3d3c0161c54d3959d9ca1c410eea386453991e3442300f1e1a295"
    },
    {
      "key": "08654e6",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Tak! Dlaczego pytasz?",
      "sourceEnText": "Yes! Why do you ask?",
      "audioFile": "fr_01_08654e6.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1486,
      "updatedAt": "2026-01-28T02:06:37Z",
      "textHash": "eb642e7903852c268d1b5223f420313e18f4648213ac68c80596d37d54a37a4f"
    },
    {
      "key": "089abda",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Widziałem Anturę wchodzącego na szczyt wieży.",
      "sourceEnText": "I saw Antura going up to the top of the tower.",
      "audioFile": "fr_01_089abda.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2740,
      "updatedAt": "2026-01-28T02:05:23Z",
      "textHash": "b10fc64a437a625cb01a2abcdd832b1d37d164689d45d84540e285c86386d74c"
    },
    {
      "key": "08bede4",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Czy to miejsce jest prawdziwe?",
      "sourceEnText": "Is this place real?",
      "audioFile": "fr_01_08bede4.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2136,
      "updatedAt": "2026-01-28T02:06:34Z",
      "textHash": "1f457864cb03d6c4cb10ede0945dd4578dc03fc482f27abb1ef78bf576f6b9bd"
    },
    {
      "key": "08c1973",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Wieża Eiffla ma 300 metrów wysokości.",
      "sourceEnText": "The Eiffel Tower is 300 meters tall.",
      "audioFile": "fr_01_08c1973.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3111,
      "updatedAt": "2026-01-28T02:05:10Z",
      "textHash": "5b0b514aa7e075376b750981b5a8c582cd57d0fac5012bd20e1544a46464f795"
    },
    {
      "key": "09a0ead",
      "nodeTitle": "talk_notre_dame_roof",
      "sourceText": "W 2019 roku wybuchł duży pożar, ale go naprawiliśmy.",
      "sourceEnText": "There was a big fire in 2019, but it is fixed now.",
      "audioFile": "fr_01_09a0ead.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 4458,
      "updatedAt": "2026-01-28T02:06:06Z",
      "textHash": "5c227246ad351ed8e33a7da00daf472614cee324eb06196724c6fc7762a9a958"
    },
    {
      "key": "09be864",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Aby wejść na górę potrzebny jest bilet.",
      "sourceEnText": "You need a ticket to go up.",
      "audioFile": "fr_01_09be864.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:08:43Z",
      "textHash": "18698c86ea1e8b0b1eb2afcf02fab9d08b24c6433820f613fa2a99c68065061d"
    },
    {
      "key": "09dd1da",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Gdzie jesteśmy?",
      "sourceEnText": "Where are we?",
      "audioFile": "fr_01_09dd1da.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1068,
      "updatedAt": "2026-01-28T02:06:30Z",
      "textHash": "6b7a8c4edac38955f4f1a72af979b02bcc10bf02414ef1a7bc7c21cc1c8835ca"
    },
    {
      "key": "09e454b",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Dzień dobry. Czego chcesz?",
      "sourceEnText": "Good morning. What do you want?",
      "audioFile": "fr_01_09e454b.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1625,
      "updatedAt": "2026-01-28T02:05:30Z",
      "textHash": "745567e6a407c46cc954b7cc86c34966eefc5eba519146c7118d5efc8a09f50c"
    },
    {
      "key": "09e5c3b",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Zbudował go Gustave Eiffel w 1887 roku.",
      "sourceEnText": "Gustave Eiffel built it in 1887.",
      "audioFile": "fr_01_09e5c3b.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 4319,
      "updatedAt": "2026-01-28T02:05:13Z",
      "textHash": "c83ffb86766e36a1803d95c3f7589b9fb804e731401efb5a4ef5e160439df869"
    },
    {
      "key": "0a3f4e1",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "W 1889 roku w Paryżu odbył się wielki jarmark.",
      "sourceEnText": "There was a big fair in Paris in 1889.",
      "audioFile": "fr_01_0a3f4e1.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3901,
      "updatedAt": "2026-01-28T02:07:58Z",
      "textHash": "d6f24851757b152de8820b63a20ab61ec9def53fbd7ba55fab8a8b985a977571"
    },
    {
      "key": "0ac5a72",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "To bardzo stary kościół. Został zbudowany dawno temu.",
      "sourceEnText": "It is a very old church. It was built long ago.",
      "audioFile": "fr_01_0ac5a72.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3808,
      "updatedAt": "2026-01-28T02:07:25Z",
      "textHash": "8881aa253794c5a850fc414bdee936614a4bca97b2a73c0f005f7c3300cb0c48"
    },
    {
      "key": "0aee9bb",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Chciałbym wejść na Wieżę Eiffla.",
      "sourceEnText": "I would like to go up the Eiffel Tower.",
      "audioFile": "fr_01_0aee9bb.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1858,
      "updatedAt": "2026-01-28T02:08:41Z",
      "textHash": "a709c8889cd4dd12b11d4e700c71b0a6cad42499bf55c699db3b6334264644ef"
    },
    {
      "key": "0ba42cd",
      "nodeTitle": "coins_collected",
      "sourceText": "Masz teraz wystarczającą ilość monet, aby kupić bilet.",
      "sourceEnText": "You have enough coins now to buy the ticket.",
      "audioFile": "fr_01_0ba42cd.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3622,
      "updatedAt": "2026-01-28T02:07:08Z",
      "textHash": "bfbb8d82d77d8686ca035840be3e7755f9321845a14f6b055405792832c9c5c9"
    },
    {
      "key": "0bc62a3",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Cóż... wygląda to jak gra wideo.",
      "sourceEnText": "Well... it looks like a video game.",
      "audioFile": "fr_01_0bc62a3.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2461,
      "updatedAt": "2026-01-28T02:06:39Z",
      "textHash": "3585615ac2ead167f409107059f4fbc9730421e3a9cba7a5f912f7dad0d01903"
    },
    {
      "key": "0bda18a",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Uwielbiam Paryż!",
      "sourceEnText": "I love Paris!",
      "audioFile": "fr_01_0bda18a.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:08:46Z",
      "textHash": "c5dc634865142ec9b3928eac2e83ba96bf255355ee558b9a46fedecf5d910281"
    },
    {
      "key": "0c4d1f6",
      "nodeTitle": "baguette_water",
      "sourceText": "To jest woda.",
      "sourceEnText": "This is water.",
      "audioFile": "fr_01_0c4d1f6.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:06:01Z",
      "textHash": "ec1f4bd6c0a61e614a9e1071ca1cb3cce8529f0ed60258c1813e64f95320ca22"
    },
    {
      "key": "0cbdcce",
      "nodeTitle": "talk_eiffell_ticket",
      "sourceText": "Niedaleko jest piekarnia. Ale otwiera się później.",
      "sourceEnText": "There is a bakery near here. But it opens later.",
      "audioFile": "fr_01_0cbdcce.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3529,
      "updatedAt": "2026-01-28T02:05:49Z",
      "textHash": "61eb3ebedf8dbdbdc37769a3df14d296b8764085289a46bb6c3f16cae8d8c2d3"
    },
    {
      "key": "0d11596",
      "nodeTitle": "quest_end",
      "sourceText": "Gratulacje!",
      "sourceEnText": "Congratulations!",
      "audioFile": "fr_01_0d11596.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T02:05:37Z",
      "textHash": "6b18832aade701b04b99329e9b074893d209f74c4343eba7263b391785c29d5c"
    },
    {
      "key": "0d11596a",
      "nodeTitle": "quest_end",
      "sourceText": "Czy podobał Ci się Paryż?",
      "sourceEnText": "Did you like Paris?",
      "audioFile": "fr_01_0d11596a.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:07:32Z",
      "textHash": "296fd92013a58f59b6495ce4cc289edd6906cba852fd2599fdcb4e17d95fe071"
    },
    {
      "key": "0d59ade",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Jest zrobiony z żelaza!",
      "sourceEnText": "It is made of iron!",
      "audioFile": "fr_01_0d59ade.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:05:15Z",
      "textHash": "4490ca78424850ee3b55f45fafb1b73df07257b40c70f240970fb74236b684e9"
    },
    {
      "key": "0d59ade_fair",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Został zbudowany dawno temu na duże targi.",
      "sourceEnText": "It was built for a big fair long ago.",
      "audioFile": "fr_01_0d59ade_fair.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2694,
      "updatedAt": "2026-01-28T02:07:05Z",
      "textHash": "2374dc237abe93a9819f82917ea1eb0d835c57047ec5a6aa68c7d89e0e089673"
    },
    {
      "key": "0d6f3c4",
      "nodeTitle": "spawned_eiffell_tourist",
      "sourceText": "Wieża Eiffla została zbudowana specjalnie na tę wielką imprezę.",
      "sourceEnText": "The Eiffel Tower was built for that big party.",
      "audioFile": "fr_01_0d6f3c4.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3855,
      "updatedAt": "2026-01-28T02:08:00Z",
      "textHash": "c48b4161e06019a534da7d7b5ba1a243427fd571297c57357c5d40746cb07304"
    },
    {
      "key": "0d91dc0",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Czym jest Wieża Eiffla?",
      "sourceEnText": "What is the Eiffel Tower?",
      "audioFile": "fr_01_0d91dc0.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1533,
      "updatedAt": "2026-01-28T02:06:22Z",
      "textHash": "896940560bb08123dbd52c91972b354ebc256e398b9bf15b74d4d8ad0a2160fe"
    },
    {
      "key": "0da46e8",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Witamy na szczycie Wieży Eiffla!",
      "sourceEnText": "Welcome to the top of the Eiffel Tower!",
      "audioFile": "fr_01_0da46e8.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2136,
      "updatedAt": "2026-01-28T02:08:58Z",
      "textHash": "b7ac1e973cd4a2e49edd40c498fe3b90fb792f96756c64c1624161f4b2cad1ac"
    },
    {
      "key": "0ee51fc",
      "nodeTitle": "spawned_man",
      "sourceText": "Do widzenia",
      "sourceEnText": "Goodbye",
      "audioFile": "fr_01_0ee51fc.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 882,
      "updatedAt": "2026-01-28T02:06:58Z",
      "textHash": "b0108383d3fd094ed09f94458c40592192f9edc42ff4558708c05593d71468bb"
    },
    {
      "key": "0f17af0",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Wysoka żelazna wieża, o wysokości około 300 metrów.",
      "sourceEnText": "A tall iron tower, about 300 meters high.",
      "audioFile": "fr_01_0f17af0.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 4087,
      "updatedAt": "2026-01-28T02:06:25Z",
      "textHash": "eb696d7a611d16759771a21fe3c58eba41b146d2d327e2234cc21b20c6e0b316"
    },
    {
      "key": "0f18ad3",
      "nodeTitle": "spawned_man",
      "sourceText": "Widziałeś Anturę?",
      "sourceEnText": "Have you seen Antura?",
      "audioFile": "fr_01_0f18ad3.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1161,
      "updatedAt": "2026-01-28T02:06:44Z",
      "textHash": "5e8b795e6cbcad1e0d727e5584a6377a9d66854768c48d6aa7959ce19ae1cfb8"
    },
    {
      "key": "0f7f9d8",
      "nodeTitle": "gargoyle",
      "sourceText": "Spójrz na tę statuę! Czyż nie jest przerażająca?",
      "sourceEnText": "Look at that statue! Is it scary?",
      "audioFile": "fr_01_0f7f9d8.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3715,
      "updatedAt": "2026-01-28T02:08:37Z",
      "textHash": "9d3b18737619d06c80c77f7a5a00a58edd3a118008fb18acfa60f4761b51fba1"
    },
    {
      "key": "0f9dd62",
      "nodeTitle": "spawned_man",
      "sourceText": "Nie. Kim jest Antura?",
      "sourceEnText": "No. Who is Antura?",
      "audioFile": "fr_01_0f9dd62.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1672,
      "updatedAt": "2026-01-28T02:06:48Z",
      "textHash": "d157d508b245709dc7e1a172b414e593eb75b4ac86da533d50d87a93abe31647"
    },
    {
      "key": "0fe0732",
      "nodeTitle": "talk_eiffell_guide",
      "sourceText": "Nic. Pa.",
      "sourceEnText": "Nothing. Bye.",
      "audioFile": "fr_01_0fe0732.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 1022,
      "updatedAt": "2026-01-28T02:06:41Z",
      "textHash": "8f3616d81d779b24680b625a4b7ceb586d3021a7b44c3ae3b0c4ae26b5f58f63"
    },
    {
      "key": "already_solved",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Tę część już rozwiązaliśmy.",
      "sourceEnText": "We already solved this part.",
      "audioFile": "fr_01_already_solved.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T02:08:53Z",
      "textHash": "c464b89d2f471cb4fb38466440e03451c1dfd47d7ecf854de1ff09649ae1a66a"
    },
    {
      "key": "arc_0",
      "nodeTitle": "talk_arc",
      "sourceText": "To jest Łuk Triumfalny.",
      "sourceEnText": "This is the Arc de Triomphe.",
      "audioFile": "fr_01_arc_0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_F",
      "durationMs": 1858,
      "updatedAt": "2026-01-28T02:05:44Z",
      "textHash": "a596aa42f29b79060caae4d06268228455155e93b20a65295f0f05a77e9ea4f8"
    },
    {
      "key": "arc_0a",
      "nodeTitle": "talk_arc",
      "sourceText": "Upamiętnia ludzi, którzy walczyli za Francję.",
      "sourceEnText": "It honors people who fought for France.",
      "audioFile": "fr_01_arc_0a.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_F",
      "durationMs": 2833,
      "updatedAt": "2026-01-28T02:05:18Z",
      "textHash": "a48f33d9fd1526b0766f2045d2f679e1ac016917deb55339a925cb0bdefecebe"
    },
    {
      "key": "baker_0",
      "nodeTitle": "talk_baker",
      "sourceText": "Cześć! Jestem piekarzem. Piekę chleb codziennie.",
      "sourceEnText": "Hello! I am a baker. I make bread every day.",
      "audioFile": "fr_01_baker_0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 4040,
      "updatedAt": "2026-01-28T02:07:42Z",
      "textHash": "03629cba1509a824241c476b8af47b2ee2aed11cbcf024543cc3ad3efeb51781"
    },
    {
      "key": "baker_1",
      "nodeTitle": "talk_baker",
      "sourceText": "Dzisiaj chcę zrobić bagietkę...",
      "sourceEnText": "Today I want to make a baguette...",
      "audioFile": "fr_01_baker_1.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:07:40Z",
      "textHash": "70702996aa91431e703689bdd2479816a13654077339489d417f26f2cf23c543"
    },
    {
      "key": "baker_2",
      "nodeTitle": "talk_baker",
      "sourceText": "Ale zgubiłem składniki!",
      "sourceEnText": "But I lost the ingredients!",
      "audioFile": "fr_01_baker_2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1765,
      "updatedAt": "2026-01-28T02:06:52Z",
      "textHash": "3975917964b4fbf3696925d6afa3e994ff59cee8bb201f7fdc0becfb694dd1cf"
    },
    {
      "key": "baker_3",
      "nodeTitle": "talk_baker",
      "sourceText": "Duży niebieski pies ukradł mi mąkę!",
      "sourceEnText": "A big blue dog stole my flour!",
      "audioFile": "fr_01_baker_3.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2972,
      "updatedAt": "2026-01-28T02:05:03Z",
      "textHash": "683aff193abb15623979e29519150c737b6013ea746a551fd620c278cb23e329"
    },
    {
      "key": "baker_4",
      "nodeTitle": "talk_baker",
      "sourceText": "Czy możesz mi pomóc je znaleźć?",
      "sourceEnText": "Can you help me find them?",
      "audioFile": "fr_01_baker_4.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T02:06:13Z",
      "textHash": "b86742cc8ffbf0250a8cc54312eb3175ad2ac4f51a9fca3eb81d62e722193608"
    },
    {
      "key": "baker_5",
      "nodeTitle": "talk_baker",
      "sourceText": "Najpierw idź do Wieży Eiffla. Potrzebujemy mąki.",
      "sourceEnText": "Go to the Eiffel Tower.",
      "audioFile": "fr_01_baker_5.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3344,
      "updatedAt": "2026-01-28T02:05:54Z",
      "textHash": "12e68d9e6413cc2e664d157a78ca481c8c5da58fb778239217ca69ffd838ec5b"
    },
    {
      "key": "baker_finish_0",
      "nodeTitle": "baker_finish",
      "sourceText": "Znalazłeś wszystkie składniki!",
      "sourceEnText": "You found all the ingredients!",
      "audioFile": "fr_01_baker_finish_0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2322,
      "updatedAt": "2026-01-28T02:06:08Z",
      "textHash": "468fcf3ef1c56810fa22af7b66e26f73de9ec703ff82cd4fdd31bed688595088"
    },
    {
      "key": "baker_finish_1",
      "nodeTitle": "baker_finish",
      "sourceText": "Teraz potrafię zrobić bagietkę.",
      "sourceEnText": "Now I can make a baguette.",
      "audioFile": "fr_01_baker_finish_1.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T02:06:18Z",
      "textHash": "788871fbeb5a526c08974f0935b9e27441369a759a05620c92b0450e25c59bb5"
    },
    {
      "key": "baker_finish_2",
      "nodeTitle": "final_activity_done",
      "sourceText": "W Paryżu pachnie świeżym chlebem!",
      "sourceEnText": "Paris smells like fresh bread!",
      "audioFile": "fr_01_baker_finish_2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T02:06:47Z",
      "textHash": "6cb2c28755e39c777618cf58679a6bb26c3d24a632e04218deadef3ee3a23e7c"
    },
    {
      "key": "baker_r1",
      "nodeTitle": "talk_baker",
      "sourceText": "Świetnie! Mamy mąkę.",
      "sourceEnText": "Great! We have flour.",
      "audioFile": "fr_01_baker_r1.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T02:08:05Z",
      "textHash": "1aaf9830bc81aa480eb64ebf53937a7a6585423cc7dcc57b5f9877173d252e42"
    },
    {
      "key": "baker_r2",
      "nodeTitle": "talk_baker",
      "sourceText": "Idź do Notre-Dame.",
      "sourceEnText": "Go to Notre-Dame.",
      "audioFile": "fr_01_baker_r2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1347,
      "updatedAt": "2026-01-28T02:08:02Z",
      "textHash": "74e94bb1ff5fe8de118036f4239b6614ee8d322fd67d00b9eb4461a64e5c9cd3"
    },
    {
      "key": "baker_r3",
      "nodeTitle": "talk_baker",
      "sourceText": "Sól! Dobra robota.",
      "sourceEnText": "Salt! Well done.",
      "audioFile": "fr_01_baker_r3.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1904,
      "updatedAt": "2026-01-28T02:07:47Z",
      "textHash": "eb3f19057df0b4fc4ca9dc4bd568e316f07142a228572005cbf963ada9f76d5d"
    },
    {
      "key": "baker_r4",
      "nodeTitle": "talk_baker",
      "sourceText": "Antura wziął wodę i poszedł do Luwru.",
      "sourceEnText": "Antura took the water and went to the Louvre.",
      "audioFile": "fr_01_baker_r4.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2368,
      "updatedAt": "2026-01-28T02:07:45Z",
      "textHash": "467634dc7f5181b985b0d6e0268c9bf65bcd2616e1c65a1845a6b30f11f061c4"
    },
    {
      "key": "baker_r5",
      "nodeTitle": "talk_baker",
      "sourceText": "Świetnie! Mamy wodę.",
      "sourceEnText": "Perfect! We have water.",
      "audioFile": "fr_01_baker_r5.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T02:07:37Z",
      "textHash": "5164b2fd11ea1fd47cbb3e4ab062205b295f558b8361911d76ddae91ba1d784a"
    },
    {
      "key": "baker_r6",
      "nodeTitle": "talk_baker",
      "sourceText": "Być może drożdże są przy Łuku Triumfalnym.",
      "sourceEnText": "Maybe the yeast is at the Arc de Triomphe.",
      "audioFile": "fr_01_baker_r6.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2694,
      "updatedAt": "2026-01-28T02:07:35Z",
      "textHash": "6382d99549dee043848da85e2d918c0af332b26b25de7d4162e5a3a47f28f7ad"
    },
    {
      "key": "chest_unlocked",
      "nodeTitle": "eiffell_activity_done",
      "sourceText": "Skrzynia jest teraz odblokowana.",
      "sourceEnText": "The chest is now unlocked.",
      "audioFile": "fr_01_chest_unlocked.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T02:05:40Z",
      "textHash": "31a9f11bfcdba872f4f92a2c860c6aedbd09c5f5306cfbb4a969bda9a7628a4e"
    },
    {
      "key": "come_back_later",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Wróć później.",
      "sourceEnText": "Come back later.",
      "audioFile": "fr_01_come_back_later.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1254,
      "updatedAt": "2026-01-28T02:05:52Z",
      "textHash": "194f1fa7b843c858271ec0aaeb8332ddee73cc7173ba6e95e2f5d18854cc57c6"
    },
    {
      "key": "fr01_bakery_1",
      "nodeTitle": "npc_bakery",
      "sourceText": "Zapach świeżego chleba uszczęśliwia ludzi.",
      "sourceEnText": "Fresh bread smell makes people happy.",
      "audioFile": "fr_01_fr01_bakery_1.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2508,
      "updatedAt": "2026-01-28T02:08:27Z",
      "textHash": "f0d4ad1d9451e90446384a4c1fb8653afe9f5749e6a8f4a8929b8649cf5c42b3"
    },
    {
      "key": "fr01_bakery_2",
      "nodeTitle": "npc_bakery",
      "sourceText": "Do przygotowania bagietki używamy mąki, wody, drożdży i soli.",
      "sourceEnText": "A baguette needs flour, water, yeast, and salt.",
      "audioFile": "fr_01_fr01_bakery_2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 4598,
      "updatedAt": "2026-01-28T02:08:30Z",
      "textHash": "6f222d186634dfad08ca5483ace258a7154136b3b6fe0e988d50692927263b7c"
    },
    {
      "key": "fr01_bakery_2a",
      "nodeTitle": "npc_bakery",
      "sourceText": "Drożdże pomagają wyrosnąć chlebowi.",
      "sourceEnText": "Yeast helps bread rise.",
      "audioFile": "fr_01_fr01_bakery_2a.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:08:25Z",
      "textHash": "4783dbebc280e42ab2b643cc6e222999883dd9244c0901d2a14b55439266042d"
    },
    {
      "key": "fr01_bakery_3",
      "nodeTitle": "npc_bakery",
      "sourceText": "Piekarze wstają bardzo wcześnie rano, aby zacząć wypiekać chleb.",
      "sourceEnText": "Bakers wake up very early to start making bread.",
      "audioFile": "fr_01_fr01_bakery_3.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3947,
      "updatedAt": "2026-01-28T02:08:33Z",
      "textHash": "e8996b1b597cb0d3cbcf8cf1e95dca36dd001c412857052973b0e36b524c1b0d"
    },
    {
      "key": "fr01_eiffel_elevator_1",
      "nodeTitle": "npc_eiffell_elevator",
      "sourceText": "Winda pomaga ludziom dostać się na szczyt wieży.",
      "sourceEnText": "The elevator helps people go up the tower.",
      "audioFile": "fr_01_fr01_eiffel_elevator_1.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3019,
      "updatedAt": "2026-01-28T02:07:10Z",
      "textHash": "2cd6bb260d873625adf49f77642212e2f33e8b74aed77a8169dd11202e852f5b"
    },
    {
      "key": "fr01_notredame_base_3",
      "nodeTitle": "talk_notre_dame",
      "sourceText": "Kościół to miejsce, do którego ludzie chodzą się modlić.",
      "sourceEnText": "A church is a place where people go to pray.",
      "audioFile": "fr_01_fr01_notredame_base_3.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3437,
      "updatedAt": "2026-01-28T02:08:17Z",
      "textHash": "1375abb424fed903bc67af3ab4def5c4e0fa31d2dca88d148a541480a2eb178e"
    },
    {
      "key": "fr01_notredame_roof_1",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Z dachu można zobaczyć większą część Paryża.",
      "sourceEnText": "From the roof you can see much of Paris.",
      "audioFile": "fr_01_fr01_notredame_roof_1.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3111,
      "updatedAt": "2026-01-28T02:08:19Z",
      "textHash": "8cda4c82a3a7038b9631b4de0ffa17f2ce3bbd8b6530a668974e27d74b970854"
    },
    {
      "key": "fr01_notredame_roof_2",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Siedzą tu kamienne stworzenia zwane gargulcami.",
      "sourceEnText": "Stone creatures called gargoyles sit up here.",
      "audioFile": "fr_01_fr01_notredame_roof_2.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3762,
      "updatedAt": "2026-01-28T02:08:22Z",
      "textHash": "3a1369e8a4e205c771743bdf9987402c2a9f01dc47cbd87146ea0fdcab78679e"
    },
    {
      "key": "fr01_notredame_roof_2b",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Te kamienne ramiona pomagają podtrzymywać mury.",
      "sourceEnText": "These stone arms help hold up the walls.",
      "audioFile": "fr_01_fr01_notredame_roof_2b.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3529,
      "updatedAt": "2026-01-28T02:07:15Z",
      "textHash": "85f9ff4590a596f4048606857caa972355648e1d25eb34b2f703ef37957807d2"
    },
    {
      "key": "fr01_notredame_roof_2c",
      "nodeTitle": "npc_notredame_roof",
      "sourceText": "Duże, okrągłe okno nazywane jest rozetą.",
      "sourceEnText": "The big round window is called a rose window.",
      "audioFile": "fr_01_fr01_notredame_roof_2c.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2786,
      "updatedAt": "2026-01-28T02:07:13Z",
      "textHash": "42829a06a0e2a87e2bcc07f12e2e690c55f47245f5efa3687d3685d731237362"
    },
    {
      "key": "fr01_region_1",
      "nodeTitle": "npc_paris_region",
      "sourceText": "Paryż leży w regionie Île-de-France.",
      "sourceEnText": "Paris is in a place called Île-de-France.",
      "audioFile": "fr_01_fr01_region_1.ogg",
      "voiceProfile": "alice",
      "actorId": "ADULT_F",
      "durationMs": 2647,
      "updatedAt": "2026-01-28T02:07:57Z",
      "textHash": "078d950688d3ba759122924aad4c5e77b021d290ee462380eafbd328bbda86db"
    },
    {
      "key": "go_back_baker",
      "nodeTitle": "baguette_flour",
      "sourceText": "Wróć do piekarza.",
      "sourceEnText": "Go back to the baker.",
      "audioFile": "fr_01_go_back_baker.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1393,
      "updatedAt": "2026-01-28T02:05:42Z",
      "textHash": "f8a914416e206cdb55e7a1e86fff8e6114f5e968a19009c0fe01bf84d8eced22"
    },
    {
      "key": "louvre_0",
      "nodeTitle": "talk_louvre",
      "sourceText": "To jest Luwr, słynne muzeum.",
      "sourceEnText": "This is the Louvre, a famous museum.",
      "audioFile": "fr_01_louvre_0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_F",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T02:05:05Z",
      "textHash": "b1efec0b89fd84637025746d7f3c6041b8bb60e1b12d4dff72cc2bc0195ca356"
    },
    {
      "key": "puzzle_done",
      "nodeTitle": "final_activity_done",
      "sourceText": "Świetnie! Rozwiązałeś zagadkę.",
      "sourceEnText": "Great! You solved the puzzle.",
      "audioFile": "fr_01_puzzle_done.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2461,
      "updatedAt": "2026-01-28T02:05:47Z",
      "textHash": "42f7864b9727c69795d7dc4f9956832f2667e7f20870a6f9751bdbe0089fb50d"
    },
    {
      "key": "solve_puzzle",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Aby otworzyć skrzynię, rozwiąż zagadkę!",
      "sourceEnText": "To open the chest, solve the puzzle!",
      "audioFile": "fr_01_solve_puzzle.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2554,
      "updatedAt": "2026-01-28T02:06:15Z",
      "textHash": "683c3553686adea54ac322e6274f9a05e829a48bf9a2a71b0ad3d21e06588dfb"
    },
    {
      "key": "start",
      "nodeTitle": "quest_start",
      "sourceText": "Jesteśmy w Paryżu, stolicy Francji.",
      "sourceEnText": "We are in Paris, the capital of France.",
      "audioFile": "fr_01_start.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2461,
      "updatedAt": "2026-01-28T02:07:18Z",
      "textHash": "1a7615e2f4c2655db3c64edfad7423c6b432a65b2b9ab91d148a488078d02ce4"
    },
    {
      "key": "start_1",
      "nodeTitle": "quest_start",
      "sourceText": "Dzisiaj zwiedzimy Wieżę Eiffla",
      "sourceEnText": "Today we will explore the Eiffel Tower",
      "audioFile": "fr_01_start_1.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T02:07:54Z",
      "textHash": "b58e67ada28c3e8722dcc1a1dcdacfc97b8d9d63908c35bb28663baeae5b10a0"
    },
    {
      "key": "start_1a",
      "nodeTitle": "quest_start",
      "sourceText": "i Notre-Dame",
      "sourceEnText": "and Notre-Dame.",
      "audioFile": "fr_01_start_1a.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1254,
      "updatedAt": "2026-01-28T02:07:52Z",
      "textHash": "88ab7c2cf50c3d2de94f67a0a4f316f01cdfd9eb76a9aae6f6a8c792937b25a6"
    },
    {
      "key": "start_1b",
      "nodeTitle": "quest_start",
      "sourceText": "ale najpierw zjedzmy bagietkę!",
      "sourceEnText": "But first, let's eat a baguette!",
      "audioFile": "fr_01_start_1b.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T02:07:50Z",
      "textHash": "2279d31e91aa8f5eaec6934799c26053eca552dc89bb4fd94b00e3026b7aae61"
    },
    {
      "key": "start_2",
      "nodeTitle": "quest_start",
      "sourceText": "Jesteś gotowy? Zaczynajmy!",
      "sourceEnText": "Are you ready? Let's start!",
      "audioFile": "fr_01_start_2.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1950,
      "updatedAt": "2026-01-28T02:07:20Z",
      "textHash": "265687674922088f93c23260999965c01eac59f44557b58c4b03b4b6fd7fcf3d"
    },
    {
      "key": "talk_tutor",
      "nodeTitle": "talk_tutor",
      "sourceText": "Widziałem, jak Antura poszedł do piekarza. Chodźmy tam!",
      "sourceEnText": "I saw Antura go to the baker. Let's go there!",
      "audioFile": "fr_01_talk_tutor.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 3297,
      "updatedAt": "2026-01-28T02:07:23Z",
      "textHash": "7bf0e52bf6dfab76b54a95d399a5ae1ff94ae410a89fa5e502d6f399b4bceda9"
    },
    {
      "key": "talk_tutor_0",
      "nodeTitle": "talk_tutor",
      "sourceText": "Cześć i witaj w Paryżu!",
      "sourceEnText": "Hello! Have you ever been here in Paris!",
      "audioFile": "fr_01_talk_tutor_0.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T02:07:30Z",
      "textHash": "80cdb30d10d890c3bd111aacbb802c169705210c60cdd521cc1f77447b44d7aa"
    },
    {
      "key": "talk_tutor_0b",
      "nodeTitle": "talk_tutor",
      "sourceText": "Tak!",
      "sourceEnText": "Yes!",
      "audioFile": "fr_01_talk_tutor_0b.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 836,
      "updatedAt": "2026-01-28T02:08:14Z",
      "textHash": "6e790f7b34fafd477766062455b5450b67d0f9e02a429724ef3eae6f6629cef4"
    },
    {
      "key": "talk_tutor_0c",
      "nodeTitle": "talk_tutor",
      "sourceText": "Świetnie! Zobaczmy, czy pamiętasz te miejsca.",
      "sourceEnText": "Great! Let's see if you remember these places.",
      "audioFile": "fr_01_talk_tutor_0c.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 3483,
      "updatedAt": "2026-01-28T02:08:12Z",
      "textHash": "2d5c70781bb5283b970073b68858680499e0ec8eeb0e064deadaae176858e999"
    },
    {
      "key": "talk_tutor_0d",
      "nodeTitle": "talk_tutor",
      "sourceText": "NIE.",
      "sourceEnText": "No.",
      "audioFile": "fr_01_talk_tutor_0d.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 929,
      "updatedAt": "2026-01-28T02:08:10Z",
      "textHash": "680c1abe72ac661a41de368a2c55b5bf5588beca9878c850ab5c5b911f071649"
    },
    {
      "key": "talk_tutor_0e",
      "nodeTitle": "talk_tutor",
      "sourceText": "Mam nadzieję, że kiedyś tu przyjedziesz!",
      "sourceEnText": "I hope you can come here one day!",
      "audioFile": "fr_01_talk_tutor_0e.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2368,
      "updatedAt": "2026-01-28T02:08:07Z",
      "textHash": "a73e1346c19c2e5329d980513669d98d0386e74df15a1feddd4ef585bd9df370"
    }
  ]
}