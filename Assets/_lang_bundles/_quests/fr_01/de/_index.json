    },
    {
      "key": "start_1a",
      "nodeTitle": "quest_start",
      "sourceText": "und Notre-Dame",
      "sourceEnText": "and Notre-Dame.",
      "audioFile": "fr_01_start_1a.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 1254,
      "updatedAt": "2026-01-28T01:48:51Z",
