      "textHash": "9edcdff770b3a6a4962c0d515d883edac360f389d2a8388b91b1438445776e74"
    },
    {
      "key": "louvre_0",
      "nodeTitle": "talk_louvre",
      "sourceText": "Dies ist der Louvre, ein berühmtes Museum.",
      "sourceEnText": "This is the Louvre, a famous museum.",
      "audioFile": "fr_01_louvre_0.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_F",
      "durationMs": 3019,
      "updatedAt": "2026-01-28T01:45:45Z",
      "textHash": "dd25fcf08989d8a55e74d865816bfb865bd838b335d42ea81a2912dbc85470e9"
    },
    {
      "key": "puzzle_done",
      "nodeTitle": "final_activity_done",
      "sourceText": "Großartig! Du hast das Rätsel gelöst.",
      "sourceEnText": "Great! You solved the puzzle.",
      "audioFile": "fr_01_puzzle_done.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
