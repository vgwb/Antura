    },
    {
      "key": "start",
      "nodeTitle": "quest_start",
      "sourceText": "Wir befinden uns in Paris, der Hauptstadt Frankreichs.",
      "sourceEnText": "We are in Paris, the capital of France.",
      "audioFile": "fr_01_start.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 3437,
      "updatedAt": "2026-01-28T01:48:13Z",
      "textHash": "2ea5b6ad3c3492e920e47341e4e59b1a1239b75030a1892ec1b8ed6030d7350b"
    },
    {
      "key": "start_1",
      "nodeTitle": "quest_start",
      "sourceText": "Heute erkunden wir den Eiffelturm.",
      "sourceEnText": "Today we will explore the Eiffel Tower",
      "audioFile": "fr_01_start_1.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2322,
      "updatedAt": "2026-01-28T01:48:54Z",
