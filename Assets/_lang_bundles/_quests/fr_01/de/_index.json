    {
      "key": "solve_puzzle",
      "nodeTitle": "talk_eiffell_roof",
      "sourceText": "Um die Truhe zu öffnen, muss das Rätsel gelöst werden!",
      "sourceEnText": "To open the chest, solve the puzzle!",
      "audioFile": "fr_01_solve_puzzle.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 3529,
      "updatedAt": "2026-01-28T01:47:03Z",
