    {
      "key": "baker_r1",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Genial! Tenemos harina.",
      "sourceEnText": "Great! We have flour.",
      "audioFile": "fr_01_baker_r1.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2043,
      "updatedAt": "2026-01-28T01:55:53Z",
      "textHash": "dc7001575f045f7fcde83c1380ffa00f3066576cdf5524e94bda95bcb00e6153"
    },
    {
      "key": "baker_r2",
      "nodeTitle": "talk_baker",
      "sourceText": "Ve a Notre Dame.",
      "sourceEnText": "Go to Notre-Dame.",
      "audioFile": "fr_01_baker_r2.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1207,
      "updatedAt": "2026-01-28T01:55:52Z",
      "textHash": "a4ce9dba9fad301eefc152ec85d6494fb3daaa3fc4dac356df97abae05db5b22"
