      "audioFile": "fr_01_0bc62a3.ogg",
      "voiceProfile": "ninoska",
      "actorId": "ADULT_F",
      "durationMs": 1997,
      "updatedAt": "2026-01-28T01:54:38Z",
      "textHash": "87f5bedfe1e04e95f0b6346820d89b125440f3ab979fe452c76768fe1b8c8581"
