    {
      "key": "baker_r3",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Sal! ¡Bien hecho!",
      "sourceEnText": "Salt! Well done.",
      "audioFile": "fr_01_baker_r3.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1904,
      "updatedAt": "2026-01-28T01:55:35Z",
      "textHash": "383e6bfa4598141b58d804f5914f76ef4e7227793cdab209ddadd899e55fc5e4"
    },
    {
      "key": "baker_r4",
      "nodeTitle": "talk_baker",
      "sourceText": "Antura tomó el agua y se dirigió al Louvre.",
      "sourceEnText": "Antura took the water and went to the Louvre.",
      "audioFile": "fr_01_baker_r4.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2926,
      "updatedAt": "2026-01-28T01:55:33Z",
      "textHash": "c1c22ff6dbd4a85278fc611150cc7c5e716826ff31326e739e29cce4b6fa2adc"
    },
    {
      "key": "baker_r5",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Perfecto! Tenemos agua.",
      "sourceEnText": "Perfect! We have water.",
      "audioFile": "fr_01_baker_r5.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T01:55:27Z",
      "textHash": "380fbbd69a6a767bcbb40b53d4726b4e9074d3a77921c0e0c8273eb47ff3cb11"
    },
    {
      "key": "baker_r6",
      "nodeTitle": "talk_baker",
      "sourceText": "Quizás la levadura esté en el Arco del Triunfo.",
      "sourceEnText": "Maybe the yeast is at the Arc de Triomphe.",
      "audioFile": "fr_01_baker_r6.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2972,
      "updatedAt": "2026-01-28T01:55:26Z",
      "textHash": "6f05265943b29d678041dd9d85503b772ca89e6702669b42bd8b7035d4eab18f"
    },
    {
      "key": "chest_unlocked",
      "nodeTitle": "eiffell_activity_done",
      "sourceText": "El cofre ahora está desbloqueado.",
      "sourceEnText": "The chest is now unlocked.",
      "audioFile": "fr_01_chest_unlocked.ogg",
      "voiceProfile": "ninoska",
      "actorId": "Default",
      "durationMs": 2183,
      "updatedAt": "2026-01-28T01:53:51Z",
      "textHash": "43d205b5ddc2e3563dfdf0364d0225cd2462127e1e660db37d53b78f24c1795f"
