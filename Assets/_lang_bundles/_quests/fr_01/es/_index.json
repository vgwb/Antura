      "key": "fr01_bakery_2a",
      "nodeTitle": "npc_bakery",
      "sourceText": "La levadura ayuda a que el pan suba.",
      "sourceEnText": "Yeast helps bread rise.",
      "audioFile": "fr_01_fr01_bakery_2a.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
