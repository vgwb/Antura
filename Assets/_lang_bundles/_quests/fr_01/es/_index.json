      "textHash": "54c7b183efd39b2ccbda89aa8c4d7a6f30c5a6d2cd63dd8398cfdc16c8180ec2"
    },
    {
      "key": "baker_4",
      "nodeTitle": "talk_baker",
      "sourceText": "¿Puedes ayudarme a encontrarlos?",
      "sourceEnText": "Can you help me find them?",
      "audioFile": "fr_01_baker_4.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2090,
      "updatedAt": "2026-01-28T01:54:18Z",
      "textHash": "aea20c6959e780a8f4af050e49e28724a28f87c978dc94dc72fb0887764681c6"
    },
    {
      "key": "baker_5",
      "nodeTitle": "talk_baker",
      "sourceText": "Primero, vamos a la Torre Eiffel. Necesitamos harina.",
      "sourceEnText": "Go to the Eiffel Tower.",
      "audioFile": "fr_01_baker_5.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 4458,
      "updatedAt": "2026-01-28T01:54:02Z",
      "textHash": "137f0f3f31c02d15e27348f80daa71e423e94c09dfa5095ff4c92f6e69b5bb92"
    },
    {
      "key": "baker_finish_0",
      "nodeTitle": "baker_finish",
      "sourceText": "¡Encontraste todos los ingredientes!",
      "sourceEnText": "You found all the ingredients!",
      "audioFile": "fr_01_baker_finish_0.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2276,
      "updatedAt": "2026-01-28T01:54:14Z",
      "textHash": "5e32fafb82a7500ee803fec074e4a7fede1c19ac4b5ef304f2caf59a5e28aae5"
    },
    {
      "key": "baker_finish_1",
      "nodeTitle": "baker_finish",
      "sourceText": "Ahora puedo hacer una baguette.",
      "sourceEnText": "Now I can make a baguette.",
      "audioFile": "fr_01_baker_finish_1.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T01:54:22Z",
      "textHash": "7860516045a7a7b6a57dd3991373d23878c463d66033938c57627e2f11ddd1df"
    },
    {
      "key": "baker_finish_2",
      "nodeTitle": "final_activity_done",
      "sourceText": "¡París huele a pan fresco!",
      "sourceEnText": "Paris smells like fresh bread!",
      "audioFile": "fr_01_baker_finish_2.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1718,
      "updatedAt": "2026-01-28T01:54:45Z",
      "textHash": "8783e34b40b8605b4518745647ead496b2c10979a6e1fbeee2ce861e0c3d256d"
