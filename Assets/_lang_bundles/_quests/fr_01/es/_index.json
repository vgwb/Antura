    },
    {
      "key": "arc_0a",
      "nodeTitle": "talk_arc",
      "sourceText": "Honra a las personas que lucharon por Francia.",
      "sourceEnText": "It honors people who fought for France.",
      "audioFile": "fr_01_arc_0a.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_F",
