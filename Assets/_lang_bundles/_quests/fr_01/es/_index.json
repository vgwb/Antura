    {
      "key": "arc_0",
      "nodeTitle": "talk_arc",
      "sourceText": "Este es el Arco del Triunfo.",
      "sourceEnText": "This is the Arc de Triomphe.",
      "audioFile": "fr_01_arc_0.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_F",
