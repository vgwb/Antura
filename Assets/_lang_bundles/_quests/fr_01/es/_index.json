    {
      "key": "baker_0",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Hola! Soy el panadero. Hago pan todos los días.",
      "sourceEnText": "Hello! I am a baker. I make bread every day.",
      "audioFile": "fr_01_baker_0.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 3483,
      "updatedAt": "2026-01-28T01:55:31Z",
      "textHash": "9fd3a74143db7952882c8edc3dc3b704b4fe959fecba00c7ee9e23547f220807"
    },
    {
      "key": "baker_1",
      "nodeTitle": "talk_baker",
      "sourceText": "Hoy quiero hacer una baguette...",
      "sourceEnText": "Today I want to make a baguette...",
      "audioFile": "fr_01_baker_1.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1672,
      "updatedAt": "2026-01-28T01:55:29Z",
      "textHash": "408ed85039ce18bcec788974a1254c71d13e414d2ee26bb051a1c64a36c62315"
    },
    {
      "key": "baker_2",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Pero perdí los ingredientes!",
      "sourceEnText": "But I lost the ingredients!",
      "audioFile": "fr_01_baker_2.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 1904,
      "updatedAt": "2026-01-28T01:54:50Z",
      "textHash": "167e6f23449da2d1c9f4571c0dae33fe111925d63a935ad2af6884753840454d"
    },
    {
      "key": "baker_3",
      "nodeTitle": "talk_baker",
      "sourceText": "¡Un gran perro azul me robó la harina!",
      "sourceEnText": "A big blue dog stole my flour!",
      "audioFile": "fr_01_baker_3.ogg",
      "voiceProfile": "ninoska",
      "actorId": "SENIOR_M",
      "durationMs": 2322,
