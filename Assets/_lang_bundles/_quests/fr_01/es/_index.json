      "voiceProfile": "ninoska",
      "actorId": "Default",
      "durationMs": 2368,
      "updatedAt": "2026-01-28T01:53:41Z",
      "textHash": "dc9d34ae71f2b5700028b3d2594b9c817ee90224af5606bdadaf3ade9f0762f0"
