      "textHash": "cb246ccf468310b3f24f8069a11f50238aecf7c57313353d399ee129c7c26f6a"
    },
    {
      "key": "0635a6a",
      "nodeTitle": "spawned_man",
      "sourceText": "Yo no nací en este país.",
      "sourceEnText": "I was not born in this country.",
      "audioFile": "fr_01_0635a6a.ogg",
      "voiceProfile": "ninoska",
      "actorId": "ADULT_M",
