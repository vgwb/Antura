    },
    {
      "key": "talk_tutor",
      "nodeTitle": "talk_tutor",
      "sourceText": "Vi a Antura ir a la panadería. ¡Vamos allá!",
      "sourceEnText": "I saw Antura go to the baker. Let's go there!",
      "audioFile": "fr_01_talk_tutor.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 4133,
      "updatedAt": "2026-01-28T01:55:16Z",
      "textHash": "6f50ed289bf3a0ecdc0252c93393e838ef4af28f34b4ee96867c02dbc0a81d4b"
    },
    {
      "key": "talk_tutor_0",
      "nodeTitle": "talk_tutor",
      "sourceText": "¡Hola! ¿Has estado alguna vez aquí en París?",
      "sourceEnText": "Hello! Have you ever been here in Paris?",
      "audioFile": "fr_01_talk_tutor_0.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 2740,
      "updatedAt": "2026-01-28T16:26:55Z",
      "textHash": "d223b2cf67e721614b3e9fb20380c4cb96bed6c465739d22fe558456c86aba9a"
    },
    {
      "key": "talk_tutor_0b",
      "nodeTitle": "talk_tutor",
      "sourceText": "¡Sí!",
      "sourceEnText": "Yes!",
      "audioFile": "fr_01_talk_tutor_0b.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 743,
      "updatedAt": "2026-01-28T01:56:01Z",
      "textHash": "45beed2c3c028a73ac14aebdfe7b778e0b0f04ae56990f5af018e923968652b4"
    },
    {
      "key": "talk_tutor_0c",
      "nodeTitle": "talk_tutor",
      "sourceText": "¡Genial! A ver si recuerdas estos lugares.",
      "sourceEnText": "Great! Let's see if you remember these places.",
      "audioFile": "fr_01_talk_tutor_0c.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 2926,
      "updatedAt": "2026-01-28T01:55:59Z",
      "textHash": "6ea6d0b4d85a2a6811f5c07cb028bb87b0872aa5c0a078c0d3389798abc8b84a"
    },
    {
      "key": "talk_tutor_0d",
      "nodeTitle": "talk_tutor",
      "sourceText": "No.",
      "sourceEnText": "No.",
      "audioFile": "fr_01_talk_tutor_0d.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 697,
      "updatedAt": "2026-01-28T01:55:57Z",
      "textHash": "b97d1c233523dfc0f3bb365741322004db0f7f3710dc814597966adcf1bd07a0"
    },
    {
      "key": "talk_tutor_0e",
      "nodeTitle": "talk_tutor",
      "sourceText": "¡Espero que vengas aquí algún día!",
      "sourceEnText": "I hope you can come here one day!",
      "audioFile": "fr_01_talk_tutor_0e.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
