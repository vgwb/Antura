    },
    {
      "key": "start",
      "nodeTitle": "quest_start",
      "sourceText": "Estamos en París, la capital de Francia.",
      "sourceEnText": "We are in Paris, the capital of France.",
      "audioFile": "fr_01_start.ogg",
      "voiceProfile": "ninoska",
      "actorId": "NARRATOR",
      "durationMs": 2786,
      "updatedAt": "2026-01-28T01:55:12Z",
