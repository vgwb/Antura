      "textHash": "82348389097abdbee9bc0ed7f3196fc3b06b5b0ce7cabaf8047bcd78c2310cf7"
    },
    {
      "key": "0b3d05f",
      "nodeTitle": "RETURN_DIRECTOR",
      "sourceText": "Poczekaj, spójrz!",
      "sourceEnText": "Wait, look!",
      "audioFile": "pl_04_0b3d05f.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T06:20:55Z",
      "textHash": "2380dd46174dd83d6e539886700adb604ce617f6af8a62410910f3c389c2a0c0"
    },
    {
      "key": "0b79d01",
      "nodeTitle": "elephant_activity_done",
      "sourceText": "FLAGA? Nie mam jej.",
      "sourceEnText": "A FLAG? I do not have it.",
      "audioFile": "pl_04_0b79d01.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:20:10Z",
      "textHash": "aa7ad40827bc5f2b853579c88845cbd52f5af9cd176e1787076fa40863802797"
    },
    {
      "key": "0bc2b46",
      "nodeTitle": "director_activity_done",
      "sourceText": "Wszystkie ZWIERZĘTA są niewinne.",
      "sourceEnText": "All ANIMALS are innocent.",
      "audioFile": "pl_04_0bc2b46.ogg",
      "voiceProfile": "alice",
      "actorId": "NARRATOR",
      "durationMs": 2325,
      "updatedAt": "2025-10-09T06:20:50Z",
      "textHash": "0ae4715fdce514abfba9e867ee86d3354a267af2441936f13c257dd15f23e04b"
    },
    {
      "key": "0bcc257",
      "nodeTitle": "quest_end",
      "sourceText": "To zadanie zostało ukończone.",
      "sourceEnText": "This quest is complete.",
      "audioFile": "pl_04_0bcc257.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2038,
      "updatedAt": "2025-10-09T06:21:07Z",
      "textHash": "6c8486cb43b5b46396e210507ded15b4f290d901a74838ca52cd0b66418686e3"
    },
    {
      "key": "0bee484",
      "nodeTitle": "GIRAFFE_KID",
      "sourceText": "Stąd mogłem widzieć swój dom.",
      "sourceEnText": "I could see my house from here.",
      "audioFile": "pl_04_0bee484.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1985,
      "updatedAt": "2025-10-09T06:17:51Z",
      "textHash": "3a337c3aa7f87111c4c15a57383fa5c14607178830e6c12fbcfc2aae2c034b3b"
    },
    {
      "key": "0bf2346",
      "nodeTitle": "MONKEY_KID",
      "sourceText": "Skopiował mój taniec!",
      "sourceEnText": "He copied my dance!",
      "audioFile": "pl_04_0bf2346.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1907,
      "updatedAt": "2025-10-09T06:18:28Z",
      "textHash": "575cc2152a942031c2f51a61cd12afc9a56578277cff1a1f96d01c749e19cccb"
    },
    {
      "key": "0c33d20",
      "nodeTitle": "monkey_talk",
      "sourceText": "Szybciej!",
      "sourceEnText": "Be quick!",
      "audioFile": "pl_04_0c33d20.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1097,
      "updatedAt": "2025-10-09T10:41:36Z",
      "textHash": "d707d8a814b844ff38da0b4a199f8e6fd6191bbf1508c818b8aff6d31412d8d0"
    },
    {
      "key": "0c53945",
      "nodeTitle": "monkey_activity_done",
      "sourceText": "Nie wziąłem FLAG-a.",
      "sourceEnText": "I did not take the FLAG.",
      "audioFile": "pl_04_0c53945.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1332,
      "updatedAt": "2025-10-09T06:20:29Z",
      "textHash": "7762af3ba7f93e00faaf6ec7ab928dcf721dae5e0ceaa76a22579befebb4d2d6"
    },
    {
      "key": "0c71176",
      "nodeTitle": "director_task",
      "sourceText": "O nie!",
      "sourceEnText": "Oh no!",
      "audioFile": "pl_04_0c71176.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 679,
      "updatedAt": "2025-10-09T10:41:03Z",
      "textHash": "755e2702e392fe2c30f522fb44537c44f1c346860a3fe9824410cf604d828314"
    },
    {
      "key": "0cd794c",
      "nodeTitle": "penguin_talk",
      "sourceText": "Świetnie!",
      "sourceEnText": "Nice!",
      "audioFile": "pl_04_0cd794c.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1202,
      "updatedAt": "2025-10-09T10:41:39Z",
      "textHash": "243fb46ea41f377a538ac3577144e4cea4d71416a00cbe01e2418abb7e9bef7f"
    },
    {
      "key": "0d2883b",
      "nodeTitle": "LION_SIGN",
      "sourceText": "Samce mają GRZYWY.",
      "sourceEnText": "Males have MANE.",
      "audioFile": "pl_04_0d2883b.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1489,
      "updatedAt": "2025-10-09T06:18:06Z",
      "textHash": "6b6e65a861609e3627dcf72d5f889d639fe12e04dcd893e6c2a89994bd3a6018"
    },
    {
      "key": "0d4f5f7",
      "nodeTitle": "node",
      "sourceText": "Do zobaczenia nad ODRĄ!",
      "sourceEnText": "See you on the ODRA RIVER!",
      "audioFile": "pl_04_0d4f5f7.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1802,
      "updatedAt": "2025-10-09T06:19:20Z",
      "textHash": "925e09a173c8c29411891dfb1a7fa4699afcf770c16f81e0af9bb1f6a46937c8"
    },
    {
      "key": "0d5c607",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Jestem najwyższym ZWIERZĘCIEM.",
      "sourceEnText": "I am the tallest ANIMAL.",
      "audioFile": "pl_04_0d5c607.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2273,
      "updatedAt": "2025-10-09T06:17:34Z",
      "textHash": "0a4afebf67511bffbf82ac68cd30689c80bb3598a40045825c6b8b7884b76354"
    },
    {
      "key": "0d91701",
      "nodeTitle": "CEREMONY_END",
      "sourceText": "FLAG jest domem.",
      "sourceEnText": "The FLAG is at home.",
      "audioFile": "pl_04_0d91701.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1620,
      "updatedAt": "2025-10-09T06:19:09Z",
      "textHash": "93758a9bedf08fff66eb62fbbfd9dfdf5a898ecdc2dd5d62bf3f899204993438"
    },
    {
      "key": "0da284c",
      "nodeTitle": "task_animals_desc",
      "sourceText": "Znajdź FLAGĘ!",
      "sourceEnText": "Find the FLAG!",
      "audioFile": "pl_04_0da284c.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1254,
      "updatedAt": "2025-10-09T06:19:57Z",
      "textHash": "6a8bdbf22932a71129f7a3430648345dc3d6ab3f65f062f7ca85b8a49e88ec31"
    },
    {
      "key": "0e24973",
      "nodeTitle": "RETURN_DIRECTOR",
      "sourceText": "ANTURA ma FLAGĘ!",
      "sourceEnText": "ANTURA has the FLAG!",
      "audioFile": "pl_04_0e24973.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T06:19:02Z",
      "textHash": "a9ca55f02780205b3d218faad5e9a561528184b3fdce90841950a2383078f850"
    },
    {
      "key": "0e53f7d",
      "nodeTitle": "lion_talk",
      "sourceText": "Hej, spróbuj jeszcze raz.",
      "sourceEnText": "Hey, try again.",
      "audioFile": "pl_04_0e53f7d.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T10:41:28Z",
      "textHash": "ec8dde2f75f2c689009b1efa58464cc5cc08c79de82e3744a69b33af1ba40ddd"
    },
    {
      "key": "0e7e733",
      "nodeTitle": "node",
      "sourceText": "Gotowy do recenzji?",
      "sourceEnText": "Ready to review?",
      "audioFile": "pl_04_0e7e733.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1437,
      "updatedAt": "2025-10-09T06:18:54Z",
      "textHash": "2b77871a086a50a61d910d420740483e4143331761c91a23221f456b036e420d"
    },
    {
      "key": "0e84545",
      "nodeTitle": "animal_parrot",
      "sourceText": "Krzyk! Czerwony i żółty przebiegli!",
      "sourceEnText": "Squawk! Red and yellow ran by!",
      "audioFile": "pl_04_0e84545.ogg",
      "voiceProfile": "alice",
      "actorId": "SPECIAL",
      "durationMs": 2508,
      "updatedAt": "2025-10-09T06:17:09Z",
      "textHash": "4492f768e4108cbd1b8560532f782f758f0aee83fd5f65ef231777840d8c0721"
    },
    {
      "key": "0e957cf",
      "nodeTitle": "node",
      "sourceText": "Mój kapelusz podskoczył!",
      "sourceEnText": "My hat jumped!",
      "audioFile": "pl_04_0e957cf.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:18:11Z",
      "textHash": "84a9ee532968484ef0d244abf083f2a3132c14db6e9b32ea3480addec06ea4ed"
    },
