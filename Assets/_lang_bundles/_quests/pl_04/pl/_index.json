  "lines": [
    {
      "key": "001c615",
      "nodeTitle": "node",
      "sourceText": "Ceremonia",
      "sourceEnText": "Ceremony",
      "audioFile": "pl_04_001c615.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 914,
      "updatedAt": "2025-10-09T06:19:06Z",
      "textHash": "ff8a6517edf865023a0916e5cc6c1f2bfb5801223f7c48d039bbb56a114500b2"
    },
    {
      "key": "004abc7",
      "nodeTitle": "ELEPHANT_KID",
      "sourceText": "Czy może mnie wachlować latem?",
      "sourceEnText": "Can it fan me in summer?",
      "audioFile": "pl_04_004abc7.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 2508,
      "updatedAt": "2025-10-09T06:17:31Z",
      "textHash": "b17a215fd327eb37cc3b9a4e288beeb2d77b74a74b475acbbc16bc95c5fc9e52"
    },
    {
      "key": "005dd46",
      "nodeTitle": "quest_start",
      "sourceText": "Tutaj jest wiele ZWIERZĄT!",
      "sourceEnText": "There are many ANIMALS here!",
      "audioFile": "pl_04_005dd46.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1620,
      "updatedAt": "2025-09-28T22:11:22Z",
      "textHash": "73e5eaf7c682a90839a16f18fc8662d72dc7b4b90ae9b0590cf73587b2a9ed89"
    },
    {
      "key": "0085a8a",
      "nodeTitle": "animal_peacock",
      "sourceText": "Nie potrzebuję FLAG-u.",
      "sourceEnText": "I do not need a FLAG.",
      "audioFile": "pl_04_0085a8a.ogg",
      "voiceProfile": "alice",
      "actorId": "SPECIAL",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T06:20:07Z",
      "textHash": "5987c4b6d7a84e6393b24c59174bb0cca71070c96377a0a5e6fa092fe92ae4db"
    },
    {
      "key": "010adc5",
      "nodeTitle": "task_animals_desc",
      "sourceText": "Porozmawiaj ze wszystkimi zwierzętami. Może wiedzą, gdzie jest flaga.",
      "sourceEnText": "Talk to all the animals. Maybe they know where the flag is.",
      "audioFile": "pl_04_010adc5.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 4676,
      "updatedAt": "2025-10-09T06:20:04Z",
      "textHash": "230ae2c1b64b21fd1b2df0bcdfa68017436a5607148af6297e0077293c7971cb"
    },
    {
      "key": "010b191",
      "nodeTitle": "node",
      "sourceText": "Odpowiedz na pytania.",
      "sourceEnText": "Answer the questions.",
      "audioFile": "pl_04_010b191.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:19:14Z",
      "textHash": "556f9919a8a52e8a7b4b83dc70695494b9efaddc1019604d355fb468c7579558"
    },
    {
      "key": "0124e1c",
      "nodeTitle": "lion_talk",
      "sourceText": "Spójrz na to małe CUB!",
      "sourceEnText": "Look at this small CUB!",
      "audioFile": "pl_04_0124e1c.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1802,
      "updatedAt": "2025-10-09T06:18:00Z",
      "textHash": "5b72941e62f5209f88f4daf902c3380a8cc87fc472f261da123a518c66186002"
    },
    {
      "key": "012b933",
      "nodeTitle": "director_task",
      "sourceText": "Porozmawiaj ze ZWIERZĘTAMI. Jedno może je mieć.",
      "sourceEnText": "Talk to the ANIMALS. Maybe one of them has it.",
      "audioFile": "pl_04_012b933.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3474,
      "updatedAt": "2025-10-09T06:20:01Z",
      "textHash": "ced5f256e9217044f6d849352bd75d87055a5e6f3a9e81cf82d3fb7b1e1b307c"
    },
    {
      "key": "01b3593",
      "nodeTitle": "lion_activity_done",
      "sourceText": "Lubię patrzeć na niego na wietrze.",
      "sourceEnText": "I like watching it in wind.",
      "audioFile": "pl_04_01b3593.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2220,
      "updatedAt": "2025-10-09T06:20:25Z",
      "textHash": "0ce5ce01044bc0f99ebe3fb4eba2e32eac530823cc61b2f5c43e4ac039726bdb"
    },
    {
      "key": "021b299",
      "nodeTitle": "MONKEY_SIGN",
      "sourceText": "Uwielbia OWOCE.",
      "sourceEnText": "Loves FRUIT.",
      "audioFile": "pl_04_021b299.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:18:25Z",
      "textHash": "8ad08f3eabacac8a384072d5d37d6fefd31a0642668e60dec27713b764d857d7"
    },
    {
      "key": "027b51f",
      "nodeTitle": "elephant_talk",
      "sourceText": "Jestem największym ZWIERZĘCIEM lądowym.",
      "sourceEnText": "I am the biggest land ANIMAL.",
      "audioFile": "pl_04_027b51f.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2273,
      "updatedAt": "2025-10-09T06:17:12Z",
      "textHash": "b82377bd69011884d30c8ae58c5b0c842242a04cf4cc7bf8af0ba221be13df12"
    },
    {
      "key": "0291317",
      "nodeTitle": "GIRAFFE_SIGN",
      "sourceText": "Długie RZĘSY.",
      "sourceEnText": "Long EYELASHES.",
      "audioFile": "pl_04_0291317.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1254,
      "updatedAt": "2025-10-09T06:17:46Z",
      "textHash": "ac07b7dfca0cc52012facaac85cde9b145a71313071acd73475818ce74932cc2"
    },
    {
      "key": "02d00e2",
      "nodeTitle": "giraffe_activity_done",
      "sourceText": "To jest dla mnie za wysoko!",
      "sourceEnText": "It is too high for me!",
      "audioFile": "pl_04_02d00e2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1672,
      "updatedAt": "2025-10-09T06:20:18Z",
      "textHash": "af7a622668becc506bd69cf29732afef675d5d28f1a20c6e788f23ffba941bf4"
    },
    {
      "key": "02f35e4",
      "nodeTitle": "director_task",
      "sourceText": "Miało to miejsce na IGLICA w CENTENNIAL HALL.",
      "sourceEnText": "It was on the IGLICA at CENTENNIAL HALL.",
      "audioFile": "pl_04_02f35e4.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3109,
      "updatedAt": "2025-10-09T06:17:00Z",
      "textHash": "3edd2deff94ee65f6f40234cdf31664dc22fe07601f5415fd1428dbbc531b442"
    },
    {
      "key": "032811f",
      "nodeTitle": "director_task_done",
      "sourceText": "Dobrze. Rozmawiałeś ze WSZYSTKIMI ZWIERZĘTAMI.",
      "sourceEnText": "Good. You talked to all ANIMALS.",
      "audioFile": "pl_04_032811f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3187,
      "updatedAt": "2025-10-09T06:20:43Z",
      "textHash": "8cce262667c99404534b46f5a6c30add3ebf002362d88929bb34959b2221223e"
    },
    {
      "key": "03321d5",
      "nodeTitle": "item_chest",
      "sourceText": "Znalazłeś ciasteczka!",
      "sourceEnText": "You found some cookies!",
      "audioFile": "pl_04_03321d5.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1672,
      "updatedAt": "2025-10-09T10:41:12Z",
      "textHash": "45bd8b253584d9b40b67219fbf9968595af6a171498fbb6f0c6eca9ed2b3f872"
    },
    {
      "key": "0364f30",
      "nodeTitle": "director_task_done",
      "sourceText": "Teraz przyjrzyjmy się faktom.",
      "sourceEnText": "Now let's review facts.",
      "audioFile": "pl_04_0364f30.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:20:45Z",
      "textHash": "4fc2cb4b8a30390fe831e8fe1a3231d3125292b51868fba3297b5f4512ecaabf"
    },
    {
      "key": "03a150c",
      "nodeTitle": "elephant_talk",
      "sourceText": "Mam dobrą PAMIĘĆ.",
      "sourceEnText": "I have great MEMORY.",
      "audioFile": "pl_04_03a150c.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:17:15Z",
      "textHash": "fa0ab4b253e59c45ad3086c4eb2c63f63089a3b89a74ba991b8972c3e2a8d26d"
    },
    {
      "key": "03c4553",
      "nodeTitle": "animal_fox",
      "sourceText": "Hehe! Właściwie to nie pochodzę z zoo.",
      "sourceEnText": "Hehe! I'm not actually from the zoo.",
      "audioFile": "pl_04_03c4553.ogg",
      "voiceProfile": "alice",
      "actorId": "SPECIAL",
      "durationMs": 2691,
      "updatedAt": "2025-10-09T10:41:06Z",
      "textHash": "c58e3861a3ea43393f8a8bbe097c3d93a48b89854fadc37cea7da48ab717963e"
    },
    {
      "key": "042266c",
      "nodeTitle": "lion_talk",
      "sourceText": "Żyję w DUMIE.",
      "sourceEnText": "I live in a PRIDE.",
      "audioFile": "pl_04_042266c.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1332,
      "updatedAt": "2025-10-09T06:17:57Z",
      "textHash": "d0383d1941924fef3118eac1b7106ba0445bd0b6463e6e18c1d211b6e0307052"
    },
    {
      "key": "048e8a1",
      "nodeTitle": "ELEPHANT_SIGN",
      "sourceText": "SŁOŃ. Gruba skóra. Duże USZY.",
      "sourceEnText": "ELEPHANT. Thick skin. Big EARS.",
      "audioFile": "pl_04_048e8a1.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2586,
      "updatedAt": "2025-10-09T06:17:23Z",
      "textHash": "3713d44257cb710f5adc5dc35fb5cec05a13c8363e54b5923ae60716755cea3a"
    },
    {
      "key": "04b42f2",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Jem liście akacji!",
      "sourceEnText": "I eat WATTLE leaves!",
      "audioFile": "pl_04_04b42f2.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:17:40Z",
      "textHash": "0637e9e9a1f7e911752ce1c3cac0f6a8954a52258ba69750ce69728d85245cc2"
    },
    {
      "key": "04d057e",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Fajne, prawda?",
      "sourceEnText": "Cool, right?",
      "audioFile": "pl_04_04d057e.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1149,
      "updatedAt": "2025-10-09T10:41:23Z",
      "textHash": "1863af95fdd98849d5b6452a2f66cacafc4a2b3f2a173e2d4f5a3587b02c809a"
    },
    {
      "key": "04f37fa",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Chcesz spróbować jeszcze raz?",
      "sourceEnText": "Want to try again?",
      "audioFile": "pl_04_04f37fa.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1907,
      "updatedAt": "2025-10-09T10:41:25Z",
      "textHash": "2bab5096a21ad1ca645bbb86d0b53ae4c6120f83d8e8b1b2a5e7f5b26a4cdbe3"
    },
    {
      "key": "04fff6b",
      "nodeTitle": "animal_peacock",
      "sourceText": "Mój ogon to FLAGA!",
      "sourceEnText": "My tail is a FLAG!",
      "audioFile": "pl_04_04fff6b.ogg",
      "voiceProfile": "alice",
      "actorId": "SPECIAL",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:17:06Z",
      "textHash": "457a42cfb137bf9ca51779c77493dedab7d02aef95e69e000825ac414a551d34"
    },
    {
      "key": "0540c5a",
      "nodeTitle": "penguin_talk",
      "sourceText": "Nie umiem latać, ale dobrze pływam!",
      "sourceEnText": "I can't fly, but I swim well!",
      "audioFile": "pl_04_0540c5a.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2638,
      "updatedAt": "2025-10-09T06:18:37Z",
      "textHash": "2489b6fc2ab5b154938293005d4dc042d838dd2695a5bdf034619a1a42b85f1c"
    },
    {
      "key": "054cc37",
      "nodeTitle": "quest_end",
      "sourceText": "Dowiedziałeś się o ZWIERZĘTACH ZOOLOGICZNYCH.",
      "sourceEnText": "You learned about ZOO ANIMALS.",
      "audioFile": "pl_04_054cc37.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2586,
      "updatedAt": "2025-10-09T06:21:10Z",
      "textHash": "b6c82b5f7a8a17b7ac5df6bf6e9cdd5365b4912389c2ab16f72f887b2517a878"
    },
    {
      "key": "056d51c",
      "nodeTitle": "node",
      "sourceText": "Świetna robota w ZOO!",
      "sourceEnText": "Great work at the ZOO!",
      "audioFile": "pl_04_056d51c.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:19:17Z",
      "textHash": "c235d0c6102ccee1ccef24f37578edb2205bc206c2a9c34ee8253d0a1eba2dce"
    },
    {
      "key": "05ac327",
      "nodeTitle": "PENGUIN_KID",
      "sourceText": "Wygląda fajnie.",
      "sourceEnText": "It looks neat.",
      "audioFile": "pl_04_05ac327.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T06:18:51Z",
      "textHash": "83a7c592afe47ca07fafdf59de01e872609fae49750657d1424ddb66b61aca70"
    },
    {
      "key": "05da6d7",
      "nodeTitle": "lion_activity_done",
      "sourceText": "Musisz znaleźć FLAGĘ!",
      "sourceEnText": "You must find the FLAG!",
      "audioFile": "pl_04_05da6d7.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1802,
      "updatedAt": "2025-10-09T06:20:21Z",
      "textHash": "8f39db0c6ade60283335dd62582eedbdab63add45f646e6e74bf84ec41a44469"
    },
    {
      "key": "05f345e",
      "nodeTitle": "node",
      "sourceText": "Połącz ZWIERZĘTA z FAKTAMI.",
      "sourceEnText": "Match ANIMALS to FACTS.",
      "audioFile": "pl_04_05f345e.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2220,
      "updatedAt": "2025-10-09T06:18:57Z",
      "textHash": "fe3c144d9ee5e81787bc79ceeef25b3650b8a165a553ec466c1094ff8b6b1e4c"
    },
    {
      "key": "068daeb",
      "nodeTitle": "GIRAFFE_KID",
      "sourceText": "Z taką SZYJĄ!",
      "sourceEnText": "With a NECK like that!",
      "audioFile": "pl_04_068daeb.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T06:17:48Z",
      "textHash": "d6cfdcddb05767163f7896cb16324683d558bdaa9bebac025172275bbc60aab3"
    },
    {
      "key": "074fe29",
      "nodeTitle": "animal_fox",
      "sourceText": "Nie widziałem nigdzie flagi.",
      "sourceEnText": "I didn't see a flag anywhere.",
      "audioFile": "pl_04_074fe29.ogg",
      "voiceProfile": "alice",
      "actorId": "SPECIAL",
      "durationMs": 1672,
      "updatedAt": "2025-10-09T10:41:09Z",
      "textHash": "de816addea68d5491ad651f63fcda153414c1627ffa549a92307c74f2d7a80b9"
    },
    {
      "key": "0774b92",
      "nodeTitle": "lion_talk",
      "sourceText": "Dorastam, najpierw jestem mały, potem średni, a potem stary...",
      "sourceEnText": "I grow up small, then medium, then old...",
      "audioFile": "pl_04_0774b92.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 4075,
      "updatedAt": "2025-10-09T10:41:01Z",
      "textHash": "ebc92da0dbb3474e6d454594d7242a081b1c3bbfcf6c86bbc0cb73790a2f407b"
    },
    {
      "key": "078190f",
      "nodeTitle": "penguin_activity_done",
      "sourceText": "Nie, nie wziąłem FLAG-a.",
      "sourceEnText": "No, I did not take the FLAG.",
      "audioFile": "pl_04_078190f.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1489,
      "updatedAt": "2025-10-09T06:20:34Z",
      "textHash": "058dfb29dac18070b46ba2e457b6a57f18059fb59d2d98a466112a8489e546b4"
    },
    {
      "key": "079451b",
      "nodeTitle": "monkey_talk",
      "sourceText": "Gdzie dostałem te OWOCE?",
      "sourceEnText": "Where did I get these FRUITS?",
      "audioFile": "pl_04_079451b.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1750,
      "updatedAt": "2025-10-09T06:18:19Z",
      "textHash": "8bf1eeddc449dfefb3f85bf3dc27bc5b31e01c140bb431311a9010f4da18a8a7"
    },
    {
      "key": "079f4e0",
      "nodeTitle": "LION_KID",
      "sourceText": "Cóż za ryk!",
      "sourceEnText": "What a roar!",
      "audioFile": "pl_04_079f4e0.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1202,
      "updatedAt": "2025-10-09T06:18:08Z",
      "textHash": "fea1caca1a1051d5ec0dbcc3c2b8a63e6c28ae76c623ef8e4a44bed9c6db743c"
    },
    {
      "key": "07f2e15",
      "nodeTitle": "lion_talk",
      "sourceText": "Jestem LWEM w AFRYCE.",
      "sourceEnText": "I am a LION from AFRICA.",
      "audioFile": "pl_04_07f2e15.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:17:54Z",
      "textHash": "9e4edf43b324dae5ee7bf0d6bd685e876c72fae44390525ef8ee81cfb17dd148"
    },
    {
      "key": "0809ac5",
      "nodeTitle": "post_quest_activity",
      "sourceText": "Narysuj swoje ulubione ZWIERZĘ.",
      "sourceEnText": "Draw your favorite ANIMAL.",
      "audioFile": "pl_04_0809ac5.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2586,
      "updatedAt": "2025-10-09T06:21:13Z",
      "textHash": "dfd7064ec41e47e51f99fc5ab184f11425f223debdd054412e540995ca1df465"
    },
    {
      "key": "080d627",
      "nodeTitle": "lion_talk",
      "sourceText": "Utrudnię ci zadanie.",
      "sourceEnText": "I'll make it harder.",
      "audioFile": "pl_04_080d627.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1672,
      "updatedAt": "2025-10-09T10:41:31Z",
      "textHash": "c9b4a92e31e2c2c62d4e07770243053fcc90d6d435a49076b46dcf499706d612"
    },
    {
      "key": "0810771",
      "nodeTitle": "item_chest",
      "sourceText": "Teraz jest puste.",
      "sourceEnText": "It's empty now.",
      "audioFile": "pl_04_0810771.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1437,
      "updatedAt": "2025-10-09T10:41:14Z",
      "textHash": "2dc088d1a5c97a4253c7a7e54998bde5f376ec5a716fe3ccdb6c5f57b72e2ceb"
    },
    {
      "key": "084fedc",
      "nodeTitle": "penguin_talk",
      "sourceText": "Możesz to powtórzyć?",
      "sourceEnText": "Can you do it again?",
      "audioFile": "pl_04_084fedc.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1489,
      "updatedAt": "2025-10-09T10:41:42Z",
      "textHash": "962992fce7c33372ba8a94b0649b195d93794da42227760abcdf39b48f59d761"
    },
    {
      "key": "08568f5",
      "nodeTitle": "penguin_activity_done",
      "sourceText": "Nie potrafię latać, pamiętasz?",
      "sourceEnText": "I can't fly, remember?",
      "audioFile": "pl_04_08568f5.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2116,
      "updatedAt": "2025-10-09T06:20:37Z",
      "textHash": "f07aece86d3cef6a6e3eac960deac416d67db07b82fbb44cf0935cd1fe2b71f6"
    },
    {
      "key": "0867233",
      "nodeTitle": "monkey_talk",
      "sourceText": "Jestem MAŁPĄ. Wspinam się na drzewa.",
      "sourceEnText": "I am a CHIMPANZEE. I climb TREES.",
      "audioFile": "pl_04_0867233.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2926,
      "updatedAt": "2025-10-09T06:18:14Z",
      "textHash": "ee24e9643a33733971df9c6588c04bac8368d27793628c5114486194ae0c412c"
    },
    {
      "key": "0877d6f",
      "nodeTitle": "giraffe_activity_done",
      "sourceText": "Nie wziąłem FLAG-a.",
      "sourceEnText": "I did not take the FLAG.",
      "audioFile": "pl_04_0877d6f.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1332,
      "updatedAt": "2025-10-09T06:20:15Z",
      "textHash": "7762af3ba7f93e00faaf6ec7ab928dcf721dae5e0ceaa76a22579befebb4d2d6"
    },
    {
      "key": "0877d95",
      "nodeTitle": "PENGUIN_SIGN",
      "sourceText": "PINGWIN. Ptak. Pływak.",
      "sourceEnText": "PENGUIN. Bird. Swimmer.",
      "audioFile": "pl_04_0877d95.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1855,
      "updatedAt": "2025-10-09T06:18:43Z",
      "textHash": "d1acdcbcbbac66632c149c6c6f0c71ecd70155d9d087e812da5e72dd0db92576"
    },
    {
      "key": "08c70e8",
      "nodeTitle": "penguin_talk",
      "sourceText": "Jestem PINGWINEM, dziwnym PTAKEM.",
      "sourceEnText": "I am a PENGUIN, a strange BIRD.",
      "audioFile": "pl_04_08c70e8.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2952,
      "updatedAt": "2025-10-09T06:18:34Z",
      "textHash": "d3d1c0d86b5d09e11c715a71fbad482e444ccfdc79dce11644f0350a28981691"
    },
    {
      "key": "08c71db",
      "nodeTitle": "CEREMONY_END",
      "sourceText": "Dziękuję, pomocniku.",
      "sourceEnText": "Thank you, helper.",
      "audioFile": "pl_04_08c71db.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T06:19:11Z",
      "textHash": "01016441636342aa35811a5cf1c6d9bb11b0d1e8dd06f2bd52e4f57c235ed2dc"
    },
    {
      "key": "08de86f",
      "nodeTitle": "director_task_done",
      "sourceText": "Pomóż mi złożyć wszystko w całość.",
      "sourceEnText": "Help me put pieces together.",
      "audioFile": "pl_04_08de86f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2116,
      "updatedAt": "2025-10-09T06:20:47Z",
      "textHash": "71edb2baa3c8fd9e8f4e6c556fd0b8ce5a45eaee07a46297a5f108e4c2f295cb"
    },
    {
      "key": "0900219",
      "nodeTitle": "MONKEY_SIGN",
      "sourceText": "MAŁPA. Świetny WSPINACZ.",
      "sourceEnText": "CHIMPANZEE. Great CLIMBER.",
      "audioFile": "pl_04_0900219.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2325,
      "updatedAt": "2025-10-09T06:18:23Z",
      "textHash": "281e8f5714536f690c4f699ff7da52d5eaf2cfa0d449cea5ad4fcfa7f83ff4ce"
    },
    {
      "key": "092486d",
      "nodeTitle": "monkey_talk",
      "sourceText": "Och, jesteś dobry!",
      "sourceEnText": "Oh, you're good!",
      "audioFile": "pl_04_092486d.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T10:41:34Z",
      "textHash": "86dfd0d55285305826613565772aa7fb1e020e2140cedace9cf35bd56220a936"
    },
    {
      "key": "092cdc9",
      "nodeTitle": "elephant_talk",
      "sourceText": "Masz dobrą pamięć!",
      "sourceEnText": "Your memory is good!",
      "audioFile": "pl_04_092cdc9.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T10:41:17Z",
      "textHash": "e6624995399e487cc03138d48f382df5b1c721788e5a756fdfea9aba58f4783f"
    },
    {
      "key": "09523fa",
      "nodeTitle": "elephant_talk",
      "sourceText": "Chcesz spróbować jeszcze raz?",
      "sourceEnText": "Want to try again?",
      "audioFile": "pl_04_09523fa.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T10:41:20Z",
      "textHash": "2bab5096a21ad1ca645bbb86d0b53ae4c6120f83d8e8b1b2a5e7f5b26a4cdbe3"
    },
    {
      "key": "09c6bf7",
      "nodeTitle": "director_task",
      "sourceText": "O nie! FLAGA zaginęła!",
      "sourceEnText": "The FLAG is missing!",
      "audioFile": "pl_04_09c6bf7.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1907,
      "updatedAt": "2025-10-09T06:16:57Z",
      "textHash": "95df51814a0b4808693e341aeddcdd30c7f9ff59096d8c5635feb50e15684c20"
    },
    {
      "key": "0a0499e",
      "nodeTitle": "node",
      "sourceText": "Więcej informacji",
      "sourceEnText": "More info",
      "audioFile": "pl_04_0a0499e.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:17:21Z",
      "textHash": "000cc9c7e51f1a19e40a44b9825cdb1a2a2638d4268d141f0cabcad9117e46a9"
    },
    {
      "key": "0a3420c",
      "nodeTitle": "penguin_talk",
      "sourceText": "Czy potrafisz znaleźć ścieżkę w ICE?",
      "sourceEnText": "Can you find the path in ICE?",
      "audioFile": "pl_04_0a3420c.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2508,
      "updatedAt": "2025-10-09T06:18:40Z",
      "textHash": "be68d977dae0ebf5d718e71c96f668ffa33f661de2f8c7ffdb27a6576275f7ba"
    },
    {
      "key": "0a43c85",
      "nodeTitle": "monkey_activity_done",
      "sourceText": "Byłoby fajnie wspiąć się na ten słup.",
      "sourceEnText": "It would be fun to climb that pole.",
      "audioFile": "pl_04_0a43c85.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 2534,
      "updatedAt": "2025-10-09T06:20:31Z",
      "textHash": "a081631ddb70b821ceaeb533b24029ee8b0378c687a9d8afc3c0abc73046b91c"
    },
    {
      "key": "0a4d24e",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Moja długa SZYJA pomaga mi dosięgnąć liści.",
      "sourceEnText": "My long NECK helps me reach leaves.",
      "audioFile": "pl_04_0a4d24e.ogg",
      "voiceProfile": "alice",
      "actorId": "SENIOR_M",
      "durationMs": 3291,
      "updatedAt": "2025-10-09T06:17:37Z",
      "textHash": "66d8b59a2bfbc5c992e20150f0f467f0b2365492b34b27a87089c89e4dc89308"
    },
    {
      "key": "0a8a73f",
      "nodeTitle": "GIRAFFE_SIGN",
      "sourceText": "ŻYRAFA. Wysoka. Długa szyja.",
      "sourceEnText": "GIRAFFE. Tall. Long NECK.",
      "audioFile": "pl_04_0a8a73f.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2586,
      "updatedAt": "2025-10-09T06:17:43Z",
      "textHash": "59ee9b00ae698f9c83c541cdfc231adb28c41f03d5d9f285996fb21a25a68376"
    },
    {
      "key": "0a93d9b",
      "nodeTitle": "task_animals_done",
      "sourceText": "ZADANIE WYKONANE! Wróć do DYREKTORA.",
      "sourceEnText": "TASK DONE! Go back to the ZOO DIRECTOR.",
      "audioFile": "pl_04_0a93d9b.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2821,
      "updatedAt": "2025-10-09T06:20:40Z",
      "textHash": "a10f75d0c58837cdd4e4bdb91cf61b0660247a4c7358fde101e36c7b03b78011"
    },
    {
      "key": "0ac6cc0",
      "nodeTitle": "LION_SIGN",
      "sourceText": "LEW. DUMA = rodzina lwów.",
      "sourceEnText": "LION. Lives in a PRIDE, a family of lions.",
      "audioFile": "pl_04_0ac6cc0.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2821,
      "updatedAt": "2025-10-09T06:18:03Z",
      "textHash": "9d1540495d90a89895a95868ef424ed7749131b1144e988869bb464f64fc6744"
    },
    {
      "key": "0ae73f3",
      "nodeTitle": "PENGUIN_KID",
      "sourceText": "Jakie słodkie!",
      "sourceEnText": "How cute!",
      "audioFile": "pl_04_0ae73f3.ogg",
      "voiceProfile": "alice",
      "actorId": "KID_F",
      "durationMs": 1437,
      "updatedAt": "2025-10-09T06:18:48Z",
      "textHash": "6fc328e3da4b4900e4a4e1f12ea28b2fac2ef745becc08ee67acd2ddad262119"
    },
    {
      "key": "0b1cca2",
      "nodeTitle": "ELEPHANT_SIGN",
      "sourceText": "Największe ZWIERZĘ LĄDOWE.",
      "sourceEnText": "Largest land ANIMAL.",
      "audioFile": "pl_04_0b1cca2.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1933,
