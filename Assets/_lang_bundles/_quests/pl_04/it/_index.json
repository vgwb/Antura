  "lines": [
    {
      "key": "001c615",
      "nodeTitle": "node",
      "sourceText": "Cerimonia",
      "sourceEnText": "Ceremony",
      "audioFile": "pl_04_001c615.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 784,
      "updatedAt": "2025-10-09T06:15:08Z",
      "textHash": "2fe27a9c81c9c3fef2d240599530f75fe379435e0506925444a41530ec8c3dda"
    },
    {
      "key": "004abc7",
      "nodeTitle": "ELEPHANT_KID",
      "sourceText": "Può ventilarmi d'estate?",
      "sourceEnText": "Can it fan me in summer?",
      "audioFile": "pl_04_004abc7.ogg",
      "voiceProfile": "francesco",
      "actorId": "KID_F",
      "durationMs": 1489,
      "updatedAt": "2025-10-09T08:43:06Z",
      "textHash": "0709643681e8ab45adad93a65cb30a903a3ac52d8a73c02e386261235cd67576"
    },
    {
      "key": "005dd46",
      "nodeTitle": "quest_start",
      "sourceText": "Ci sono molti ANIMALI qui!",
      "sourceEnText": "There are many ANIMALS here!",
      "audioFile": "pl_04_005dd46.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1437,
      "updatedAt": "2025-10-09T08:10:50Z",
      "textHash": "6fc928e92b33038f96f2dd779d7bbddb6de36435dca99b52ef5deee99aceccb8"
    },
    {
      "key": "0085a8a",
      "nodeTitle": "animal_peacock",
      "sourceText": "Non ho bisogno di una BANDIERA.",
      "sourceEnText": "I do not need a FLAG.",
      "audioFile": "pl_04_0085a8a.ogg",
      "voiceProfile": "francesco",
      "actorId": "SPECIAL",
      "durationMs": 1750,
      "updatedAt": "2025-10-09T06:15:51Z",
      "textHash": "680ee4207e877bf6ed7671feec12963da30326e92ea359d147901fcfeb92d077"
    },
    {
      "key": "010adc5",
      "nodeTitle": "task_animals_desc",
      "sourceText": "Parla con tutti gli animali. Forse sanno dov'è la bandiera.",
      "sourceEnText": "Talk to all the animals. Maybe they know where the flag is.",
      "audioFile": "pl_04_010adc5.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 3187,
      "updatedAt": "2025-10-09T06:15:49Z",
      "textHash": "317d41386935a8bfcb5fc76c6a761660016ca2c11e39d4bc4fb5f1fd499b94d5"
    },
    {
      "key": "010b191",
      "nodeTitle": "node",
      "sourceText": "Rispondere alle domande.",
      "sourceEnText": "Answer the questions.",
      "audioFile": "pl_04_010b191.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T06:15:14Z",
      "textHash": "669652ccf17f96523084bd3673b4a57dcd2992e02a4ba4ce1ce5351cbad911e4"
    },
    {
      "key": "0124e1c",
      "nodeTitle": "lion_talk",
      "sourceText": "Guarda questo CUCCIOLO!",
      "sourceEnText": "Look at this small CUB!",
      "audioFile": "pl_04_0124e1c.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T08:41:33Z",
      "textHash": "7ecf7c31b2a50c91aa93aeb7fe09450c897221885b14df38cf7a206b9f50d962"
    },
    {
      "key": "012b933",
      "nodeTitle": "director_task",
      "sourceText": "Parla con gli ANIMALI. Uno potrebbe averlo.",
      "sourceEnText": "Talk to the ANIMALS. Maybe one of them has it.",
      "audioFile": "pl_04_012b933.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 2508,
      "updatedAt": "2025-10-09T06:15:47Z",
      "textHash": "d8091726e66ff69605467e15da1b1582b7595de1955ede3cc342289f9335f066"
    },
    {
      "key": "01b3593",
      "nodeTitle": "lion_activity_done",
      "sourceText": "Mi piace guardarlo nel vento.",
      "sourceEnText": "I like watching it in wind.",
      "audioFile": "pl_04_01b3593.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1750,
      "updatedAt": "2025-10-09T06:16:03Z",
      "textHash": "037b68821861db626127285ed863be91a03ba2f128b9847e6e056cb984c2bda2"
    },
    {
      "key": "021b299",
      "nodeTitle": "MONKEY_SIGN",
      "sourceText": "Ama la FRUTTA.",
      "sourceEnText": "Loves FRUIT.",
      "audioFile": "pl_04_021b299.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 967,
      "updatedAt": "2025-10-09T06:14:37Z",
      "textHash": "7f5f5337e01cbd8dd84303a17e1d811eb1ca5d6f986664435277106797812a7f"
    },
    {
      "key": "027b51f",
      "nodeTitle": "elephant_talk",
      "sourceText": "Sono il più grande ANIMALE terrestre.",
      "sourceEnText": "I am the biggest land ANIMAL.",
      "audioFile": "pl_04_027b51f.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 2038,
      "updatedAt": "2025-10-09T06:13:46Z",
      "textHash": "d7c735ff8ca7d310d13b4da2c4b0424975e4ce7545efb28dd2df5a5236758410"
    },
    {
      "key": "0291317",
      "nodeTitle": "GIRAFFE_SIGN",
      "sourceText": "CIGLIA lunghe.",
      "sourceEnText": "Long EYELASHES.",
      "audioFile": "pl_04_0291317.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1149,
      "updatedAt": "2025-10-09T06:14:10Z",
      "textHash": "aa348ae4ed8b5f800db10b9934a0c8f4bb8e90124f79945e881ebcb1d84eb5d6"
    },
    {
      "key": "02d00e2",
      "nodeTitle": "giraffe_activity_done",
      "sourceText": "Per me è troppo alto!",
      "sourceEnText": "It is too high for me!",
      "audioFile": "pl_04_02d00e2.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1332,
      "updatedAt": "2025-10-09T08:42:37Z",
      "textHash": "c891640ab609f31505aa548555761b0dab0025c56e32b273e41755e77c7a1a65"
    },
    {
      "key": "02f35e4",
      "nodeTitle": "director_task",
      "sourceText": "Era sull'IGLICA al CENTENNIAL HALL.",
      "sourceEnText": "It was on the IGLICA at CENTENNIAL HALL.",
      "audioFile": "pl_04_02f35e4.ogg",
      "voiceProfile": "francesco",
      "actorId": "ADULT_M",
      "durationMs": 2351,
      "updatedAt": "2025-10-09T08:10:22Z",
      "textHash": "f6b99949cd43fdeb473fef7ec451aff6d4004e37960020b726196c970bea131a"
    },
    {
      "key": "032811f",
      "nodeTitle": "director_task_done",
      "sourceText": "Bene. Hai parlato con tutti gli ANIMALI.",
      "sourceEnText": "Good. You talked to all ANIMALS.",
      "audioFile": "pl_04_032811f.ogg",
      "voiceProfile": "francesco",
      "actorId": "ADULT_M",
      "durationMs": 2534,
      "updatedAt": "2025-10-09T06:16:15Z",
      "textHash": "6001d03d9a25caed40818661e1087dd53f4cd3879eb477767c46386cc504d311"
    },
    {
      "key": "03321d5",
      "nodeTitle": "item_chest",
      "sourceText": "Hai trovato dei biscotti!",
      "sourceEnText": "You found some cookies!",
      "audioFile": "pl_04_03321d5.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T08:38:36Z",
      "textHash": "05a00055b0d575c9b804b3373678f782c7c331b466558ce0fce1658b284283bb"
    },
    {
      "key": "0364f30",
      "nodeTitle": "director_task_done",
      "sourceText": "Ora passiamo in rassegna i fatti.",
      "sourceEnText": "Now let's review facts.",
      "audioFile": "pl_04_0364f30.ogg",
      "voiceProfile": "francesco",
      "actorId": "ADULT_M",
      "durationMs": 1985,
      "updatedAt": "2025-10-09T06:16:17Z",
      "textHash": "e655f0050b68314c5feb0f488a5ae3d8d6b3d2d8d1c4ffadad6f0a7b7d114ab2"
    },
    {
      "key": "03a150c",
      "nodeTitle": "elephant_talk",
      "sourceText": "Ho una buona MEMORIA.",
      "sourceEnText": "I have great MEMORY.",
      "audioFile": "pl_04_03a150c.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1280,
      "updatedAt": "2025-10-09T06:13:47Z",
      "textHash": "9ed7e420a8ac2311496cb4cf94c4a34ad36d95372c0b3d822459e104ed7a29b4"
    },
    {
      "key": "03c4553",
      "nodeTitle": "animal_fox",
      "sourceText": "Hehe! In realtà non vengo dallo zoo.",
      "sourceEnText": "Hehe! I'm not actually from the zoo.",
      "audioFile": "pl_04_03c4553.ogg",
      "voiceProfile": "francesco",
      "actorId": "SPECIAL",
      "durationMs": 2638,
      "updatedAt": "2025-10-09T08:38:19Z",
      "textHash": "45109b8a6a74fd9e5a4f9fe0f370a73507aab948564e1960330d2ceb55b2c219"
    },
    {
      "key": "042266c",
      "nodeTitle": "lion_talk",
      "sourceText": "Vivo in un BRANCO.",
      "sourceEnText": "I live in a PRIDE.",
      "audioFile": "pl_04_042266c.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1097,
      "updatedAt": "2025-10-09T08:41:39Z",
      "textHash": "d0dcd9a74c1d74adb1c3412e44c9184b6d7b0a39fdf16dfd1396a492e745a2f4"
    },
    {
      "key": "048e8a1",
      "nodeTitle": "ELEPHANT_SIGN",
      "sourceText": "ELEFANTE. Pelle spessa. ORECCHIE grandi.",
      "sourceEnText": "ELEPHANT. Thick skin. Big EARS.",
      "audioFile": "pl_04_048e8a1.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 3056,
      "updatedAt": "2025-10-09T06:13:53Z",
      "textHash": "ecbf21acf53cc1e4998b0f673efeb7149fb3768025f6435ed03bf99e86832d4c"
    },
    {
      "key": "04b42f2",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Mangio le foglie di ACACIA!",
      "sourceEnText": "I eat WATTLE leaves!",
      "audioFile": "pl_04_04b42f2.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T08:42:46Z",
      "textHash": "4d9a4aa7c5ff524c5fb6d9e685fa89221f062a74944f569a2ba9ff3f0d385163"
    },
    {
      "key": "04d057e",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Forte, vero?",
      "sourceEnText": "Cool, right?",
      "audioFile": "pl_04_04d057e.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 914,
      "updatedAt": "2025-10-09T08:42:58Z",
      "textHash": "26122b5f28fd057b61f3204a1da499d41cb8653fa93b5870f3eb9ccafd89e0b0"
    },
    {
      "key": "04f37fa",
      "nodeTitle": "giraffe_talk",
      "sourceText": "Vuoi riprovare?",
      "sourceEnText": "Want to try again?",
      "audioFile": "pl_04_04f37fa.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1071,
      "updatedAt": "2025-10-09T08:42:55Z",
      "textHash": "27f264f6d3104ba641a4b30e5bbdb3dfc34ebd506c006fb1ff6aed6813176aed"
    },
    {
      "key": "04fff6b",
      "nodeTitle": "animal_peacock",
      "sourceText": "La mia coda è una BANDIERA!",
      "sourceEnText": "My tail is a FLAG!",
      "audioFile": "pl_04_04fff6b.ogg",
      "voiceProfile": "francesco",
      "actorId": "SPECIAL",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T08:43:47Z",
      "textHash": "f317fb2c7195c564d48608dae85119b43685ee36b6ee5d24e834f03bd3bcbf2b"
    },
    {
      "key": "0540c5a",
      "nodeTitle": "penguin_talk",
      "sourceText": "Non so volare, ma nuoto bene!",
      "sourceEnText": "I can't fly, but I swim well!",
      "audioFile": "pl_04_0540c5a.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T06:14:45Z",
      "textHash": "5b15a44c248d95878958dd7212b6b510f4853d34ba12a2af8925022eb5c7de5e"
    },
    {
      "key": "054cc37",
      "nodeTitle": "quest_end",
      "sourceText": "Hai imparato a conoscere gli ANIMALI DELLO ZOO.",
      "sourceEnText": "You learned about ZOO ANIMALS.",
      "audioFile": "pl_04_054cc37.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 2586,
      "updatedAt": "2025-10-09T06:16:35Z",
      "textHash": "188b7d9250f14fd56a88fe79b631536d428dcf5ac6995ca4add89ecc309eadee"
    },
    {
      "key": "056d51c",
      "nodeTitle": "node",
      "sourceText": "Ottimo lavoro allo ZOO!",
      "sourceEnText": "Great work at the ZOO!",
      "audioFile": "pl_04_056d51c.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1489,
      "updatedAt": "2025-10-09T06:15:16Z",
      "textHash": "7a0c0c268ce872369042daf278dc820edc338e18d8556656714ab00e8da682da"
    },
    {
      "key": "05ac327",
      "nodeTitle": "PENGUIN_KID",
      "sourceText": "Sembra ordinato.",
      "sourceEnText": "It looks neat.",
      "audioFile": "pl_04_05ac327.ogg",
      "voiceProfile": "francesco",
      "actorId": "KID_F",
      "durationMs": 1097,
      "updatedAt": "2025-10-09T06:14:55Z",
      "textHash": "6df908385a55c3e07d13dc316611cd2daa1a229b4671f309b8a193dd97e84447"
    },
    {
      "key": "05da6d7",
      "nodeTitle": "lion_activity_done",
      "sourceText": "Devi trovare la BANDIERA!",
      "sourceEnText": "You must find the FLAG!",
      "audioFile": "pl_04_05da6d7.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1437,
      "updatedAt": "2025-10-09T06:16:01Z",
      "textHash": "e79742767b19e6f65df084110ffaae51a3cc8562935a07b3f7b1217b9b1d9de9"
    },
    {
      "key": "05f345e",
      "nodeTitle": "node",
      "sourceText": "Abbina gli ANIMALI ai FATTI.",
      "sourceEnText": "Match ANIMALS to FACTS.",
      "audioFile": "pl_04_05f345e.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1672,
      "updatedAt": "2025-10-09T06:14:59Z",
      "textHash": "1c148d5bd2714d80a5f8bdfe72a884171e38be196fd1aaba8a7371be9a9ab27e"
    },
    {
      "key": "068daeb",
      "nodeTitle": "GIRAFFE_KID",
      "sourceText": "Con un COLLO così!",
      "sourceEnText": "With a NECK like that!",
      "audioFile": "pl_04_068daeb.ogg",
      "voiceProfile": "francesco",
      "actorId": "KID_F",
      "durationMs": 1332,
      "updatedAt": "2025-10-09T08:42:20Z",
      "textHash": "bb17a9d8fcddccb9440efa6a1197f7940d58a329c9c5adce4a2038835392f84e"
    },
    {
      "key": "074fe29",
      "nodeTitle": "animal_fox",
      "sourceText": "Non ho visto nessuna bandiera.",
      "sourceEnText": "I didn't see a flag anywhere.",
      "audioFile": "pl_04_074fe29.ogg",
      "voiceProfile": "francesco",
      "actorId": "SPECIAL",
      "durationMs": 1698,
      "updatedAt": "2025-10-09T08:38:26Z",
      "textHash": "d1a1c1b54ebb65c727921d2f44ef2478cece7da64a1fe531483ae713fa3c319a"
    },
    {
      "key": "0774b92",
      "nodeTitle": "lion_talk",
      "sourceText": "Cresco piccolo, poi medio, poi vecchio...",
      "sourceEnText": "I grow up small, then medium, then old...",
      "audioFile": "pl_04_0774b92.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 2638,
      "updatedAt": "2025-10-09T08:41:28Z",
      "textHash": "b0676106510103ef973e0f87523e304607e802c64a0c3df6cfe72bef9eb14916"
    },
    {
      "key": "078190f",
      "nodeTitle": "penguin_activity_done",
      "sourceText": "No, non ho preso la BANDIERA.",
      "sourceEnText": "No, I did not take the FLAG.",
      "audioFile": "pl_04_078190f.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1620,
      "updatedAt": "2025-10-09T06:16:09Z",
      "textHash": "77165a9e71fce4c29edd5f116f1a98acfed2d236142164c82fe0a072aaad6bd8"
    },
    {
      "key": "079451b",
      "nodeTitle": "monkey_talk",
      "sourceText": "Dove ho preso questa FRUTTA?",
      "sourceEnText": "Where did I get these FRUITS?",
      "audioFile": "pl_04_079451b.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1567,
      "updatedAt": "2025-10-09T06:14:33Z",
      "textHash": "339c134d6984ee4de2cc2f4a1ed8b13596a02032cfe59593fa6ab642dd7d0a87"
    },
    {
      "key": "079f4e0",
      "nodeTitle": "LION_KID",
      "sourceText": "Che boato!",
      "sourceEnText": "What a roar!",
      "audioFile": "pl_04_079f4e0.ogg",
      "voiceProfile": "francesco",
      "actorId": "KID_F",
      "durationMs": 862,
      "updatedAt": "2025-10-09T06:14:26Z",
      "textHash": "f1f3cfadaa5321c0f583afb5f52c853fbdda63d7331fd16dbb6576d2773c3d49"
    },
    {
      "key": "07f2e15",
      "nodeTitle": "lion_talk",
      "sourceText": "Sono un LEONE e vengo dall'AFRICA.",
      "sourceEnText": "I am a LION from AFRICA.",
      "audioFile": "pl_04_07f2e15.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 2090,
      "updatedAt": "2025-10-09T09:13:55Z",
      "textHash": "cc478deae78529dfd530049e8a826f9b9d61b302a1a15489a4b700608561468f"
    },
    {
      "key": "0809ac5",
      "nodeTitle": "post_quest_activity",
      "sourceText": "Disegna il tuo ANIMALE preferito.",
      "sourceEnText": "Draw your favorite ANIMAL.",
      "audioFile": "pl_04_0809ac5.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 1855,
      "updatedAt": "2025-10-09T06:16:37Z",
      "textHash": "46a26d278904f15f32bf2b30e741e8ec0cb999d25478f25b1b79d41a737f38f9"
    },
    {
      "key": "080d627",
      "nodeTitle": "lion_talk",
      "sourceText": "Lo renderò più difficile.",
      "sourceEnText": "I'll make it harder.",
      "audioFile": "pl_04_080d627.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1515,
      "updatedAt": "2025-10-09T08:42:03Z",
      "textHash": "3689099f8cd9ca5b49a57f18aa4b41d226d9c3152d5b6565af896b9da0cfe74e"
    },
    {
      "key": "0810771",
      "nodeTitle": "item_chest",
      "sourceText": "Ora è vuoto.",
      "sourceEnText": "It's empty now.",
      "audioFile": "pl_04_0810771.ogg",
      "voiceProfile": "francesco",
      "actorId": "Default",
      "durationMs": 967,
      "updatedAt": "2025-10-09T08:38:32Z",
      "textHash": "d3bb0a4aa967faf533c738ae5d2cd2efaff01af211d13b1377b77ec7fced62f4"
    },
    {
      "key": "084fedc",
      "nodeTitle": "penguin_talk",
      "sourceText": "Riesci a farlo di nuovo?",
      "sourceEnText": "Can you do it again?",
      "audioFile": "pl_04_084fedc.ogg",
      "voiceProfile": "francesco",
      "actorId": "SENIOR_M",
      "durationMs": 1384,
      "updatedAt": "2025-10-09T08:40:38Z",
      "textHash": "468782b7eef02b5a903c40734a2de69872af2ce6d5795603ba6a2ce463f869d3"
    },
    {
      "key": "08568f5",
      "nodeTitle": "penguin_activity_done",
      "sourceText": "Non so volare, ricordi?",
      "sourceEnText": "I can't fly, remember?",
      "audioFile": "pl_04_08568f5.ogg",
      "voiceProfile": "francesco",
