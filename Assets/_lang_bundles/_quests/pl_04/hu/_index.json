  "lines": [
    {
      "key": "001cba3",
      "nodeTitle": "director_task",
      "sourceText": "90 méter magas!",
      "sourceEnText": "It's 90 meters tall!",
      "audioFile": "pl_04_001cba3.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1384,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "1bc97027a1df4c77685084876820bf8159ca55f759e9bca94d65b5c1d6f28d7c"
    },
    {
      "key": "005dd46",
      "nodeTitle": "quest_start",
      "sourceText": "Lengyelország legnagyobb állatkertje!",
      "sourceEnText": "The biggest zoo in Poland!",
      "audioFile": "pl_04_005dd46.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 1985,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "9006f8e719baf443348fd95a2e330200ef9e5c7bdc4bd425deac749a9b094cad"
    },
    {
      "key": "012b933",
      "nodeTitle": "director_task",
      "sourceText": "Talán valamelyiküknek megvan.",
      "sourceEnText": "Maybe one of them has it.",
      "audioFile": "pl_04_012b933.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1802,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "3047be2209f1a04d82b787fe943ae14b2b91869af1a4b1f20f5c2fcc055dcce5"
    },
    {
      "key": "02f35e4",
      "nodeTitle": "director_task",
      "sourceText": "A CENTENÁRIUM TEREMÉBEN a Tornyon került sor.",
      "sourceEnText": "It was on the IGLICA at CENTENNIAL HALL.",
      "audioFile": "pl_04_02f35e4.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2769,
      "updatedAt": "2026-01-08T15:39:52Z",
      "textHash": "36a79d93dacac892a27fa9335ebfba0bbfa69f7cedbc8db77da7a148959a249e"
    },
    {
      "key": "032811f",
      "nodeTitle": "director_task_done",
      "sourceText": "Minden állattal beszéltél.",
      "sourceEnText": "You talked to all ANIMALS.",
      "audioFile": "pl_04_032811f.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1698,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "a55eec1508b411a577be54cf45a220032ccbfa9cf5296c9b3cb5834850876ffc"
    },
    {
      "key": "0335bf7",
      "nodeTitle": "director_task",
      "sourceText": "Vékony fém torony, városunk jelképe.",
      "sourceEnText": "It is a thin metal tower and a symbol of our city.",
      "audioFile": "pl_04_0335bf7.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2220,
      "updatedAt": "2026-01-08T15:46:02Z",
      "textHash": "481402fe5e809a11271898fa8bbdbb218229eec43dc79119b09a1ccc7fae92b4"
    },
    {
      "key": "0364f30",
      "nodeTitle": "director_task_done",
      "sourceText": "És nem vitték el a FLAG-ot.",
      "sourceEnText": "And they didn't take the FLAG.",
      "audioFile": "pl_04_0364f30.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1567,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "5f3d2c0fdc03d9a312ef49f354c1b6e386f2a0cf8f2185f49fe4912a259d594c"
    },
    {
      "key": "07b60e7",
      "nodeTitle": "director",
      "sourceText": "Beszélj az összes ÁLLATTAL!",
      "sourceEnText": "Talk to all the ANIMALS!",
      "audioFile": "pl_04_07b60e7.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1620,
      "updatedAt": "2026-01-08T15:37:06Z",
      "textHash": "adbfe80638b6b3bd9c32f265a9cc84f1748d620f7ef73ecf8226fbc75bea1f18"
    },
    {
      "key": "0815405",
      "nodeTitle": "director_task",
      "sourceText": "Sziasztok, én vagyok az állatkert igazgatója.",
      "sourceEnText": "Hi, I am the Zoo Director.",
      "audioFile": "pl_04_0815405.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 2534,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "a1a62e992ea0e7cecb87d173616a26bc8b1bd174f2e920bb244bca512bc95d41"
    },
    {
      "key": "09c6bf7",
      "nodeTitle": "director_task",
      "sourceText": "A ZÁSZLÓNK hiányzik!",
      "sourceEnText": "Our FLAG is missing!",
      "audioFile": "pl_04_09c6bf7.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1567,
      "updatedAt": "2025-11-15T09:55:53Z",
      "textHash": "f6be9524564abb67d9c314bc52e3f59fd4c67ed8d04c6974ff4eebccd2b3fea4"
    },
    {
      "key": "0bc6238",
      "nodeTitle": "director_task_done",
      "sourceText": "Térjünk vissza a Centennial Hallba.",
      "sourceEnText": "Let's go back to the Centennial Hall.",
      "audioFile": "pl_04_0bc6238.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1985,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "57c9f680f30cf0530f7a85b748ba74c551302e11f3bec239cadfc098a66518c6"
    },
    {
      "key": "0c71176",
      "nodeTitle": "director_task",
      "sourceText": "Ma van egy problémánk.",
      "sourceEnText": "Today we have a problem.",
      "audioFile": "pl_04_0c71176.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1384,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "6b5ee882be313d0956b49096ff98c46af3bbfc40513654219a1fd8de92e23df7"
    },
    {
      "key": "0da284c",
      "nodeTitle": "director_task",
      "sourceText": "Keresd meg a ZÁSZLÓT. Beszélj az ÁLLATOKKAL.",
      "sourceEnText": "Find the FLAG. Talk to the ANIMALS.",
      "audioFile": "pl_04_0da284c.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 3056,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "945cc93f09de8350070ce69f3494961b81a5f22a6113642ece3736ddd119c461"
    },
    {
      "key": "0fc7e35",
      "nodeTitle": "director_task",
      "sourceText": "Ki vehette el?",
      "sourceEnText": "Who could have taken it?",
      "audioFile": "pl_04_0fc7e35.ogg",
      "voiceProfile": "shelley",
      "actorId": "ADULT_M",
      "durationMs": 1202,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "cc69faf69eb1424f881c7b852410f1d82631655c7c4832cf537c1ce06aa9ec51"
    },
    {
      "key": "0fe55d1",
      "nodeTitle": "quest_start",
      "sourceText": "Üdvözöljük a WROCŁAW-I ÁLLATKERTBEN!",
      "sourceEnText": "Welcome to WROCŁAW ZOO.",
      "audioFile": "pl_04_0fe55d1.ogg",
      "voiceProfile": "alice",
      "actorId": "Default",
      "durationMs": 2508,
      "updatedAt": "2025-12-06T13:33:06Z",
      "textHash": "a026e8b698b7f0f668a9ba04e50c2059fe2c5adb7dd875ce894bb7559060f8d8"
    }
  ]
}