  m_Script: {fileID: 11500000, guid: e9620f8c34305754d8cc9a7e49e852d9, type: 3}
  m_Name: FR_01_it
  m_EditorClassIdentifier: Unity.Localization::UnityEngine.Localization.Tables.StringTable
  m_LocaleId:
    m_Code: it
  m_SharedData: {fileID: 11400000, guid: a2d282f2ce2954d209bda497fbc2e661, type: 2}
  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 1408135847940
    m_Localized: "donna: Questo \xE8 l'ingresso del Louvre, il nostro museo d'arte
      nazionale."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847941
    m_Localized: 'donna: Vuoi entrare?'
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847942
    m_Localized: "S\xCC"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847943
    m_Localized: Buona visita!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847944
    m_Localized: NO
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847945
    m_Localized: Va bene.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847946
    m_Localized: "La Torre Eiffel \xE8 alta 300 metri."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847947
    m_Localized: Costruito dal signor Eiffel nel 1887.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847948
    m_Localized: "\xC8 fatto di ferro!"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847949
    m_Localized: Ho visto Antura dirigersi verso Notre Dame.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847950
    m_Localized: Arrivate!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847951
    m_Localized: Ecco il tuo biglietto.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847952
    m_Localized: Ho visto Antura salire in cima alla torre.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847953
    m_Localized: Prendi l'ascensore!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847954
    m_Localized: Raccogli tutte le monete!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847955
    m_Localized: Il biglietto per la Torre Eiffel costa 3 monete.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847956
    m_Localized: Sono il sindaco di Parigi.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847957
    m_Localized: "Questa \xE8 la cattedrale di Notre-Dame."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847958
    m_Localized: "\xC8 una famosa chiesa gotica, costruita nel 1182."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847959
    m_Localized: Vieni con me sul tetto della chiesa!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847960
    m_Localized: OTTIMO! Ora posso cuocere la baguette. E...
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847961
    m_Localized: "CONGRATULAZIONI! Hai vinto la partita! Ti \xE8 piaciuta?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847962
    m_Localized: Aiutatemi! Antura ha fatto un pasticcio nella mia cucina!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847963
    m_Localized: Non riesco a trovare gli ingredienti per preparare la baguette.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847964
    m_Localized: Il nostro pane francese speciale!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847965
    m_Localized: 'Per favore portami 4 ingredienti:'
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847966
    m_Localized: farina, acqua, lievito e sale.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847967
    m_Localized: Si possono trovare numerose sculture e dipinti.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847968
    m_Localized: Vai a trovare la Monna Lisa!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847969
    m_Localized: "Ora cerca Antura! \xC8 andata in panetteria a prendere una baguette!"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847970
    m_Localized: Affrettarsi!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847971
    m_Localized: "Questo \xE8 sale."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847972
    m_Localized: "Questa \xE8 farina."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847973
    m_Localized: "Questa \xE8 acqua."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847974
    m_Localized: "Questo \xE8 lievito."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847975
    m_Localized: "Nel 2019 c'\xE8 stato un grande incendio, ma siamo riusciti a ripararlo."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847976
    m_Localized: Ho visto Antura correre al Museo del Louvre.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847977
    m_Localized: Si trova proprio dall'altra parte della Senna.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847978
    m_Localized: Benvenuti al Museo del Louvre. Cosa volete fare?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847979
    m_Localized: Parlami del Louvre
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847980
    m_Localized: Uscita
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847981
    m_Localized: Ritorno!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847984
    m_Localized: "La Libert\xE0 che guida il popolo, dell'artista francese Eug\xE8ne
      Delacroix, rappresenta la libert\xE0."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847985
    m_Localized: La Venere di Milo, antica scultura greca in marmo.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847986
    m_Localized: Ciao. Cosa vuoi sapere?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847987
    m_Localized: "Cos'\xE8 la Torre Eiffel?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847988
    m_Localized: La famosa torre di ferro, alta 300 metri.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847989
    m_Localized: Il simbolo di Parigi!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847990
    m_Localized: Dove siamo?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847991
    m_Localized: "Siamo a Parigi, la citt\xE0 dell'amore!"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847992
    m_Localized: "Questo posto \xE8 reale?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847993
    m_Localized: "Certo! Perch\xE9 me lo chiedi?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847994
    m_Localized: "Beh... sembra un videogioco, non \xE8 vero?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847995
    m_Localized: Niente. Ciao.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847996
    m_Localized: Hai qualche domanda?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847997
    m_Localized: Hai visto Antura?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847998
    m_Localized: "S\xEC! Parla con tutti e segui le luci!"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847999
    m_Localized: "No. Chi \xE8 Antura?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848000
    m_Localized: Cosa fai?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848001
    m_Localized: Vado a lavorare!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848002
    m_Localized: Vado a comprare il pane al panificio.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848003
    m_Localized: Da dove vieni?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848004
    m_Localized: Non sono nato in questo paese.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848005
    m_Localized: Dal pianeta Terra.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848006
    m_Localized: Arrivederci
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848007
    m_Localized: CIAO!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848008
    m_Localized: Come stai?
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940736
    m_Localized: "Questa \xE8 la famosa Monna Lisa."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940737
    m_Localized: Leonardo lo dipinse intorno al 1500.
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940738
    m_Localized: dell'artista Leonardo da Vinci.
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940739
    m_Localized: "Questo dipinto rappresenta la libert\xE0."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940740
    m_Localized: "Si chiama La libert\xE0 che guida il popolo."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940741
    m_Localized: "dell'artista francese Eug\xE8ne Delacroix."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870144
    m_Localized: Benvenuti a Parigi!
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870145
    m_Localized: Vai a parlare con il tutor!
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870147
    m_Localized: Ho visto Antura andare alla Torre Eiffel.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870148
    m_Localized: Segui la luce o usa la mappa!
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870149
    m_Localized: Andateci subito!
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870152
    m_Localized: "Questa piramide di vetro \xE8 l'ingresso principale del museo."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870153
    m_Localized: "Fu costruito negli anni '80 per accogliere pi\xF9 visitatori."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870154
    m_Localized: Questa pietra ha leggi molto antiche, risalenti all'antica Mesopotamia.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870155
    m_Localized: Furono scritti quasi 4.000 anni fa.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870156
    m_Localized: Questo grande dipinto raffigura Napoleone che diventa imperatore.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870157
    m_Localized: L'artista Jacques-Louis David dipinse molti dettagli.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870158
    m_Localized: Questo dipinto raffigura due fratelli che fanno una promessa coraggiosa.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870159
    m_Localized: Insegna il dovere e il coraggio dell'antica Roma.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870160
    m_Localized: Questa statua raffigura un uomo che scrive nell'antico Egitto.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870161
    m_Localized: I suoi occhi sembrano molto reali e luminosi.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870162
    m_Localized: Questa statua raffigura una figura alata che atterra su una nave.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870163
    m_Localized: Il vento modella i suoi vestiti e le sue ali.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870166
    m_Localized: Nel 1889 ci fu una grande fiera a Parigi.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870168
    m_Localized: Per quell'evento fu costruita la Torre Eiffel.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870172
    m_Localized: "Il Louvre \xE8 uno dei musei pi\xF9 grandi del mondo."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870173
    m_Localized: "Si pu\xF2 camminare qui per ore e non riuscire a vedere tutto."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870174
    m_Localized: "Molte delle opere d'arte qui esposte sono pi\xF9 vecchie dei nonni
      dei tuoi nonni."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870175
    m_Localized: La piramide di vetro lascia entrare la luce nei corridoi sottostanti.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870176
    m_Localized: "Notre-Dame \xE8 una famosa cattedrale gotica."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870177
    m_Localized: "I costruttori iniziarono a costruirlo pi\xF9 di 800 anni fa."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870178
    m_Localized: "Le grandi campane risuonano in tutta la citt\xE0."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870179
    m_Localized: "Dal tetto si pu\xF2 ammirare gran parte di Parigi."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870180
    m_Localized: Qui siedono delle creature di pietra chiamate gargoyle.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870181
    m_Localized: Gli operai stanno ancora restaurando alcune parti della cattedrale.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870182
    m_Localized: Il profumo del pane fresco rende le persone felici.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870183
    m_Localized: Per preparare la baguette si usano farina, acqua, lievito e sale.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870184
    m_Localized: I fornai si svegliano molto presto per iniziare a preparare l'impasto.
    m_Metadata:
      m_Items: []
  - m_Id: 2484251529306112
    m_Localized: "Perch\xE9 non disegni la Torre Eiffel?"
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500416
    m_Localized: Guarda quella statua!
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500417
    m_Localized: "Non \xE8 spaventoso?"
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500418
    m_Localized: Vorrei salire sulla Torre Eiffel.
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500419
    m_Localized: "Per salire \xE8 necessario un biglietto."
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500420
    m_Localized: "L'evento si \xE8 svolto per celebrare il centenario della Rivoluzione
      francese."
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500421
    m_Localized: Adoro Parigi!
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500422
    m_Localized: Buongiorno!
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500423
    m_Localized: Ca va?
    m_Metadata:
      m_Items: []
  references:
    version: 2
    RefIds: []
