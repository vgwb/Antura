  m_Name: FR_01_fr
  m_EditorClassIdentifier: Unity.Localization::UnityEngine.Localization.Tables.StringTable
  m_LocaleId:
    m_Code: fr
  m_SharedData: {fileID: 11400000, guid: a2d282f2ce2954d209bda497fbc2e661, type: 2}
  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 1408135847940
    m_Localized: "femme : C'est l'entr\xE9e du Louvre, notre mus\xE9e d'art national."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847941
    m_Localized: 'femme : Tu veux entrer ?'
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847942
    m_Localized: Oui
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847943
    m_Localized: Bonne visite !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847944
    m_Localized: Non
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847945
    m_Localized: D'accord.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847946
    m_Localized: "La tour Eiffel mesure 300 m\xE8tres de haut."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847947
    m_Localized: Construit par Monsieur Eiffel en 1887.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847948
    m_Localized: C'est fait de fer !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847949
    m_Localized: J'ai vu Antura se diriger vers Notre Dame.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847950
    m_Localized: Allez-y !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847951
    m_Localized: Voici votre billet.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847952
    m_Localized: J'ai vu Antura monter au sommet de la tour.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847953
    m_Localized: Prenez l'ascenseur !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847954
    m_Localized: "Collectez toutes les pi\xE8ces !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847955
    m_Localized: "Le billet pour la Tour Eiffel co\xFBte 3 pi\xE8ces."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847956
    m_Localized: Je suis le maire de Paris.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847957
    m_Localized: "C'est la cath\xE9drale Notre-Dame."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847958
    m_Localized: "C'est une c\xE9l\xE8bre \xE9glise gothique, construite en 1182."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847959
    m_Localized: "Viens avec moi sur le toit de l'\xE9glise !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847960
    m_Localized: "Super ! Je peux maintenant faire cuire la baguette. Et\u2026"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847961
    m_Localized: "F\xC9LICITATIONS ! Vous avez gagn\xE9 ! Vous avez aim\xE9 ?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847962
    m_Localized: Au secours ! Antura a mis le bazar dans ma cuisine !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847963
    m_Localized: "Je ne trouve pas les ingr\xE9dients pour faire la baguette."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847964
    m_Localized: "Notre pain fran\xE7ais sp\xE9cial !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847965
    m_Localized: "S'il vous pla\xEEt, apportez-moi 4 ingr\xE9dients :"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847966
    m_Localized: farine, eau, levure et sel.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847967
    m_Localized: "Vous pourrez y d\xE9couvrir de nombreuses sculptures et peintures."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847968
    m_Localized: Allez chercher la Joconde !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847969
    m_Localized: "Maintenant, cherchez Antura ! Elle est all\xE9e \xE0 la boulangerie
      chercher une baguette !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847970
    m_Localized: "D\xE9p\xEAche-toi!"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847971
    m_Localized: C'est du sel.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847972
    m_Localized: C'est de la farine.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847973
    m_Localized: C'est de l'eau.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847974
    m_Localized: C'est de la levure.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847975
    m_Localized: "Il y a eu un gros incendie en 2019, mais nous avons pu le r\xE9parer."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847976
    m_Localized: "J'ai vu Antura courir dans le mus\xE9e du Louvre."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847977
    m_Localized: "C'est juste de l'autre c\xF4t\xE9 de la Seine."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847978
    m_Localized: "Bienvenue au mus\xE9e du Louvre. Que souhaitez-vous faire\_?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847979
    m_Localized: Parlez-moi du Louvre
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847980
    m_Localized: Sortie
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847981
    m_Localized: Revenir!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847984
    m_Localized: "La Libert\xE9 guidant le peuple de l'artiste fran\xE7ais Eug\xE8ne
      Delacroix repr\xE9sente la libert\xE9."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847985
    m_Localized: "La V\xE9nus de Milo, une sculpture en marbre de la Gr\xE8ce antique."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847986
    m_Localized: Salut. Que veux-tu savoir ?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847987
    m_Localized: Qu'est-ce que la Tour Eiffel ?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847988
    m_Localized: "La c\xE9l\xE8bre tour de fer, haute de 300 m\xE8tres."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847989
    m_Localized: Le symbole de Paris !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847990
    m_Localized: "O\xF9 sommes-nous?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847991
    m_Localized: "Nous sommes \xE0 Paris, la ville de l'amour !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847992
    m_Localized: "Cet endroit est-il r\xE9el ?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847993
    m_Localized: "Bien s\xFBr ! Pourquoi demandes-tu \xE7a ?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847994
    m_Localized: "Eh bien... \xE7a ressemble \xE0 un jeu vid\xE9o, n'est-ce pas ?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847995
    m_Localized: Rien. Au revoir.
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847996
    m_Localized: Avez-vous des questions?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847997
    m_Localized: Avez-vous vu Antura ?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847998
    m_Localized: "Oui ! Parlez \xE0 tout le monde et suivez les lumi\xE8res !"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135847999
    m_Localized: Non. Qui est Antura ?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848000
    m_Localized: Que fais-tu?
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848001
    m_Localized: Je vais au travail !
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848002
    m_Localized: "Je vais acheter du pain \xE0 la boulangerie."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848003
    m_Localized: "D'o\xF9 viens-tu?"
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848004
    m_Localized: "Je ne suis pas n\xE9 dans ce pays."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848005
    m_Localized: "De la plan\xE8te Terre."
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848006
    m_Localized: Au revoir
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848007
    m_Localized: Salut!
    m_Metadata:
      m_Items: []
  - m_Id: 1408135848008
    m_Localized: Comment vas-tu?
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940736
    m_Localized: "C'est la c\xE9l\xE8bre Mona Lisa."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940737
    m_Localized: "L\xE9onard l'a peint vers 1500."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940738
    m_Localized: "par l'artiste L\xE9onard de Vinci."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940739
    m_Localized: "Ce tableau repr\xE9sente la libert\xE9."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940740
    m_Localized: "Cela s'appelle La Libert\xE9 guidant le peuple."
    m_Metadata:
      m_Items: []
  - m_Id: 1025167277940741
    m_Localized: "par l'artiste fran\xE7ais Eug\xE8ne Delacroix."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870144
    m_Localized: "Bienvenue \xE0 Paris !"
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870145
    m_Localized: Va parler au tuteur !
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870147
    m_Localized: "J'ai vu Antura aller \xE0 la Tour Eiffel."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870148
    m_Localized: "Suivez la lumi\xE8re ou utilisez la carte !"
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870149
    m_Localized: Allez-y maintenant !
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870152
    m_Localized: "Cette pyramide de verre est l'entr\xE9e principale du mus\xE9e."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870153
    m_Localized: "Il a \xE9t\xE9 construit dans les ann\xE9es 1980 pour accueillir
      davantage de visiteurs."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870154
    m_Localized: "Cette pierre poss\xE8de de tr\xE8s anciennes lois de l'ancienne
      M\xE9sopotamie."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870155
    m_Localized: "Ils ont \xE9t\xE9 \xE9crits il y a pr\xE8s de 4 000 ans."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870156
    m_Localized: "Ce grand tableau montre Napol\xE9on devenant empereur."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870157
    m_Localized: "L'artiste Jacques-Louis David a peint de nombreux d\xE9tails."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870158
    m_Localized: "Ce tableau montre des fr\xE8res faisant une promesse courageuse."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870159
    m_Localized: Il enseigne le devoir et le courage depuis la Rome antique.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870160
    m_Localized: "Cette statue repr\xE9sente un homme en train d'\xE9crire dans l'\xC9gypte
      ancienne."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870161
    m_Localized: "Ses yeux semblent tr\xE8s r\xE9els et brillants."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870162
    m_Localized: "Cette statue repr\xE9sente une figure ail\xE9e atterrissant sur
      un navire."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870163
    m_Localized: "Le vent fa\xE7onne ses v\xEAtements et ses ailes."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870166
    m_Localized: "Il y a eu une grande foire \xE0 Paris en 1889."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870168
    m_Localized: "La Tour Eiffel a \xE9t\xE9 construite pour cet \xE9v\xE9nement."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870172
    m_Localized: "Le Louvre est l\u2019un des plus grands mus\xE9es du monde."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870173
    m_Localized: Vous pouvez marcher ici pendant des heures sans toujours tout voir.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870174
    m_Localized: "De nombreuses \u0153uvres d\u2019art ici sont plus anciennes que
      les grands-parents de vos grands-parents."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870175
    m_Localized: "La pyramide de verre laisse entrer la lumi\xE8re dans les salles
      situ\xE9es en dessous."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870176
    m_Localized: "Notre-Dame est une c\xE9l\xE8bre cath\xE9drale gothique."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870177
    m_Localized: "Les constructeurs ont commenc\xE9 \xE0 construire ce b\xE2timent
      il y a plus de 800 ans."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870178
    m_Localized: Les grosses cloches sonnent dans toute la ville.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870179
    m_Localized: Depuis le toit, vous pouvez voir une grande partie de Paris.
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870180
    m_Localized: "Des cr\xE9atures de pierre appel\xE9es gargouilles sont assises
      ici."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870181
    m_Localized: "Les ouvriers restaurent encore certaines parties de la cath\xE9drale."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870182
    m_Localized: "L\u2019odeur du pain frais rend les gens heureux."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870183
    m_Localized: "Une baguette utilise de la farine, de l\u2019eau, de la levure
      et du sel."
    m_Metadata:
      m_Items: []
  - m_Id: 2409346116870184
    m_Localized: "Les boulangers se l\xE8vent tr\xE8s t\xF4t pour commencer \xE0
      faire la p\xE2te."
    m_Metadata:
      m_Items: []
  - m_Id: 2484251529306112
    m_Localized: Pourquoi ne dessines-tu pas la Tour Eiffel ?
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500416
    m_Localized: Regardez cette statue !
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500417
    m_Localized: N'est-ce pas effrayant ?
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500418
    m_Localized: J'aimerais monter sur la Tour Eiffel.
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500419
    m_Localized: Il faut un ticket pour monter.
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500420
    m_Localized: "C'\xE9tait pour c\xE9l\xE9brer le 100e anniversaire de la R\xE9volution
      fran\xE7aise."
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500421
    m_Localized: J'adore Paris !
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500422
    m_Localized: Bonjour!
    m_Metadata:
      m_Items: []
  - m_Id: 2484251533500423
    m_Localized: "\xC7a va ?"
    m_Metadata:
      m_Items: []
  references:
    version: 2
    RefIds: []
