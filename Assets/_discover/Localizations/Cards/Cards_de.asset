  m_Metadata:
    m_Items: []
  m_TableData:
  - m_Id: 8508073304064
    m_Localized: Jules Verne
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692672
    m_Localized: Ein Schriftsteller, der spannende Abenteuer erfand. Er schrieb Geschichten
      wie '20.000 Meilen unter dem Meer'.
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692673
    m_Localized: Louvre
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692674
    m_Localized: "Ein riesiges Museum voller Kunst. Hier h\xE4ngt die Mona Lisa."
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692675
    m_Localized: Notre-Dame de Paris
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692676
    m_Localized: "Eine sch\xF6ne Kathedrale in Paris. Die bunten Glasfenster erz\xE4hlen
      Geschichten."
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692677
    m_Localized: Eiffelturm
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692678
    m_Localized: Ein hoher Turm und Symbol von Paris. Man kann hinauf und die ganze
      Stadt sehen.
    m_Metadata:
      m_Items: []
  - m_Id: 8508081692679
    m_Localized: Marseillaise
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886976
    m_Localized: "Die franz\xF6sische Nationalhymne. Man singt sie bei gro\xDFen
      Ereignissen und Spielen."
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886977
    m_Localized: Baguette
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886978
    m_Localized: "Ein langes franz\xF6sisches Brot. Au\xDFen knusprig und innen weich."
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886979
    m_Localized: "Br\xFCcken f\xFCr Autos"
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886980
    m_Localized: "Br\xFCcken, die f\xFCr Autos gebaut wurden, um Fl\xFCsse oder Stra\xDFen
      zu \xFCberqueren. Manche sind sehr lang."
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886981
    m_Localized: "Fu\xDFg\xE4ngerbr\xFCcken"
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886982
    m_Localized: "Kleine Br\xFCcken f\xFCr Fu\xDFg\xE4nger. Sie sch\xFCtzen vor dem
      Verkehr."
    m_Metadata:
      m_Items: []
  - m_Id: 8508085886983
    m_Localized: "Br\xFCcken f\xFCr Z\xFCge"
    m_Metadata:
      m_Items: []
  - m_Id: 8508090081280
    m_Localized: "Br\xFCcken, \xFCber die Z\xFCge Fl\xFCsse oder T\xE4ler \xFCberqueren.
      Manche haben tolle Aussichten."
    m_Metadata:
      m_Items: []
  - m_Id: 8508094275584
    m_Localized: "Charta der Laizit\xE4t"
    m_Metadata:
      m_Items: []
  - m_Id: 8508094275585
    m_Localized: Ein Satz Regeln, um den Glauben aller zu respektieren. Hilft, friedlich
      zusammenzuleben.
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469888
    m_Localized: Kindergarten
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469889
    m_Localized: "Schule f\xFCr kleine Kinder von 3 bis 5 Jahren. Man lernt beim
      Spielen."
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469891
    m_Localized: Grundschule
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469892
    m_Localized: "Schule f\xFCr Kinder von 6 bis 10 Jahren. Man lernt lesen, schreiben
      und rechnen."
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469893
    m_Localized: Mittelschule
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469894
    m_Localized: "Schule f\xFCr Kinder von 11 bis 15 Jahren. Man lernt mehr F\xE4cher."
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469895
    m_Localized: Gymnasium
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469896
    m_Localized: "Schule f\xFCr Jugendliche vor der Uni. Du w\xE4hlst deine Lieblingsf\xE4cher."
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469897
    m_Localized: "Franz\xF6sische Flagge"
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469898
    m_Localized: "Die franz\xF6sische Flagge ist blau, wei\xDF und rot. Sie steht
      f\xFCr Freiheit."
    m_Metadata:
      m_Items: []
  - m_Id: 8508098469899
    m_Localized: Zug
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664192
    m_Localized: "Z\xFCge bringen schnell Menschen und G\xFCter durchs Land."
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664193
    m_Localized: "Hei\xDFluftballon"
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664194
    m_Localized: "Ein Ballon, der mit hei\xDFer Luft fliegt. Der erste startete in
      Frankreich."
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664195
    m_Localized: U-Boot (Die Nautilus)
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664196
    m_Localized: Ein U-Boot aus Jules Vernes Geschichte. Es erforschte die Tiefsee.
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664197
    m_Localized: Rakete
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664198
    m_Localized: Eine Rakete, die ins All fliegt. Frankreich hilft beim Start von
      Satelliten.
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664199
    m_Localized: 24 Stunden von Le Mans
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664200
    m_Localized: "Ein Autorennen \xFCber 24 Stunden. Man f\xE4hrt Tag und Nacht."
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664201
    m_Localized: "Stra\xDFe: STOP-Schild"
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664202
    m_Localized: Ein rotes Schild, das bedeutet, dass man anhalten muss.
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664203
    m_Localized: "Stra\xDFe: Gefahrenschild"
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664204
    m_Localized: Ein Schild, das vor Gefahr warnt. Sei vorsichtig.
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664205
    m_Localized: "Stra\xDFe: Die Ampel"
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664206
    m_Localized: "Ein Licht in Rot, Gelb und Gr\xFCn zur Verkehrsregelung."
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664207
    m_Localized: Mont Blanc
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664208
    m_Localized: "Der h\xF6chste Berg Westeuropas. Immer schneebedeckt."
    m_Metadata:
      m_Items: []
  - m_Id: 8508102664209
    m_Localized: "B\xE4cker"
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858496
    m_Localized: "Eine Person, die Brot, Kuchen und Geb\xE4ck macht."
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858497
    m_Localized: "Fischh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858498
    m_Localized: "Eine Person, die frischen Fisch und Meeresfr\xFCchte verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858499
    m_Localized: "K\xE4seh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858500
    m_Localized: "Eine Person, die viele K\xE4sesorten verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858501
    m_Localized: "Gem\xFCseh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858502
    m_Localized: "Eine Person, die frisches Obst und Gem\xFCse verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858503
    m_Localized: "Lebensmittelh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858504
    m_Localized: "Eine Person, die viele Lebensmittel und Getr\xE4nke verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858505
    m_Localized: Jean Michel Jarre
    m_Metadata:
      m_Items: []
  - m_Id: 8508106858506
    m_Localized: "Ein franz\xF6sischer Musiker, ber\xFChmt f\xFCr elektronische Musik.
      Macht gro\xDFe Lichtshows."
    m_Metadata:
      m_Items: []
  - m_Id: 17917000278016
    m_Localized: Fryderyk Chopin
    m_Metadata:
      m_Items: []
  - m_Id: 17917021249536
    m_Localized: "Polnischer Komponist und Klaviervirtuose. Er schrieb gef\xFChlsvolle
      Musik."
    m_Metadata:
      m_Items: []
  - m_Id: 17917021249537
    m_Localized: Nikolaus Kopernikus
    m_Metadata:
      m_Items: []
  - m_Id: 17917021249538
    m_Localized: "Ein Wissenschaftler, der sagte, dass die Erde um die Sonne kreist.
      Diese Idee ver\xE4nderte unser Weltbild."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443840
    m_Localized: "Pr\xE4sident"
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443841
    m_Localized: "Der gew\xE4hlte Anf\xFChrer des Landes. Die Menschen w\xE4hlen
      bei Wahlen."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443842
    m_Localized: "K\xF6nig Sigismund"
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443843
    m_Localized: "Ein ber\xFChmter K\xF6nig der polnischen Geschichte. In Warschau
      steht eine hohe S\xE4ule mit seiner Statue."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443844
    m_Localized: Wars und Sawa
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443845
    m_Localized: "Eine Legende \xFCber den Anfang Warschaus. Sie erz\xE4hlt von Zwillingen
      oder von einem Fischer und einer Meerjungfrau."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443846
    m_Localized: Meerjungfrau von Warschau
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443847
    m_Localized: Das Stadtsymbol ist eine mutige Meerjungfrau mit Schwert und Schild.
      Ihre Statue steht am Fluss.
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443848
    m_Localized: Marie Curie
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443849
    m_Localized: "Wissenschaftlerin, die Radioaktivit\xE4t erforschte und zwei Nobelpreise
      gewann. Sie entdeckte Polonium und Radium."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443850
    m_Localized: Polnische Flagge
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443851
    m_Localized: "Eine Flagge mit Wei\xDF oben und Rot unten. Sie zeigt Polens Farben."
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443852
    m_Localized: "Geld: Z\u0142oty"
    m_Metadata:
      m_Items: []
  - m_Id: 17917025443853
    m_Localized: "Die polnische W\xE4hrung hei\xDFt Z\u0142oty. M\xFCnzen und Scheine
      zeigen polnische Symbole."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638144
    m_Localized: Fluss Weichsel
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638145
    m_Localized: "Der l\xE4ngste Fluss Polens hei\xDFt Weichsel. Er flie\xDFt durch
      Krakau und Warschau."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638146
    m_Localized: 'Breslau: Altes Rathaus'
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638147
    m_Localized: "Ein gotisches Geb\xE4ude am Hauptplatz. Es hat eine ber\xFChmte
      Uhr."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638148
    m_Localized: 'Breslau: Kathedrale'
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638149
    m_Localized: Hohe Backsteinkirche auf einer Flussinsel. Man kann den Turm besteigen.
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638150
    m_Localized: 'Breslau: Sky Tower'
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638151
    m_Localized: "Eines der h\xF6chsten Geb\xE4ude Polens. Mit Gesch\xE4ften und
      Aussichtspunkt."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638152
    m_Localized: "Polnischer Zwerg (Wroc\u0142aw-Zwerge)"
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638153
    m_Localized: "Kleine Zwergenstatuen verstecken sich in Wroc\u0142aw. Sie zu finden
      macht Spa\xDF."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638154
    m_Localized: Fluss Oder
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638155
    m_Localized: "Ein gro\xDFer Fluss im Westen Polens. Er hilft Schiffen zu reisen
      und zu handeln."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638156
    m_Localized: Ostsee
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638157
    m_Localized: "Ein Meer im Norden Polens mit Sandstr\xE4nden. Man segelt und angelt
      dort."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638158
    m_Localized: Elefant
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638159
    m_Localized: "Ein gro\xDFes Tier mit R\xFCssel. Im Zoo mag es Duschen und Schlammb\xE4der."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638160
    m_Localized: Giraffe
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638161
    m_Localized: "Ein sehr gro\xDFes Tier mit langem Hals. Es frisst Bl\xE4tter von
      hohen B\xE4umen."
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638162
    m_Localized: "L\xF6we"
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832448
    m_Localized: "Eine gro\xDFe Katze, der K\xF6nig der Tiere. Lebt in Gruppen, den
      Rudeln."
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832449
    m_Localized: Affe
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832450
    m_Localized: "Ein schlaues Tier, das klettern und spielen kann. Manche leben
      in gro\xDFen Familien."
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832451
    m_Localized: Pinguin
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832452
    m_Localized: Ein Vogel, der nicht fliegt, aber sehr gut schwimmt. Er lebt in
      kalten Gegenden.
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832453
    m_Localized: Pierogi (Essen)
    m_Metadata:
      m_Items: []
  - m_Id: 17917033832454
    m_Localized: "Teigtaschen mit F\xFCllungen wie Kartoffeln, K\xE4se oder Obst.
      Ein ber\xFChmtes polnisches Gericht."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548228337664
    m_Localized: Elefant
    m_Metadata:
      m_Items: []
  - m_Id: 7717548232531968
    m_Localized: "Ein gro\xDFes Tier mit einem R\xFCssel. Elefanten sind die gr\xF6\xDFten
      Landtiere. Sie haben gro\xDFe Ohren und lieben es, Wasser zu spritzen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892096
    m_Localized: Giraffe
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892097
    m_Localized: "Ein sehr gro\xDFes Tier mit einem langen Hals. Giraffen sind gr\xF6\xDFer
      als B\xE4ume! Sie fressen Bl\xE4tter, die andere Tiere nicht erreichen k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892098
    m_Localized: "L\xF6we"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892099
    m_Localized: "Eine Gro\xDFkatze, die als K\xF6nig der Tiere gilt. Sie lebt in
      Gruppen, die als Rudel bezeichnet werden."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892100
    m_Localized: Affe
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892101
    m_Localized: "Ein intelligentes Tier, das klettern und spielen kann. Manche Affen
      leben in gro\xDFen Familien."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892102
    m_Localized: Pinguin
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892103
    m_Localized: Ein Vogel, der nicht fliegen kann, aber sehr gut schwimmt. Er lebt
      an kalten Orten.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892104
    m_Localized: "Die Freiheit f\xFChrt das Volk"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892105
    m_Localized: "Ein ber\xFChmtes Gem\xE4lde \xFCber Freiheit und Mut. Es zeigt
      eine mutige Frau, die die franz\xF6sische Flagge h\xE4lt und die Menschen anf\xFChrt,
      f\xFCr ihre Rechte zu k\xE4mpfen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892106
    m_Localized: Mona Lisa
    m_Metadata:
      m_Items: []
  - m_Id: 7553696484286464
    m_Localized: "Das ber\xFChmteste Gem\xE4lde der Welt! Eine Frau mit einem geheimnisvollen
      L\xE4cheln, gemalt von Leonardo da Vinci. Sie scheint einem mit ihren Augen
      zu folgen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892107
    m_Localized: Venus von Milo
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892108
    m_Localized: "Eine wundersch\xF6ne antike Frauenstatue aus wei\xDFem Marmor.
      Ihre Arme fehlen, dennoch gilt sie als eine der sch\xF6nsten Statuen aller
      Zeiten."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892109
    m_Localized: 24 Stunden von Mans
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892110
    m_Localized: Ein Autorennen, das 24 Stunden dauert. Die Teams fahren Tag und
      Nacht.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892111
    m_Localized: Boot auf der Seine
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892112
    m_Localized: "Ein besonderes Boot, das auf der Seine in Paris f\xE4hrt. Vom Wasser
      aus k\xF6nnen Sie den Eiffelturm und andere wundersch\xF6ne Geb\xE4ude sehen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892113
    m_Localized: "Charta des S\xE4kularismus"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892114
    m_Localized: "Ein Regelwerk zum Respektieren der \xDCberzeugungen aller. Es tr\xE4gt
      dazu bei, dass die Menschen in Frieden zusammenleben."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086400
    m_Localized: Schreibschrift
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086401
    m_Localized: Eine besondere Schreibweise, bei der alle Buchstaben eines Wortes
      miteinander verbunden sind. In Frankreich lernen Kinder diese Schreibweise
      in der Schule.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086402
    m_Localized: Europakarte
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086403
    m_Localized: "Eine Karte mit allen L\xE4ndern Europas. Sie k\xF6nnen sehen, wo
      sich Frankreich, Polen, Deutschland und andere L\xE4nder befinden."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086404
    m_Localized: Meerjungfrau von Warschau
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086405
    m_Localized: Das Symbol der Stadt ist eine tapfere Meerjungfrau mit Schwert und
      Schild. Ihre Statue ist am Fluss zu sehen.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086406
    m_Localized: Brand von Notre-Dame
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086407
    m_Localized: "Im Jahr 2019 besch\xE4digte ein Gro\xDFbrand die wundersch\xF6ne
      Kathedrale Notre-Dame in Paris. Viele Menschen arbeiteten zusammen, um dieses
      wichtige Geb\xE4ude zu retten."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086408
    m_Localized: "Franz\xF6sische Piraten"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548266086409
    m_Localized: "Tapfere Seeleute, die f\xFCr den K\xF6nig von Frankreich \xFCber
      die Meere segelten. Sie wurden Korsaren genannt und sch\xFCtzten franz\xF6sische
      Schiffe vor Feinden."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280704
    m_Localized: Polnischer Zwerg (Breslauer Zwerge)
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280705
    m_Localized: In ganz Breslau sind kleine Zwergstatuen versteckt. Sie zu finden
      ist ein lustiges Stadtspiel.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280706
    m_Localized: Kriege und Sawa
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280707
    m_Localized: "Zwei legend\xE4re Figuren gaben Warschau seinen Namen. Wars war
      ein tapferer Krieger und Sawa war eine wundersch\xF6ne Meerjungfrau, die in
      der Weichsel lebte."
    m_Metadata:
      m_Items: []
  - m_Id: 7604111984812032
    m_Localized: "Euro-W\xE4hrung"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280708
    m_Localized: "In vielen L\xE4ndern Europas wird dieses Zahlungsmittel verwendet.
      Mit Euro k\xF6nnen Sie Spielzeug und Eis kaufen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280709
    m_Localized: "Zloty W\xE4hrung"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280710
    m_Localized: "Die polnische W\xE4hrung hei\xDFt Z\u0142oty. M\xFCnzen und Banknoten
      tragen polnische Symbole."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280711
    m_Localized: "Coll\xE8ge in Frankreich"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280712
    m_Localized: "Mittelschule in Frankreich f\xFCr Kinder im Alter von 11 bis 15
      Jahren. Die Sch\xFCler lernen viele F\xE4cher und bereiten sich auf die High
      School vor."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280713
    m_Localized: "\xC9cole Maternelle"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280714
    m_Localized: "Schule f\xFCr Kleinkinder von 3 bis 5 Jahren. Sie lernen durch
      Spielen und Entdecken."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280715
    m_Localized: Grundschule in Frankreich
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280716
    m_Localized: "Schule f\xFCr Kinder von 6 bis 10 Jahren. Sie lernen Lesen, Schreiben
      und Rechnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280717
    m_Localized: "Lyc\xE9e in Frankreich"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280718
    m_Localized: "Gymnasium in Frankreich f\xFCr Jugendliche im Alter von 16 bis
      18 Jahren. Die Sch\xFCler lernen flei\xDFig f\xFCr die Abiturpr\xFCfung, um
      an die Universit\xE4t zu gehen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280719
    m_Localized: Flagge Andorras
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280720
    m_Localized: Die Flagge Andorras hat blaue, gelbe und rote Streifen mit einem
      Wappen in der Mitte. Andorra ist ein kleines Land zwischen Frankreich und Spanien.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280721
    m_Localized: Flagge von Belarus
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280722
    m_Localized: "Die Flagge von Wei\xDFrussland hat horizontale rote und gr\xFCne
      Streifen mit sch\xF6nen traditionellen Mustern an den Seiten. Wei\xDFrussland
      ist Polens Nachbarland."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280723
    m_Localized: Flagge von Belgien
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280724
    m_Localized: "Die Flagge Belgiens hat drei vertikale Streifen: Schwarz, Gelb
      und Rot. Belgien ist ber\xFChmt f\xFCr Schokolade und Waffeln!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280725
    m_Localized: Flagge der Tschechischen Republik
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280726
    m_Localized: "Die Flagge hat wei\xDFe und rote Streifen mit einem blauen Dreieck.
      Die Tschechische Republik ist ber\xFChmt f\xFCr ihre wundersch\xF6ne Prager
      Burg und ihr Kristallglas."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280727
    m_Localized: Flagge von Frankreich
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280728
    m_Localized: "Die Flagge Frankreichs hat drei vertikale Streifen: Blau, Wei\xDF
      und Rot. Diese Farben stehen f\xFCr Freiheit, Gleichheit und Br\xFCderlichkeit!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280729
    m_Localized: Flagge Deutschlands
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280730
    m_Localized: "Die Flagge Deutschlands hat drei horizontale Streifen: Schwarz,
      Rot und Gelb. Deutschland ist ber\xFChmt f\xFCr Autos, Schl\xF6sser und M\xE4rchen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280731
    m_Localized: Flagge von Italien
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280732
    m_Localized: "Die Flagge Italiens hat drei vertikale Streifen: Gr\xFCn, Wei\xDF
      und Rot. Die Farben sehen aus wie Basilikum, Mozzarella und Tomaten auf einer
      Pizza!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280733
    m_Localized: Flagge Litauens
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280734
    m_Localized: "Die Flagge hat horizontale Streifen in Gelb, Gr\xFCn und Rot. Litauen
      ist ein baltisches Land mit wundersch\xF6nen W\xE4ldern und Str\xE4nden."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280735
    m_Localized: Flagge von Luxemburg
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280736
    m_Localized: "Die Flagge Luxemburgs hat horizontale Streifen in Rot, Wei\xDF
      und Hellblau. Luxemburg ist ein sehr kleines Land, in dem drei Sprachen gesprochen
      werden!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280737
    m_Localized: Flagge von Monaco
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280738
    m_Localized: "Die Flagge von Monaco hat horizontale rote und wei\xDFe Streifen.
      Monaco ist klein, aber ber\xFChmt f\xFCr schicke Autos und k\xF6nigliche Pal\xE4ste
      am Meer!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280739
    m_Localized: Flagge Polens
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280740
    m_Localized: "Die Flagge Polens hat wei\xDFe und rote horizontale Streifen. Polen
      ist ber\xFChmt f\xFCr Wissenschaftler wie Kopernikus und k\xF6stliche Pierogi!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280741
    m_Localized: Flagge Russlands
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280742
    m_Localized: "Eine Flagge mit wei\xDFen, blauen und roten Streifen. Sie repr\xE4sentiert
      das Land Russland."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280743
    m_Localized: Flagge der Slowakei
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280744
    m_Localized: "Eine Flagge in den Farben Wei\xDF, Blau und Rot. Sie repr\xE4sentiert
      das Land Slowakei."
    m_Metadata:
      m_Items: []
  - m_Id: 6292376883552256
    m_Localized: Flagge von Spanien
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280745
    m_Localized: Die spanische Flagge hat horizontale rote und gelbe Streifen. Die
      Farben sind wie Sonne und Paprika! Spanien hat den Flamenco-Tanz erfunden.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280746
    m_Localized: Flagge der Schweiz
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280747
    m_Localized: "Die Flagge der Schweiz ist rot mit einem wei\xDFen Kreuz in der
      Mitte. Sie sieht aus wie ein Erste-Hilfe-Kasten! Die Schweiz ist ber\xFChmt
      f\xFCr ihre Berge und ihren K\xE4se."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280748
    m_Localized: Flagge der Ukraine
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280749
    m_Localized: "Eine Flagge in den Farben Blau und Gelb wie Himmel und Sonne. Sie
      repr\xE4sentiert das Land Ukraine."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280750
    m_Localized: "Franz\xF6sisches Baguette"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280751
    m_Localized: "Ein langes, knuspriges Brot, das ber\xFChmteste Lebensmittel Frankreichs.
      Die Franzosen kaufen jeden Tag frische Baguettes beim B\xE4cker!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280752
    m_Localized: Bouillabaisse
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280753
    m_Localized: "Eine besondere Fischsuppe aus Marseille in S\xFCdfrankreich. Sie
      wird mit vielen verschiedenen Fischarten zubereitet und duftet k\xF6stlich!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280754
    m_Localized: BROT
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280755
    m_Localized: "Ein leckeres Gericht aus Mehl und Wasser. Mit Brot k\xF6nnen Sie
      Sandwiches machen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548270280756
    m_Localized: Krabbe
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475008
    m_Localized: "Ein Meerestier mit gro\xDFen Scheren und einem harten Panzer. Krabben
      laufen seitw\xE4rts am Strand!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475009
    m_Localized: FISCH
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475010
    m_Localized: Ein Tier, das im Wasser lebt und schwimmt. Fische haben Flossen
      und Kiemen, um unter Wasser zu atmen.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475011
    m_Localized: Mehl
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475012
    m_Localized: "Wei\xDFes Pulver aus Weizen, das zur Brotherstellung verwendet
      wird."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475013
    m_Localized: Zitrone
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475014
    m_Localized: Eine gelbe Frucht, die sehr sauer schmeckt. Aus Zitronen wird Limonade
      hergestellt!
    m_Metadata:
      m_Items: []
  - m_Id: 6288487979409408
    m_Localized: Milch
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475015
    m_Localized: "Ein wei\xDFes Getr\xE4nk, das von K\xFChen stammt. Milch hilft,
      Ihre Knochen und Z\xE4hne zu st\xE4rken!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475016
    m_Localized: "Oliven\xF6l"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475017
    m_Localized: "Ein besonderes \xD6l aus Oliven. Es wird zum Kochen leckerer Gerichte
      verwendet."
    m_Metadata:
      m_Items: []
  - m_Id: 6288404936384512
    m_Localized: Orange
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475018
    m_Localized: "Eine runde, orangefarbene Frucht, die s\xFC\xDF und saftig schmeckt.
      Orangen enthalten viel Vitamin C!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475019
    m_Localized: Pfeffer Salz
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475020
    m_Localized: "Gew\xFCrze, die das Essen schmackhafter machen. Salz ist wei\xDF
      und Pfeffer ist schwarz mit winzigen St\xFCckchen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475021
    m_Localized: Polnische Pierogi
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475022
    m_Localized: "Mit Kartoffeln, K\xE4se oder Obst gef\xFCllte Teigtaschen. Sie
      sind ein ber\xFChmtes polnisches Gericht."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475023
    m_Localized: SALZ
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475024
    m_Localized: "Wei\xDFe Kristalle, die den Geschmack von Speisen verbessern."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475025
    m_Localized: Tomate
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475026
    m_Localized: "Eine rote, runde Frucht, die auf Pflanzen w\xE4chst. Aus Tomaten
      wird Pizzaso\xDFe hergestellt!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475027
    m_Localized: Wasser
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475028
    m_Localized: "Wasser ist f\xFCr alles Leben unentbehrlich"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475029
    m_Localized: Hefe
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475030
    m_Localized: Hefe ist wie ein Zauberpulver, das Brot weich und lecker macht!
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475031
    m_Localized: Kreis
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475032
    m_Localized: "Eine runde Form ohne Ecken. Kreise sehen aus wie R\xE4der, B\xE4lle
      und M\xFCnzen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475033
    m_Localized: Linie
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475034
    m_Localized: "Eine gerade Markierung, die von einem Punkt zum anderen f\xFChrt.
      Linien k\xF6nnen lang oder kurz sein."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475035
    m_Localized: Dreieck
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475036
    m_Localized: "Eine Form mit drei geraden Seiten und drei Ecken. Dreiecke sehen
      aus wie Pizzast\xFCcke!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475037
    m_Localized: Kompass
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475038
    m_Localized: "Ein Werkzeug, mit dem Sie perfekte Kreise zeichnen k\xF6nnen. Es
      hat zwei Schenkel wie eine Schere."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475039
    m_Localized: Herrscher
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475040
    m_Localized: "Ein gerades Werkzeug zum Messen der L\xE4nge von Dingen. Lineale
      haben Zahlen und Linien."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475041
    m_Localized: Quadrat
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475042
    m_Localized: Ein dreieckiges Werkzeug, das in der Mathematik zum Zeichnen von
      geraden Linien und rechten Winkeln verwendet wird.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475043
    m_Localized: Torunischer Lebkuchen
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475044
    m_Localized: "eine traditionelle polnische Leckerei aus Gew\xFCrzen und Honig,
      oft in wundersch\xF6nen Mustern geformt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475045
    m_Localized: 20.000 Meilen unter dem Meer
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475046
    m_Localized: "Ein Buch von Jules Verne \xFCber Unterwasserabenteuer in einem
      U-Boot namens Nautilus. Sie treffen riesige Meerestiere!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475047
    m_Localized: In 80 Tagen um die Welt
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475048
    m_Localized: "Ein spannendes Buch von Jules Verne \xFCber eine schnelle Weltumrundung
      mit Z\xFCgen, Schiffen und Hei\xDFluftballons!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475049
    m_Localized: "F\xFCnf Wochen im Ballon"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475050
    m_Localized: "Ein Buch von Jules Verne \xFCber eine Reise mit dem Hei\xDFluftballon
      \xFCber Afrika. Es steckt voller Abenteuer und Entdeckungen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475051
    m_Localized: Von der Erde zum Mond
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475052
    m_Localized: "Ein Buch von Jules Verne \xFCber die Reise zum Mond in einer gro\xDFen
      Kanone! Es wurde geschrieben, bevor es echte Raketen gab."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475053
    m_Localized: La Marseillaise
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475054
    m_Localized: "Das franz\xF6sische Nationallied. Es wird bei Gro\xDFveranstaltungen
      und Sportspielen gesungen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548274475055
    m_Localized: Mensa-Speisekarte
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669312
    m_Localized: "Eine Liste, die zeigt, was Sie in der Schule zu Mittag essen d\xFCrfen.
      Sie hilft Ihnen bei der Auswahl!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669313
    m_Localized: Chopin-Denkmal
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669314
    m_Localized: "Eine gro\xDFe Statue im Warschauer \u0141azienki-Park ist Fr\xE9d\xE9ric
      Chopin gewidmet. Im Sommer finden hier auch beliebte Open-Air-Klavierkonzerte
      statt. (Der besondere Baum, unter dem er sitzt, ist eine typisch polnische
      Weide.)\n"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669315
    m_Localized: GEFAHR-Zeichen
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669316
    m_Localized: Ein Schild, das vor einer Gefahr warnt. Seien Sie besonders vorsichtig.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669317
    m_Localized: Eiffelturm Karte
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669318
    m_Localized: "Eine Karte, die zeigt, wo sich der ber\xFChmte hohe Turm in Paris
      befindet."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669319
    m_Localized: "Ticket f\xFCr den Eiffelturm"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669320
    m_Localized: "Eine besondere Zeitung, mit der Sie den ber\xFChmten hohen Turm
      in Paris, Frankreich, besuchen k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669321
    m_Localized: "Franz\xF6sisches Schulplakat"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669322
    m_Localized: "Ein gro\xDFes Bild mit Informationen, das an den W\xE4nden der
      Klassenzimmer franz\xF6sischer Schulen h\xE4ngt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669323
    m_Localized: "Hei\xDFluftballon"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669324
    m_Localized: "Ein Ballon, der mit hei\xDFer Luft fliegt. Der erste flog in Frankreich."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669325
    m_Localized: Eisen
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669326
    m_Localized: "Ein hei\xDFes Werkzeug, mit dem zerknitterte Kleidung glatt und
      glatt gemacht wird. Vorsicht, B\xFCgeleisen sind sehr hei\xDF!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669327
    m_Localized: Karte der Seine
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669328
    m_Localized: "Ein Bild, das die Seine und ihre Flie\xDFrichtung durch das Land
      zeigt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669329
    m_Localized: Karte der Seine in Paris
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669330
    m_Localized: "Ein Bild, das zeigt, wie die Seine durch die Stadt Paris flie\xDFt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669331
    m_Localized: Meerjungfrau von Warschau
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669332
    m_Localized: "Die Meerjungfrau ist ein Symbol Warschaus und erscheint auf dem
      Stadtwappen. Eine Statue von ihr steht am Fluss und h\xE4lt ein Schwert und
      einen Schild, um die Stadt zu besch\xFCtzen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669333
    m_Localized: Schulranzen
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669334
    m_Localized: "Eine Tasche, in der Sch\xFCler ihre B\xFCcher, Stifte und Hausaufgaben
      zur Schule tragen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669335
    m_Localized: Weltraumrakete
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669336
    m_Localized: Eine Rakete, die ins All fliegt.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669337
    m_Localized: STOP-Schild
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669338
    m_Localized: "Ein rotes Schild bedeutet, dass Sie Ihr Fahrzeug anhalten m\xFCssen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669339
    m_Localized: U-Boot (Die Nautilus)
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669340
    m_Localized: "Ein U-Boot aus Jules Vernes Geschichte. Es k\xF6nnte die Tiefsee
      erkunden."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669341
    m_Localized: Ampel
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669342
    m_Localized: "Eine Ampel mit Rot, Gelb und Gr\xFCn zur Verkehrsregelung."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669343
    m_Localized: Zug
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669344
    m_Localized: "Z\xFCge transportieren Menschen und G\xFCter schnell durch das
      Land."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669345
    m_Localized: "B\xE4cker"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669346
    m_Localized: "Eine Person, die Brot, Kuchen und Geb\xE4ck herstellt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669347
    m_Localized: "K\xE4seh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669348
    m_Localized: "Eine Person, die viele K\xE4sesorten verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669349
    m_Localized: "Fischh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669350
    m_Localized: "Eine Person, die frischen Fisch und Meeresfr\xFCchte verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669351
    m_Localized: Fryderyk Chopin
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669352
    m_Localized: "Ein ber\xFChmter Klavierkomponist aus Polen. Er schrieb wundersch\xF6ne
      Musik, die wie Tanzen oder Geschichtenerz\xE4hlen klingt. Seine Musik macht
      Menschen gl\xFCcklich oder traurig."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669353
    m_Localized: "Gem\xFCseh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669354
    m_Localized: "Eine Person, die frisches Obst und Gem\xFCse verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669355
    m_Localized: "Lebensmittelh\xE4ndler"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669356
    m_Localized: "Eine Person, die viele Arten von Lebensmitteln und Getr\xE4nken
      verkauft."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669357
    m_Localized: Gustave Eiffel
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669358
    m_Localized: "Der Mann, der den Eiffelturm gebaut hat! Er war ein Ingenieur,
      der es liebte, mit Eisen zu bauen und den ber\xFChmtesten Turm der Welt schuf."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669359
    m_Localized: Jean Michel Jarre
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669360
    m_Localized: "Ein franz\xF6sischer Musiker, der f\xFCr seine elektronische Musik
      bekannt ist. Er macht gro\xDFe Lichtshows."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669361
    m_Localized: Jules Verne
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669362
    m_Localized: "Ein franz\xF6sischer Schriftsteller, der sich unglaubliche Abenteuer
      ausdachte, bevor sie m\xF6glich waren! Er schrieb \xFCber U-Boote, Raketen
      und Reisen um die Welt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669363
    m_Localized: "K\xF6nig Sigismund III."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669364
    m_Localized: "Ein K\xF6nig von Polen, der in Warschau viele sch\xF6ne Geb\xE4ude
      errichten lie\xDF. Seine Statue steht auf einer hohen S\xE4ule im Stadtzentrum."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669365
    m_Localized: Leonardo da Vinci
    m_Metadata:
      m_Items: []
  - m_Id: 7717548278669366
    m_Localized: "Ein erstaunlicher K\xFCnstler und Wissenschaftler aus Italien.
      Er malte die Mona Lisa und erfand Flugmaschinen Hunderte von Jahren vor Flugzeugen!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863616
    m_Localized: Maria Curie
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863617
    m_Localized: "Eine brillante Wissenschaftlerin aus Polen, die die Radioaktivit\xE4t
      entdeckte. Sie war die erste Frau, die einen Nobelpreis erhielt, und das gleich
      zweimal!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863618
    m_Localized: Nikolaus Kopernikus
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863619
    m_Localized: "Ein brillanter Wissenschaftler aus Polen entdeckte, dass sich die
      Erde um die Sonne dreht und nicht umgekehrt! Dies ver\xE4nderte unser Verst\xE4ndnis
      des Weltraums."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863620
    m_Localized: "Pr\xE4sident"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863621
    m_Localized: "Der gew\xE4hlte F\xFChrer, der das Land f\xFChren soll. Die Menschen
      stimmen bei Wahlen ab."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863622
    m_Localized: Ostsee
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863623
    m_Localized: "Ein gro\xDFes Meer in Nordeuropa, wo Polen, Deutschland und andere
      L\xE4nder aufeinandertreffen. Es hat wundersch\xF6ne Str\xE4nde und ist ber\xFChmt
      f\xFCr seine Bernsteinsch\xE4tze!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863624
    m_Localized: "Boot f\xFCr Waren"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863625
    m_Localized: "Ein gro\xDFes Boot, das Kisten, Lebensmittel und andere Dinge von
      Ort zu Ort transportiert."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863626
    m_Localized: Bootshaus
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863627
    m_Localized: "Ein Haus, das wie ein Boot auf dem Wasser schwimmt. In Bootsh\xE4usern
      kann man leben!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863628
    m_Localized: "Boot f\xFCr Menschen"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863629
    m_Localized: "Ein Boot, das Menschen \xFCber das Wasser von einem Ort zum anderen
      bef\xF6rdert."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863630
    m_Localized: Flussboot
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863631
    m_Localized: "Ein Boot, das auf Fl\xFCssen f\xE4hrt. Fl\xFCsse sind wie Stra\xDFen
      aus Wasser!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863632
    m_Localized: "Br\xFCcke f\xFCr Autos"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863633
    m_Localized: "Eine Stra\xDFe, die \xFCber Wasser f\xFChrt, sodass Autos Fl\xFCsse
      und Seen \xFCberqueren k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863634
    m_Localized: "Fu\xDFg\xE4ngerbr\xFCcken"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863635
    m_Localized: "Kleine Br\xFCcken f\xFCr Fu\xDFg\xE4nger. Sie sch\xFCtzen Sie vor
      dem Verkehr."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863636
    m_Localized: "Br\xFCcke f\xFCr Z\xFCge"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863637
    m_Localized: "Eine spezielle Br\xFCcke, die stark genug gebaut ist, damit schwere
      Z\xFCge das Wasser \xFCberqueren k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863638
    m_Localized: "Schlossbr\xFCcke"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863639
    m_Localized: "Eine Br\xFCcke in der N\xE4he einer Burg. Burgen sind gro\xDFe,
      starke Geb\xE4ude, in denen vor langer Zeit K\xF6nige und K\xF6niginnen lebten."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863640
    m_Localized: Klassenzimmer
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863641
    m_Localized: "Ein Raum in der Schule, in dem Sch\xFCler sitzen und von ihrem
      Lehrer neue Dinge lernen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863642
    m_Localized: Eiffelturm
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863643
    m_Localized: Ein hoher Turm und ein Wahrzeichen von Paris. Er ist aus Eisen und
      300 Meter hoch. Von dort aus hat man einen herrlichen Blick auf die ganze Stadt.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863644
    m_Localized: "\xCEle-de-France"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863645
    m_Localized: Eine Region in Frankreich, in der sich die Hauptstadt Paris befindet.
      Hier leben viele Menschen!
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863646
    m_Localized: Louvre
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863647
    m_Localized: Ein riesiges Museum voller Kunst. Hier lebt die Mona Lisa.
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863648
    m_Localized: Mont Blanc
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863649
    m_Localized: "Der h\xF6chste Berg Westeuropas. Das ganze Jahr \xFCber schneebedeckt."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863650
    m_Localized: Nikolaus-Kopernikus-Haus
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863651
    m_Localized: "Heute ist es ein Museum, in dem Sie mehr \xFCber sein Leben und
      seine wissenschaftlichen Errungenschaften erfahren k\xF6nnen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863652
    m_Localized: Notre-Dame de Paris
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863653
    m_Localized: "Eine wundersch\xF6ne Kathedrale in Paris. Ihre bunten Glasfenster
      erz\xE4hlen Geschichten."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863654
    m_Localized: Oder
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863655
    m_Localized: "Ein gro\xDFer Fluss im Westen Polens. Er dient der Schifffahrt
      und dem Handel."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863656
    m_Localized: Paris
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863657
    m_Localized: "Die Hauptstadt Frankreichs. Paris hat einen ber\xFChmten hohen
      Turm namens Eiffelturm!"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863658
    m_Localized: Pont Alexandre III
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863659
    m_Localized: "Eine wundersch\xF6ne Br\xFCcke in Paris mit kunstvollen Verzierungen
      und goldenen Statuen darauf."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863660
    m_Localized: Seine
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863661
    m_Localized: Ein 777 Kilometer langer Fluss in Nordfrankreich
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863662
    m_Localized: "Weichsel (Wis\u0142a)"
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863663
    m_Localized: "Der l\xE4ngste Fluss Polens hei\xDFt Wis\u0142a oder Weichsel.
      Er flie\xDFt durch Krakau und Warschau."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863664
    m_Localized: 'Breslau: Kathedrale'
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863665
    m_Localized: "Eine hohe Backsteinkirche auf einer Insel im Fluss. Sie k\xF6nnen
      den Turm besteigen, um die Aussicht zu genie\xDFen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863666
    m_Localized: Jahrhunderthalle
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863667
    m_Localized: "Die Jahrhunderthalle ist ein riesiges Geb\xE4ude, das wie ein gro\xDFes
      Zelt mit Dach aussieht. Es ist sehr hoch. Im Inneren k\xF6nnen Sie verschiedene
      Shows sehen, Konzerte h\xF6ren oder Sportspiele verfolgen."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863668
    m_Localized: 'Breslau: Altes Rathaus'
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863669
    m_Localized: "Ein wundersch\xF6nes gotisches Geb\xE4ude auf dem Hauptplatz. Es
      hat eine ber\xFChmte Uhr."
    m_Metadata:
      m_Items: []
  - m_Id: 7717548282863670
    m_Localized: 'Breslau: Sky Tower'
    m_Metadata:
      m_Items: []
  - m_Id: 7717548287057920
    m_Localized: "Eines der h\xF6chsten Geb\xE4ude in Polen. Es verf\xFCgt \xFCber
      Gesch\xE4fte und einen Aussichtspunkt."
    m_Metadata:
      m_Items: []
  - m_Id: 6281897003966464
    m_Localized: Hefe
    m_Metadata:
      m_Items: []
  - m_Id: 6292171287158784
    m_Localized: "Oliven\xF6l"
    m_Metadata:
      m_Items: []
  - m_Id: 7553621259444224
    m_Localized: Mona Lisa
    m_Metadata:
      m_Items: []
  references:
