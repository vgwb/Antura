    m_Metadata:
      m_Items: []
  - m_Id: 17917029638150
    m_Localized: "Wroc\u0142aw: Sky Tower"
    m_Metadata:
      m_Items: []
  - m_Id: 17917029638151
    m_Localized: "Uno de los edificios m\xE1s altos de Polonia. Tiene tiendas y un
