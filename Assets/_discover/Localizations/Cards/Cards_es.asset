    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892113
    m_Localized: Carta de la Laicidad
    m_Metadata:
      m_Items: []
  - m_Id: 7717548261892114
    m_Localized: "Un conjunto de normas para respetar las creencias de todos. Facilita
